;*
public procedure Page_Selections_1_Ap

; après saisie des critères de sélection

1				i						3,0

beginp	; Page_Selections_1_Ap

	if C3XQ.PERDDT < CD.EXODDT
		Mes_Alert(7108, sdate(CD.EXODDT))
		HARMONY.RETOUR		= XMENEXT_POINT_SEQUENCE
		HARMONY.CPLRETOUR	= 1
		preturn
	endif

	if C3XQ.PERFDT < C3XQ.PERDDT
		Mes_Alert(3004)
		HARMONY.RETOUR		= XMENEXT_POINT_SEQUENCE
		HARMONY.CPLRETOUR	= 1
		preturn
	endif

	if C3XQ.ARRETEDT > C3XQ.PERFDT or C3XQ.ARRETEDT < C3XQ.PERDDT
		Mes_Alert(7109)
		HARMONY.RETOUR		= XMENEXT_POINT_SEQUENCE
		HARMONY.CPLRETOUR	= 1
		preturn
	endif

	if C3XQ.IMMO2 < C3XQ.IMMO1
		Mes_Alert(3031)
		HARMONY.RETOUR		= XMENEXT_POINT_SEQUENCE
		HARMONY.CPLRETOUR	= 2
		preturn
	endif

	if C3XQ.UOCODE2 < C3XQ.UOCODE1
		Mes_Alert(3032)
		HARMONY.RETOUR		= XMENEXT_POINT_SEQUENCE
		HARMONY.CPLRETOUR	= 2
		preturn
	endif

	if C3XQ.CPTTAB2(1) < C3XQ.CPTTAB1(1)
		Mes_Alert(3006)
		HARMONY.RETOUR		= XMENEXT_POINT_SEQUENCE
		HARMONY.CPLRETOUR	= 3
		preturn
	endif

	if C3XQ.CPTTAB2(2) < C3XQ.CPTTAB1(2)
		Mes_Alert(3006)
		HARMONY.RETOUR		= XMENEXT_POINT_SEQUENCE
		HARMONY.CPLRETOUR	= 4
		preturn
	endif

	if C3XQ.ACQDT2 < C3XQ.ACQDT1
		Mes_Alert(3004)
		HARMONY.RETOUR		= XMENEXT_POINT_SEQUENCE
		HARMONY.CPLRETOUR	= 5
		preturn
	endif

	if C3XQ.SVCDT2 < C3XQ.SVCDT1
		Mes_Alert(3004)
		HARMONY.RETOUR		= XMENEXT_POINT_SEQUENCE
		HARMONY.CPLRETOUR	= 6
		preturn
	endif

	if C3XQ.Local2 < C3XQ.Local1
		Mes_Alert(3038)
		Harmony.Retour		= XMENEXT_POINT_SEQUENCE
		Harmony.CplRetour	= 11
		preturn
	endif

	for i = 1 to 4
		if C3XQ.AXETAB2(i) < C3XQ.AXETAB1(i)
			Mes_Alert(3002)
			HARMONY.RETOUR		= XMENEXT_POINT_SEQUENCE
			HARMONY.CPLRETOUR	= i + 6
			preturn
		endif
	next

endp		; Page_Selections_1_Ap


