;*
public procedure Champ_PerDDt_1_Ap

; après saisie début exercice

beginp	; Champ_PerDDt_1_Ap

	C3XQ.PERDDT = Debut_Exercice(C3XQ.PERDDT)

	if C3XQ.PERDDT < CD.EXODDT
		Mes_Alert(7108, sdate(CD.EXODDT))
		HARMONY.RETOUR = XmeNext_Traitement_Avant
		preturn
	endif

	if not HARMONY.DATAMODIF	|	preturn	|	endif

	C3XQ.PERFDT = Fin_Exercice(C3XQ.PERDDT)
	C3XQ.ARRETEDT = C3XQ.PERFDT

endp		; Champ_PerDDt_1_Ap


;*
public procedure Champ_PerFDt_1_Ap

; après saisie fin exercice

beginp	; Champ_PerFDt_1_Ap

	if C3XQ.PERFDT = ' '
		C3XQ.PERFDT = Fin_Exercice(C3XQ.PERDDT)
	endif

	C3XQ.PERFDT = hdate(C3XQ.PERFDT, 'd')
	Controle_Fourchette(C3XQ.PERDDT, C3XQ.PERFDT, 3, 3004)

	C3XQ.ARRETEDT = C3XQ.PERFDT

endp		; Champ_PerFDt_1_Ap


;*
public procedure Champ_ArreteDt_1_Ap

; après saisie date d'arrêté

beginp	; Champ_ArreteDt_1_Ap

	if C3XQ.ARRETEDT = ' '
		C3XQ.ARRETEDT = Fin_Exercice(C3XQ.PERDDT)
	endif

	C3XQ.ARRETEDT = hdate(C3XQ.ARRETEDT, 'd')

	if C3XQ.ARRETEDT > C3XQ.PERFDT or C3XQ.ARRETEDT < C3XQ.PERDDT
		Mes_Alert(7109)
		HARMONY.RETOUR = XmeNext_Traitement_Avant
		preturn
	endif

endp		; Champ_ArreteDt_1_Ap


