[diva]
Include	"gttc000.dhsp"

Public Record "cogfdd.dhsd" cog_valid_part cog_valid_part
Public Record "cogfdd.dhsd" cog_valid_pmt cog_valid_pmt
Public Record "ccfdd.dhsd" c3 c3
Public Record "ccfdd.dhsd" c5 c5

;*
Public Procedure Champ_Cog_MtMax_1_Ap
;
Beginp

	if cog_valid_pmt.Cog_MtMax <= 0
		messagebox("Le montant doit être supérieur à 0","Alerte",mb_iconexclamation)
		harmony.Retour	= xmenext_ressaisie
	endif

Endp


;*
Public Procedure Champ_Cog_MtMax_1_Av
;
Beginp

	if cog_valid_pmt.Cog_TypeV = 1	;pas de saisie de montant pour les vérificateurs
		harmony.Retour	= xmenext_saut_donnee
	endif
	
Endp


;*
procedure Cog_ControlerUtilisateur
hfile		a5dd.dhsd	a5fdos	a5fdos_loc

record	a5dd.dhsd	muser		muser_loc
record	a5dd.dhsd	musergrp	musergrp_loc

beginp

	if (cog_valid_pmt.Cog_TypeU = 1 and hseek(a5fdos_loc,muser_loc,"B"&cog_valid_pmt.User)) or \
	(cog_valid_pmt.Cog_TypeU = 2 and hseek(a5fdos_loc,musergrp_loc,"K"&cog_valid_pmt.User))

		messagebox("Utilisateur ou Groupe d'utilisateurs inexistant","Alerte",mb_iconexclamation)
		harmony.Retour	= xmenext_traitement_avant

	endif
	
endp


;*
Public Procedure Champ_User_1_Ap
;
Beginp

	if harmony.Key = K_F8

		if cog_valid_pmt.Cog_TypeU = 1
			Zoom_Call(11009)
		else
			Zoom_Call(11015)
		endif

	elsif cog_valid_pmt.User = ""

		harmony.Retour	= xmenext_ressaisie

	else
		
		Cog_ControlerUtilisateur

	endif
		
Endp


;*
Public Procedure Page_SaisieCleCreation_Ap
;
Beginp

	Cog_ControlerUtilisateur

Endp
