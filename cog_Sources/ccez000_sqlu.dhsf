;*
public procedure Champ_ExoDDt_1_Av

; teste si saisie date début exercice

beginp	; Champ_ExoDDt_1_Av	

	if ZOOM.Action <> ZOOM_CREATION
		Harmony.Retour = XMENEXT_SAUT_DONNEE
	endif

endp		; Champ_ExoDDt_1_Av


;*
public procedure Champ_PerDDt_1_Av

; teste si saisie Début période balance

beginp	; Champ_PerDDt_1_Av	

	if ZOOM.Action <> ZOOM_CREATION
		Harmony.Retour = XMENEXT_SAUT_DONNEE
	endif

endp		; Champ_PerDDt_1_Av


;*
public procedure Champ_PerFDt_1_Av

; teste si saisie Fin période balance

beginp	; Champ_PerFDt_1_Av	

	if ZOOM.Action <> ZOOM_CREATION
		Harmony.Retour = XMENEXT_SAUT_DONNEE
	endif

endp		; Champ_PerFDt_1_Av


;*
public procedure Champ_PerDDt_1_Ap

; contrôle début période balance

beginp	; Champ_PerDDt_1_Ap	

	if DOSSIER.PerDDt < DOSSIER.ExoDDt
		Mes_alert(19001)
		Harmony.Retour = XmeNext_Traitement_Avant
	endif

endp		; Champ_PerDDt_1_Ap


;*
public procedure Champ_PerFDt_1_Ap

; contrôle fin période balance

beginp	; Champ_PerFDt_1_Ap

	if DOSSIER.PerFDt < DOSSIER.PerDDt
		Mes_alert(19002)
		Harmony.Retour = XmeNext_Traitement_Avant
	endif

endp		; Champ_PerFDt_1_Ap		


;*
public procedure Champ_ProtFDt_1_Ap

; contrôle fin protection saisie

beginp	; Champ_ProtFDt_1_Ap	

	if DOSSIER.ProtFDt < DOSSIER.ProtDDt
		Mes_alert(19003)
		Harmony.Retour = XmeNext_Traitement_Avant
	endif

endp		; Champ_ProtFDt_1_Ap		


;*
public procedure Champ_Dev_1_Av

; avant code devise

beginp	; Champ_Dev_1_Av	

	if hread(CCFECR, C8, [ 'H' DOSSIER.Dos ], 'F') = 0 and C8.Dos = DOSSIER.Dos	; il y a des écritures => plus de saisie
		Harmony.Retour = XMENEXT_SAUT_DONNEE
	endif

endp		; Champ_Dev_1_Av


