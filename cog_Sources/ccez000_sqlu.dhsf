beginp	; Champ_AxeVtl_1_Av	

	if DOSSIER.AxeLib(Harmony.DataInd1) = ' '
		Harmony.Retour = XMENEXT_SAUT_DONNEE
	endif

endp		; Champ_AxeVtl_1_Av

;*
public procedure Champ_AxeRepCod_1_Av

; si pas de libellé passer à la donnée suivante

