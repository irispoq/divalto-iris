Public Procedure Champ_EtbCod_1_Ap

; Flag gestion des établissements

Beginp	; Champ_EtbCod_1_Ap

	if DOSSIER.EtbCod <> 2
		DOSSIER.Etb 		 = ' '
		DOSSIER.EtbLettTyp = 3
	else
		Harmony.Retour 	= XMENEXT_POINT_SEQUENCE	; positionnement sur l'établissement principal
		Harmony.CplRetour = 4000							; obligatoire au cas où clic ailleurs
	endif	
	
Endp		; Champ_EtbCod_1_Ap


;*
Public Procedure Champ_EtbLettTyp_1_Av

; Méthode de lettrage automatique

Beginp 	; Champ_EtbLettTyp_1_Av

	if DOSSIER.EtbCod <> 2
		Harmony.Retour = Xmenext_Saut_Donnee
	endif

Endp		; Champ_EtbLettTyp_1_Av


