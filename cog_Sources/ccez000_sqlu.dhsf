;*
public procedure Champ_Cpt_1_Ap

; contrôle numéro compte différence de règlement

beginp	; Champ_Cpt_1_Ap	

	if DOSSIER.DRegCpt(Harmony.DataInd1) <> ' '
		Controle_Compte(DOSSIER.DRegCpt(Harmony.DataInd1), syntaxe = 2, existence = false, cptnat = 2, etablissement = false)
	else
		if Harmony.DataInd1 = 1
			DOSSIER.DRegCpt(Harmony.DataInd1) = "67180000"
		else
			DOSSIER.DRegCpt(Harmony.DataInd1) = "77180000"
		endif
	endif

endp		; Champ_Cpt_1_Ap


