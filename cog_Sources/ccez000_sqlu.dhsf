;*
public procedure Champ_AxeDcTb_1_Av

; avant découpage axe

beginp	; Champ_AxeDcTb_1_Av

	if DOSSIER.AxeLib(Harmony.DataInd1) = ' '	\
	or (Harmony.DataInd2 > 1 and DOSSIER.AxeDcTb(Harmony.DataInd1, Harmony.DataInd2 - 1) = 0)
		Harmony.Retour = XMENEXT_SAUT_DONNEE
	endif

endp		; Champ_AxeDcTb_1_Av


;*
public procedure Champ_AxeDcTb_1_Ap

; contrôle nombre de caractères découpage axe

1				wi						2,0

beginp	; Champ_AxeDcTb_1_Ap

	if DOSSIER.AxeDcTb(Harmony.DataInd1, Harmony.DataInd2) = 0	; réinitialiser suite tab
		for wi = Harmony.DataInd2 to index(DOSSIER.AxeDcTb(), 2)
			DOSSIER.AxeDcTb	(Harmony.DataInd1, wi) = ' '
			DOSSIER.AxeLibDcTb(Harmony.DataInd1, wi) = ' '
		next
	elsif DOSSIER.AxeDcTb(Harmony.DataInd1, Harmony.DataInd2) > 8
		Mes_Alert(19005)
		Harmony.Retour = XmeNext_Traitement_Avant
	elsif Harmony.DataInd2 > 1 and DOSSIER.AxeDcTb(Harmony.DataInd1, Harmony.DataInd2) <= DOSSIER.AxeDcTb(Harmony.DataInd1, Harmony.DataInd2 - 1)
		Mes_Alert(19006)
		Harmony.Retour = XmeNext_Traitement_Avant
	endif

endp		; Champ_AxeDcTb_1_Ap


;*
public procedure Champ_AxeLibDc_1_Av

; avant libellé de découpage axe

beginp	; Champ_AxeLibDc_1_Av

	if DOSSIER.AxeLib (Harmony.DataInd1) = ' '	\
	or DOSSIER.AxeDcTb(Harmony.DataInd1, Harmony.DataInd2) = 0
		Harmony.Retour = XMENEXT_SAUT_DONNEE
	endif

endp		; Champ_AxeLibDc_1_Av


