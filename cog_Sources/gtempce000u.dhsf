;*
Public Procedure Champ_TiersD_1_Ap			; Contrôle tiers origine
1		rz			L			; Résultat zoom
record gtfdd.dhsd CLI Tiers_Sve
record gtfdd.dhsd CLI CLI_Sve
Beginp

	Tiers_Sve= Tiers
	Cli_Sve= Cli
	If Harmony.Key = K_F8
		Switch GTPCE.TiCodOri
		Case 'C'	|	rz = Zoom_Call(9021)
		Case 'F'	|	rz = Zoom_Call(9022)
		Case 'P'	|	rz = Zoom_Call(9023)
		Case 'I'	|	rz = Zoom_Call(9025)
 		EndSwitch
		If rz <> E_ZOOM_OK	|	PReturn	|	EndIf
	EndIf
	Controle_Tiers(GTPCE.TiCodOri, GTPCE.TiersD, 1900)
	switch GTPCE.TiCodOri
		case 'C'
			TIERSORI = CLI
		case 'F'
			TIERSORI	=	FOU
			CLI 	= FOU
		case 'P'
			TIERSORI =	PRO
			CLI 	= PRO
		case 'I'
			TIERSORI =	TIA
			CLI 	= TIa
	endswitch
	Tiers= Tiers_Sve
	Cli = Cli_Sve
Endp				  


