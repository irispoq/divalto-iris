;*
Public Procedure Champ_FiltreOpFam_1_Ap; Contrôle liste des familles de codes opérations
1	nb			L
1	st			L
1	Ok			B
1	i			4,0
1	OpFam		>GTFDD.dhsd OpFamCod			; Code famille de codes opération
1	Filtre	>GTFDD.dhsd	FiltreOpFam		; Familles de codes opérations

Beginp
	If Harmony.Key = K_CTRL_S
		If PongReceiveAndDelete('T080_NBRE', Nb) = 0 And Nb > 0
			For i = 1 to Nb
				If PongReceiveAndDelete([ 'T080_' ToString(i) ], OpFam) = 0
					Filtre = GTSEL_Debut.FiltreOpFam(Harmony.Dataind1)
					Ok     = TRUE
					Loop (st := String(';', Filtre)) > 0
						If Left(Filtre, st - 1) = OpFam
							ok = FALSE
							ExitLoop
						EndIf
						Filtre = Right(Filtre, Length(Filtre) - st)
					EndLoop
					
					If ok = TRUE And Left(Filtre) <> OpFam
						GTSEL_Debut.FiltreOpFam(Harmony.Dataind1) = Left(GTSEL_Debut.FiltreOpFam(Harmony.Dataind1)) & \
																				Condition (GTSEL_Debut.FiltreOpFam(Harmony.Dataind1) = ' ', '', ';') & \
																				OpFam
					EndIf
				EndIf
			Next
		EndIf
	EndIf

   If A5_Filtre_Verifier_Syntaxe(GTSEL_Debut.FiltreOpFam(Harmony.Dataind1), 1) <> 0
      Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
		PReturn
   EndIf
Endp


