	G3XZ.Pong = TRUE
Endp

;*
Public Procedure Champ_FiltreOp_1_Ap	; Contrôle liste des codes opérations
1	nb			L
1	st			L
1	Ok			B
1	i			4,0
1	Op			>GTFDD.dhsd Op
1	Filtre	>GTFDD.dhsd	FiltreOp		; Code opérations

Beginp
	GTPCE.TicodDst = ticod_sv	|	GTPCE.PiCodDst = picod_sv

	If Harmony.Key = K_CTRL_S
		If PongReceiveAndDelete('T020_NBRE', Nb) = 0 And Nb > 0
			For i = 1 to Nb
				If PongReceiveAndDelete([ 'T020_' ToString(i) ], Op) = 0
					Filtre = GTSEL_Debut.FiltreOp(Harmony.Dataind1)
					Ok     = TRUE
					Loop (st := String(';', Filtre)) > 0
						If Left(Filtre, st - 1) = Op
							ok = FALSE
							ExitLoop
						EndIf
						Filtre = Right(Filtre, Length(Filtre) - st)
					EndLoop
					
					If ok = TRUE And Left(Filtre) <> Op
						GTSEL_Debut.FiltreOp(Harmony.Dataind1) = Left(GTSEL_Debut.FiltreOp(Harmony.Dataind1)) & \
																				Condition (GTSEL_Debut.FiltreOp(Harmony.Dataind1) = ' ', '', ';') & \
																				Op
					EndIf
				EndIf
			Next
		EndIf
	EndIf

   If A5_Filtre_Verifier_Syntaxe(GTSEL_Debut.FiltreOp(Harmony.Dataind1), 1) <> 0
      Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
		PReturn
   Endif
Endp


