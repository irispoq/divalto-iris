;*
Public Procedure Champ_Tiers_1_Ap			; Après le tiers en fenêtre création
1		rz			L			; Résultat zoom
Beginp
	If Harmony.Key = K_F8
		Switch GTPCE.TiCodDst
		Case 'C'	|	rz = Zoom_Call(9021)
		Case 'F'	|	rz = Zoom_Call(9022)
		Case 'P'	|	rz = Zoom_Call(9023)
		Case 'I'	|	rz = Zoom_Call(9025)
 		EndSwitch
		If rz <> E_ZOOM_OK	|	PReturn	|	EndIf
	EndIf
	If Controle_Tiers(GTPCE.TiCodDst, GTPCE.TiersF, 1900, TRUE, Today, TRUE) <> 0	And \
		G3XQ.Tunnel = 3 And GTPCE.TiersPilote = GTPCE.TiersF
		GTPCE.TiersPilote = ' '
	EndIf
	GTENT.Lib(28)= Cli.nom
	switch	GTPCE.TiCodDst
	case	'C'		; recherche d'un client
		TIERS.Nom		= CLI.Nom
		TIERS.Vil		= CLI.Vil
		TIERS.CPostal	= CLI.CPostal
		TIERS.Rue		= CLI.Rue
		TIERS.AdrCpl1	= CLI.AdrCpl1
		GTENT.NOTE 		= CLI.NOTE
		GTENT.EnAc(*) 	= CLI.EnAc(*)
		GTENT.EnMax(*)	= CLI.EnMax(*)
	case	'F'		; recherche d'un fournisseur
		TIERS.Nom		= FOU.Nom
		TIERS.Vil		= FOU.Vil
		TIERS.CPostal	= FOU.CPostal
		TIERS.Rue		= FOU.Rue
		TIERS.AdrCpl1	= FOU.AdrCpl1
		GTENT.NOTE 	= FOU.NOTE
		GTENT.EnAc(*) 	= FOU.EnAc(*)
		GTENT.EnMax(*)	= FOU.EnMax(*)
	case	'P'		; recherche d'un prospect
		TIERS.Nom		= PRO.Nom
		TIERS.Vil		= PRO.Vil
		TIERS.Rue		= PRO.Rue
		TIERS.AdrCpl1	= PRO.AdrCpl1
		TIERS.CPostal	= PRO.CPostal
		GTENT.NOTE 		= PRO.NOTE
	case	'I'		; recherche d'un prospect
		TIERS.Nom		= TIA.Nom
		TIERS.Vil		= TIA.Vil
		TIERS.CPostal	= TIA.CPostal
		TIERS.Rue		= TIA.Rue
		TIERS.AdrCpl1	= TIA.AdrCpl1
	endswitch

Endp


