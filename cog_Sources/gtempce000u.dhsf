Public Procedure	Masquage_Champs
beginp
	if T020_Ent.NstD <> ' ' or OP_TrtType_TransfertDepot(T020_Ent)
		XmeSetAttribut("transdepo", AN_VISIBILITE, AV_VISIBLE)
	else
