		XmeSetAttribut("delais", AN_VISIBILITE, AV_CACHE)
	endif

	if GTPCE.PicodOri = 4 
		XmeSetAttribut("avoir", AN_VISIBILITE, AV_VISIBLE)
	else
		XmeSetAttribut("avoir", AN_VISIBILITE, AV_CACHE)
	endif
endp


