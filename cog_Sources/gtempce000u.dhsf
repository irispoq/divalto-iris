;*
Public Procedure Page_CriteresDeSelectionFond_Av	; Affichage des champs
1		cpt			1,0		; Compteur
Beginp
	XmeSetAttribut('F_ETB'	, AN_VISIBILITE, Condition(Soc_Gerer_Etablissements, AV_VISIBLE, AV_CACHE))
	XmeSetAttribut('F_DEPOT', AN_VISIBILITE, Condition(Soc_Gerer_Depots			, AV_VISIBLE, AV_CACHE))
	XmeSetAttribut('F_LIEU'	, AN_VISIBILITE, Condition(Soc_Gerer_Emplacements	, AV_VISIBLE, AV_CACHE))
	XmeSetAttribut('F_RESA'	, AN_VISIBILITE, Condition(Soc_Gerer_Reservation	, AV_VISIBLE, AV_CACHE))

	For cpt = 1 to Index(SOC.StatLib(), 1)
		XmeSetAttribut([ 'F_STAT' ToString(cpt) ]	, AN_VISIBILITE, Condition(SOC.StatLib(cpt)	= ' ', AV_CACHE, AV_VISIBLE)) 
	Next
	For cpt = 1 to Index(SOC.FamLib(), 1)
		XmeSetAttribut([ 'F_FAM' ToString(cpt) ]	, AN_VISIBILITE, Condition(SOC.FamLib(cpt)	= ' ', AV_CACHE, AV_VISIBLE)) 
	Next
	For cpt = 1 to Index(CA.AxeLib(), 1)
		XmeSetAttribut([ 'F_AXE' ToString(cpt) '_ENT' ]	, AN_VISIBILITE, Condition(CA.AxeLib(cpt)	= ' ', AV_CACHE, AV_VISIBLE)) 
		XmeSetAttribut([ 'F_AXE' ToString(cpt) '_LIG' ]	, AN_VISIBILITE, Condition(CA.AxeLib(cpt)	= ' ', AV_CACHE, AV_VISIBLE)) 
	Next
Endp
