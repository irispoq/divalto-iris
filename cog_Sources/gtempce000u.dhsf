;*
Public Procedure Champ_Choix_1_Ap		; Après sélection d'un type de tiers
Beginp
	ValiderChoixMenu
	If GTPCE.TiCodOri in ('O', 'P')													; Que des devis pour les offres et prospects
		GTPCE.PicodOri = 1
	EndIf

	If GTPCE.TiCodOri in ('C', 'F') And GTPCE.PiCodOri = 2					; Commandes programmes
		XmeSetAttribut('F_CDEOUV', AN_VISIBILITE, AV_VISIBLE)
	Else
		XmeSetAttribut('F_CDEOUV', AN_VISIBILITE, AV_GRISE)
	EndIf

	If GTPCE.PicodOri = 1 Or GTPCE.TiCodOri not in ('C', 'F')				; Affichage contremarque
		XmeSetAttribut('CONTREMARQUE', AN_VISIBILITE, AV_GRISE)				;		Pas de contremarque pour les devis
	Else
		XmeSetAttribut('CONTREMARQUE', AN_VISIBILITE, AV_VISIBLE)			;		Traçage des autres pièces
	EndIf
	
	If GTPCE.TiCodOri = 'C' Or \														; Pièces client
		( GTPCE.TiCodOri = 'F' And GTPCE.PiCodOri <> 1 )						; Pièces fournisseur sauf devis
		GTPCE.Status = Charger_StatusPiece(GTPCE.TiCodOri, GTPCE.PiCodOri)
		XmeSetAttribut('F_STATUS', AN_VISIBILITE, AV_VISIBLE)
	Else
		XmeSetAttribut('F_STATUS', AN_VISIBILITE, AV_GRISE)
	EndIf
	GTPCE.TiCodDst = GTPCE.TiCodOri
Endp												 


