;*
Public Procedure Champ_PiNoD_1_Ap				; Après no pièce origine en duplication
Beginp
	Switch TRUE
	Case GTPCE.PiNoD <> 0 and Seek_ENT(GTPCE.TiCodOri, GTPCE.PiCodOri, GTPCE.PiNoD) <> 0	; Pièce d'origine inexistante
		Mes_Alert(2249, GTPCE.PiNoD) 
		Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
	Case ENT.AsCod = 2 																; Pièce associée
		Mes_Alert(2274) 
		Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
	
	Case MZ.PrgCdeFlg = 2 and ENT.PrgCdeFlg <> 2 							; Commande programme non ouverte
		Mes_Alert(1343, GTPCE.PiNoD) 
		Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
	
	Default
		GTPCE.TiersD = ENT.Tiers
		switch GTPCE.TiCodOri
		case 'C' |Controle_Client		(GTPCE.TiersD, 2000, TRUE, Today, TRUE) 
		case 'F' |Controle_Fournisseur(GTPCE.TiersD, 1500, TRUE, Today, TRUE)
		case 'I' |Controle_Tiers_Autre(GTPCE.TiersD, 1900, TRUE, Today, TRUE)
		endswitch
				

	EndSwitch


	
	switch	GTPCE.TiCodOri
	case	'C'		; recherche d'un client
		TIERSORI.Nom		= CLI.Nom
		TIERSORI.Vil		= CLI.Vil
		TIERSORI.CPostal	= CLI.CPostal
		GTENT.NOTE 		= CLI.NOTE
	case	'F'		; recherche d'un fournisseur
		TIERSORI.Nom		= FOU.Nom
		TIERSORI.Vil		= FOU.Vil
		TIERSORI.CPostal	= FOU.CPostal
		GTENT.NOTE 	= FOU.NOTE
	case	'P'		; recherche d'un prospect
		TIERSORI.Nom		= PRO.Nom
		TIERSORI.Vil		= PRO.Vil
		TIERSORI.CPostal	= PRO.CPostal
