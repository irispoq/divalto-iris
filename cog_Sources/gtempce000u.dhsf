Public Procedure Champ_BlAsDepo_1_Ap			; Contrôle dépôt destination en duplication
Beginp
	If Harmony.Key = K_F8
		if (not OP_TrtType_CessionInterEtb(T020_Ent))
			Zoom_Call(9866)
		else
			Zoom_Call(9057)
		endif
	endif
	if GTPCE.BLASDEPO = ' '
		GTPCE.BLASDEPO = MZ.DEPO
	endif
	
	GTENT.LIB(26) = ' '
 	if (not OP_TrtType_CessionInterEtb(T020_Ent)) and  (Controle_Depo(GTPCE.BLASDEPO,1117) = 0)
		GTENT.LIB(26) = T017.LIB
		T017_Dst	= T017
		Piece_Charger_Adresse (GTENT.ADRTB(),GTPCE.TiCodDst, GTPCE.PiCodDst,GTPCE.PiNoF,ENT.PIDT,GTENT.SENS,MajTour=true)
		if T017.Tiers <> ' '
			Seek_TIA(T017.Tiers)	
			ENT.TIERS 	 = TIA.Tiers
			GTPCE.TiersF = TIA.Tiers
			Tiers = TIA
		endif
	endif
	if (OP_TrtType_CessionInterEtb(T020_Ent)) and  (Controle_Depo_Autre_Etb(GTPCE.BLASDEPO,1117) = 0)
		GTENT.LIB(26) = T017.LIB
		T017_Dst	= T017
		Piece_Charger_Adresse (GTENT.ADRTB(),GTPCE.TiCodDst, GTPCE.PiCodDst,GTPCE.PiNoF,ENT.PIDT,GTENT.SENS,MajTour=true)
		Seek_TIA(T017.Tiers)	
		ENT.TIERS 	 = TIA.Tiers
		GTPCE.TiersF = TIA.Tiers
		Tiers = TIA
	endif

Endp																												 


