Public Procedure ValiderChoixMenu
Beginp
	XmeMenuValidateItem(IdMenuPopZoomTiers, 'ZCLI_C', FALSE, FALSE)
	XmeMenuValidateItem(IdMenuPopZoomTiers, 'ZFOU_C', FALSE, FALSE)
	XmeMenuValidateItem(IdMenuPopZoomTiers, 'ZTIA_C', FALSE, FALSE)
	XmeMenuValidateItem(IdMenuPopZoomTiers, 'ZCLI_F', FALSE, FALSE)
	XmeMenuValidateItem(IdMenuPopZoomTiers, 'ZFOU_F', FALSE, FALSE)
	XmeMenuValidateItem(IdMenuPopZoomTiers, 'ZTIA_F', FALSE, FALSE)
	XmeMenuValidateItem(IdMenuPopZoomTiers, 'ZCLI_I', FALSE, FALSE)
	XmeMenuValidateItem(IdMenuPopZoomTiers, 'ZFOU_I', FALSE, FALSE)
	XmeMenuValidateItem(IdMenuPopZoomTiers, 'ZTIA_I', FALSE, FALSE)
	XmeMenuValidateItem(IdMenuPopZoomTiers, [ 'ZCLI_' GTPCE.TiCodOri ], TRUE, FALSE)
	XmeMenuValidateItem(IdMenuPopZoomTiers, [ 'ZFOU_' GTPCE.TiCodOri ], TRUE, FALSE)
	XmeMenuValidateItem(IdMenuPopZoomTiers, [ 'ZTIA_' GTPCE.TiCodOri ], TRUE)
Endp

;*
Public Function Int ChoixTiers
; Ouvre le zoom tiers adéquat
; Renvoie :	0 si Ok
;				-1 sinon
1		Rz			5,0																; Résultat du zoom

Beginf
	If Harmony.Key = K_F8
		Switch GTPCE.TiCodOri
		Case 'P'	|	Rz = Zoom_Call(9023)			; Prospect
		Case 'F'	|	Rz = Zoom_Call(9022)			; Fournisseur
		Case 'I'	|	Rz = Zoom_Call(9025)			; Tiers interne
		Default	|	Rz = Zoom_Call(9021)			; Client
		EndSwitch
		If Rz <> E_ZOOM_OK
			FReturn(-1)
		EndIf
	EndIf
	If GTPCE.TiCodOri = 'O' And Harmony.Key = K_SF8 And Zoom_Call(9023) <> E_ZOOM_OK
		FReturn(-1)
	EndIf

	FReturn(0)
Endf

;*
Public Function Int ChoixTiersStock
; Ouvre le zoom tiers adéquat
; Renvoie :	0 si Ok
;				-1 sinon
1		Rz			5,0																; Résultat du zoom

Beginf
	If 	(	Harmony.Key = K_F8 		And GTPCE.TiCodOri = 'C'				) Or \
			(	Harmony.Key = K_SF8		And GTPCE.TiCodOri in ('F', 'I')	)
		Rz = Zoom_Call(9021)
	ElsIf	(	Harmony.Key = K_F8 		And GTPCE.TiCodOri = 'F'				) Or \
			(	Harmony.Key = K_SF8		And GTPCE.TiCodOri = 'C'				) Or \
			(	Harmony.Key = K_CTRL_F8	And GTPCE.TiCodOri = 'I'				)
		Rz = Zoom_Call(9022)
	ElsIf	(	Harmony.Key = K_F8 		And GTPCE.TiCodOri = 'I'				) Or \
			(	Harmony.Key = K_CTRL_F8	And GTPCE.TiCodOri in ('C', 'F')	)
		Rz = Zoom_Call(9025)
	EndIf

	FReturn( Condition(Rz = E_ZOOM_OK, 0, -1) )
Endf

;*
Public Procedure ControleTiers(&CodeTiers)
1		CodeTiers		>GTFDD.dhsd Tiers
Beginp
	If Harmony.Key = K_F8
		Switch GTPCE.TiCodOri
		Case 'F'	|	Zoom_Call(9022)
		Case 'I'	|	Zoom_Call(9025)
		Case 'P'	|	Zoom_Call(9023)
		Default	|	Zoom_Call(9021)
		EndSwitch
	ElsIf Harmony.Key = K_SF8 And GTPCE.TiCodOri = 'O'
		Zoom_Call(9023)
	Else
		If Left(CodeTiers, 1) = '-'
			; Recherche par nom
			Switch GTPCE.TiCodOri
			Case 'C'	|	Controle_Client(CodeTiers)
			Case 'F'	|	Controle_Fournisseur(CodeTiers)
			Case 'I'	|	Controle_Tiers_Autre(CodeTiers)
			Case 'P'	|	Controle_Prospect(CodeTiers)
			Default														; On effectue 2 recherches : 
				If Controle_Prospect(CodeTiers) <> 0			;		- prospect,
					Controle_Client(CodeTiers)						;		- puis client si la 1ère échoue.
				EndIf

			EndSwitch
		EndIf
	EndIf
Endp

;*
Public Procedure AppelZoom
Beginp
	If Harmony.Key = K_F8					; Type de tiers
		G3XZ.TiCod		= Condition(GTPCE.TiCodOri = 'O', 'C', ToString(GTPCE.TiCodOri))
	ElsIf Harmony.Key = K_SF8 And GTPCE.TiCodOri = 'O'
		G3XZ.TiCod		= 'P'
	EndIf
	Zoom_Call(9864)
Endp


