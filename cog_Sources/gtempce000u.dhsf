;*
Public Procedure Champ_PiNo_1_Ap						; après no pièce en création
1		indicePiece		2,0
Beginp
	indicePiece = ETS_IndicePiece(GTPCE.TiCodDst, GTPCE.PiCodDst)
	If GTPCE.PiNoF <> 0 
		If (GTPCE.PiNoF < 0)		
			If MZ.Etb = ' '
				Mes_Alert(2307, GTPCE.PiNoF)
			Else
				Mes_Alert(2219, Condition(ETS.PiNo1(indicePiece) > 0, ETS.PiNo1(indicePiece), 1), ETS.PiNo2(indicePiece)) 
			EndIf
			Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
		Elsif Seek_ENT(' ', ' ', GTPCE.PiNoF) = 0	 and LireProgrammeFonction <> C_Fonction_Validation
			Mes_Alert(2200, GTPCE.PiNoF)
			Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
		Elsif MZ.ETB <> ' ' And Soc_Gerer_Compteurs_Etablissement And (GTPCE.PiNoF < ETS.PiNo1(indicePiece) Or GTPCE.PiNoF > ETS.PiNo2(indicePiece))
			Mes_Alert(2219, Condition(ETS.PiNo1(indicePiece) > 0, ETS.PiNo1(indicePiece), 1), ETS.PiNo2(indicePiece)) 
			Harmony.Retour = XMENEXT_TRAITEMENT_AVANT		
		EndIf
	EndIf
	G3XZ.TIERS = ' '
Endp																

