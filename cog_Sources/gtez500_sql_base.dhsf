include 'GTTC000.dhsp'

Module A5TM000.dhop

public hfile	gtfdd.dhsd	GTFTAB	GTFTAB
public hfile	gtfdd.dhsd	GTFPCF	GTFPCF	
public hfile 	gtfdd.dhsd	GTFDOS	GTFDOS

public record	gtfdd.dhsd	SocAfr
public record	gtfdd.dhsd	CLI
public record	gtfdd.dhsd	PRO
