public record	gtfdd.dhsd	T064									; classe d'affaire

public record 	gtfdd.dhsd	G7
public record  gtfdd.dhsd 	XZ	G3XZ
public record	gtfdd.dhsd	SOC
public record	gtfdd.dhsd	ETS

public record	a5dd.dhsd		MZ

public record  a5dd.dhsd		XZ		A5XZ
Public Record "ddsys.dhsd" rowinfo zoomselection
Public Record "gafdd.dhsd" gachc gachc
Public RecordSql "gtrsproj.dhoq" Affaire affaire
Public Record "gtfdd.dhsd" prjap prjap_sel
Public Record "gtfdd.dhsd" t064 t064_sel
Public Record "gtfdd.dhsd" cli cli_sel
Public Record "gafdd.dhsd" gabmp gabmp

Const	C_Pilote_ParTiers	= 1
Const C_Pilote_ParClasse = 2
Const	C_Pilote_Modif    = 3
Const C_Pilote_Creation = 4
Const C_Pilote_UnProjet = 5


;*
;* séquences liées au masque
;*

;*
public procedure Champ_Adrcod_1_Av				;initialise g3xz.tiers pour appel du zoom
beginp
	if affaire.AdrTiers = ' '
		G3XZ.Tiers = Affaire.Tiers
	else
 		G3XZ.Tiers = Affaire.AdrTiers
	endif
	if G3XZ.Tiers = ' '
		; pas de tiers saisie impossible
		Harmony.Retour = XMENEXT_SAUT_DONNEE
	endif

endp

;*
public procedure Champ_Adrcod_1_Ap			;controle code adresse
beginp
	G7.Adr(1) = ' '
	if affaire.AdrTiers = ' '
		G3XZ.Tiers = affaire.Tiers
		Recherche_Adresse(affaire.Tiers, affaire.AdrCod, 2008)
		G7.Adr(1) = T1.Adr
	else
 		G3XZ.TIERS = affaire.AdrTiers
		Recherche_Adresse(affaire.AdrTiers, affaire.AdrCod, 2008)
		G7.Adr(1) = T1.Adr
	endif
endp


;*
public procedure Champ_AfrClas_1_Av	 ; avant classe
beginp
 
	if G7.ZoomPar = C_Pilote_ParClasse 	; pilotage par classe
		Harmony.Retour = XMENEXT_SAUT_DONNEE
		preturn
	endif

	if affaire.AffairePrinc <> ' '
		Harmony.Retour = XMENEXT_SAUT_DONNEE
	endif

EndP


;*
Public Procedure Champ_AfrClas_1_Ap	  ; Controle la classe affaire
Beginp

	affaire.LibClasseAffaire = ' '

	if affaire.AfrClas = ' '
		; si non renseignée
		affaire.AfrClas =  Condition(Ets.AfrClas = ' ', SocAfr.AfrClas, Ets.AfrClas)	
	elsif (Harmony.DataModif = True)
		if (Mes_Alert(5014, affaire.Affaire, affaire.AfrClas) <> IdYes)
			Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
			preturn
		endif
	endif

	if LecTab(64, affaire.AfrClas, 1164, today) = 0
		affaire.LibClasseAffaire = T064.Lib80
	endif

Endp

;*
public procedure Champ_AfrClas_2_Av	 ; avant classe  clé
beginp

	if G7.ZoomPar = C_Pilote_ParClasse 	; pilotage par classe
		Harmony.Retour = XMENEXT_SAUT_DONNEE
	endif

endp

;*
Public Procedure Champ_AfrClas_2_Ap	  		; Controle la classe affaire  clé
Beginp

	if LecTab(64, T064_sel.AfrClas, 1164) = 0
		T064_sel = T064
	else
		T064_sel.Lib80 = ' '
	endif

EndP


;*
public procedure Champ_Conf_1_Ap				; contrôle code de confidentialité
beginp
	Controler_Confidentialite(affaire.Conf , 4000)
endp

;*
public procedure Champ_Contact_1_Av	 		; avant contact client interdire saisie si code client non renseigné
beginp
	if affaire.Tiers = ' '
		Harmony.Retour = XMENEXT_SAUT_DONNEE
	else
		G3XZ.Tiers = affaire.Tiers
		G3XZ.TiCod = 'C'
	endif
EndP

;*
public procedure Champ_Contact_1_Ap	 		; controle  contact client
beginp
	affaire.NomContact = ' '
	if Controle_Contact(affaire.Tiers, affaire.Contact, 2016) = 0
		affaire.NomContact = T2.Nom
	endif
endp

;*
public procedure Champ_Etat_1_Ap				; initialisation en fonction de l'état
BeginP
	if Harmony.DataModif
		affaire.ProjetEtatDt = Today
	endif
EndP

;*
public procedure Champ_Etb_1_Av ; avant établissement
beginp
	if G7.ZoomPar = C_Pilote_ParTiers 	; pilotage par tiers
		Harmony.Retour = XMENEXT_SAUT_DONNEE
		preturn
	endif

	if SOC.EntCodN(22) = 1
		prjap_sel.Etb = ' '
		Harmony.Retour = XMENEXT_SAUT_DONNEE
	endif		 
endp

;*
public procedure Champ_Etb_1_Ap	 ; controle établissement
beginp
	if Controle_Ets (prjap_sel.Etb, 1400) = 0
		if prjap_sel.Etb <> ' '
			MZ.Etb = prjap_sel.Etb
		endif
	endif
endp

;*
Public Procedure Champ_Lib_1_Av			; maj du libellé abrégé / libelle
1	LibAbr	>gtfdd.dhsd	LibAbr
Beginp
	LibAbr = Convertir_Nom_Nomabr(affaire.Lib80,true)
	if LibAbr = affaire.LibAbr
		affaire.LibAbr = ' '
	endif 
Endp


;*
Public Procedure Champ_Lib_1_Ap			; maj du libelle abrégé / libelle
Beginp
	if affaire.LibAbr = ' '
		affaire.LibAbr = Convertir_Nom_Nomabr(affaire.Lib80)
	endif	
Endp

;*
public procedure Champ_LibAbr_1_Av			; en création le libellé abrége n'est pas en saisie
beginp
	if G7.ModifFlg = 0
		Harmony.Retour = XMENEXT_SAUT_DONNEE
	endif	
	G7.ModifFlg = 1
endp


;*
Public Procedure Champ_LibAbr_1_Ap			;après libelle abrégé si non renseigné
Beginp
	if affaire.LibAbr = ' ' 
		affaire.LibAbr = Convertir_Nom_Nomabr(affaire.Lib80)
	endif	
	affaire.LibAbr = Convertir_Nom_Nomabr(affaire.LibAbr)
Endp


;*
public procedure Champ_Question_1_Av	; Initialisation pour pilotage du zoom
beginp
	A5XZ.Entite = C_Entite_PrjAp
endp

;*
public procedure Champ_Question_1_Ap	; Contrôle le code questionnaire s'il est renseigné
beginp
	Controler_questionnaire (affaire.Question, 3000, entite = C_Entite_PrjAp) 
endp

;*
public procedure Champ_Repr_1_Ap	 		; controle  représentant
beginp
	affaire.NomCommercial = ' '
	if Controle_Vrp(affaire.Repr,1700) = 0
		affaire.NomCommercial = VRP.Nom
	endif
endp

;*
public procedure Champ_Salcod_1_Ap	 		; controle  salarié chef de projet
beginp
	affaire.NomCdpInterne = ' '
	if Lectab(37, affaire.SalCod, 1137) = 0
		affaire.NomCdpInterne = T037.Nom
	endif
endp

;*
public procedure Champ_Tiers_1_Av	 ; avant tiers
beginp
;	if G7.zoompar = C_Pilote_ParTiers 	; pilotage par tiers
;		Harmony.Retour = Xmenext_Saut_Donnee
;	endif
EndP

;*
public procedure Champ_Tiers_1_Ap	 ; controle tiers
beginp
	affaire.NomClient = ' '

	if affaire.Tiers = ' '
		; pas de gestion d'adresse
		affaire.AdrTiers = ' '
		affaire.AdrCod	= ' '
		G7.Adr(1) = ' '
		affaire.NomTiersAdresse = ' '
		XmeSetAttribut("tiers", AN_STYLE, "CHAMP_NON_SAISI")
	elsif Controle_Client (affaire.Tiers, 2000, ctrvisa = false)  = 0	; on accepte client uniquement
		affaire.NomClient = CLI.Nom
		if affaire.Repr = ' '
			affaire.Repr = CLI.Repr(1)
			Recherche_Vrp(affaire.Repr)
			affaire.NomCommercial = VRP.NOM
		endif
		XmeResetAttribut("tiers", AN_TOUS_ATTRIBUTS)
	endif
endp

;*
public procedure Champ_Tiers_2_Av	 ; avant tiers	clé 
beginp
;; appel 312219 => pas de pilotage dur sur le tiers.
;	if G7.zoompar = C_Pilote_ParTiers 	; pilotage par tiers
;		Harmony.Retour = Xmenext_Saut_Donnee
;	endif
EndP

;*
public procedure Champ_Tiers_2_Ap	 ; controle tiers clé
beginp
	if Controle_Client(cli_sel.Tiers, 2000, ctrvisa = false)  = 0	; on accepte client uniquement
		cli_sel = CLI
	else
		cli_sel.Nom = ' '
	endif
endp

;*
public procedure Champ_Tiers_3_Av			; avant tiers adresse
beginp
	if affaire.Tiers = ' '
		Harmony.Retour = XMENEXT_SAUT_DONNEE
	endif
endp


;*
public procedure Champ_Tiers_3_Ap			; controle tiers adresse
1	tiers		>gtfdd.dhsd tiers
beginp
	affaire.NomTiersAdresse = ' '

	if (Controle_Client(affaire.AdrTiers, 2000, 0)  <> 0)
		preturn
	endif

	affaire.NomTiersAdresse = CLI.Nom

	G7.Adr(1) = ' '
	if affaire.AdrTiers = ' '
		tiers = affaire.Tiers
	else
		tiers = affaire.AdrTiers
	endif

	if Recherche_Adresse(tiers, affaire.AdrCod, 2008) <> 0
		affaire.AdrCod = ' '
		preturn
	endif

	G7.Adr(1) = T1.Adr

endp

