;*
Public Procedure Champ_Ref_1_Ap
;
Beginp
	If Controle_Article(CogRs_UT108.Ref,2100) = 0
		CogRs_UT108.ART_DES = ART.Des
		If ART.Cog_PdvFa <> 2
			MessageBox("Cette référence n'est pas paramétrée pour la facturation PDV.","Alerte", MB_ICONEXCLAMATION)
			Harmony.Retour = XMENEXT_RESSAISIE
		EndIf
	EndIf
Endp
