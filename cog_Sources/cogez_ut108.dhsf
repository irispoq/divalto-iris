;*
Public Procedure Champ_Cog_Date_Debut_1_Av
;
Beginp
	If Not Zoom.Action in (ZOOM_CREATION,ZOOM_DUPLICATION)
 		Harmony.Retour = XMENEXT_SAUT_DONNEE
	Endif
Endp


;*
Public Procedure Champ_Cog_Date_Fin_1_Ap
;
Beginp
	If CogRs_UT108.Cog_Date_Fin < CogRs_UT108.Cog_Date_Debut
		Harmony.Retour = Xmenext_Ressaisie
	Endif
Endp


;*
Public Procedure Champ_Cog_Date_Fin_1_Av
;
Beginp
	If CogRs_UT108.Cog_Duree_Limitee = 2 Or CogRs_UT108.Cog_Nombre_Cotisation > system.esp
		Harmony.Retour = XMENEXT_SAUT_DONNEE
	Else
		CogRs_UT108.Cog_Date_Fin = Hdate(CogRs_UT108.Cog_Date_Fin,"p")
	Endif
Endp


;*
Public Procedure Champ_Cog_Nombre_Cotisation_1_Ap
;
Beginp
	If CogRs_UT108.Cog_Duree_Limitee = 2 And CogRs_UT108.Cog_Nombre_Cotisation = system.esp
 		Harmony.Retour = XMENEXT_RESSAISIE
	ElsIf Zoom.Action in (ZOOM_CREATION,ZOOM_DUPLICATION) And CogRs_UT108.Cog_Nombre_Cotisation > system.esp
		CogRs_UT108.Cog_Date_Fin = system.esp
	Endif
Endp


;*
Public Procedure Champ_Cog_Nombre_Cotisation_1_Av
;
Beginp
	If Not Zoom.Action in (ZOOM_CREATION,ZOOM_DUPLICATION)
 		Harmony.Retour = XMENEXT_SAUT_DONNEE
	Endif
Endp


