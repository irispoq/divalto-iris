;*
Public Procedure Champ_Cpt_1_Ap		; controle compte comptable
Beginp
	FOURNISSEUR.CompteFournisseur_Lib = ' '
	If FOURNISSEUR.Tiers = ' ' and FOURNISSEUR.Cpt = '********'
		; pas de controle car affectation automatique
	Else
		If FOURNISSEUR.CPT <> ' '
				If SOC.EntCodN(20) = 2
					If SOC.ENTCOD(14) not in (2,3)
						If Controle_Compte(FOURNISSEUR.Cpt, conf=FALSE, cptnat=4) <= 1		; Controle demandé et pas de création automatique
							FOURNISSEUR.CompteFournisseur_Lib = C3.Lib
						EndIf
					ElsIf Controle_Compte(FOURNISSEUR.Cpt, existence=FALSE, conf=FALSE, cptnat=4) <= 1
						; Création automatique uniquement contrôle syntaxique et classe
						FOURNISSEUR.CompteFournisseur_Lib = C3.Lib
					EndIf
				Else		
					Controle_Compte (FOURNISSEUR.Cpt, nonvide=FALSE, existence=FALSE)  ; alphanumérique + complétion
				EndIf
		else
			if FOURNISSEUR.Cpt =' '
				Harmony.Retour = XMENEXT_TRAITEMENT_AVANT	
				preturn
			endif

		EndIf 
	EndIf
Endp

