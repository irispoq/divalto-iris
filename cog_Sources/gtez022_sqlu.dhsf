;*
;* séquences liées au masque
;*

;*
Public Procedure Champ_PositionnerTicod
Beginp
	G3XZ.TiCod = 'F'
Endp

;*
Public Procedure Champ_Adrcod_1_Av				;initialise g3xz.tiers pour appel du zoom
Beginp
	G3XZ.Tiers = Condition(FOURNISSEUR.AdrTiers(Harmony.DataInd1) = ' ', FOURNISSEUR.Tiers, \
						 																		FOURNISSEUR.AdrTiers(Harmony.DataInd1) )
Endp

;*
Public Procedure Champ_Adrcod_1_Ap			; Contrôle code adresse
Beginp
	Switch Harmony.Dataind1
	Case 1	|	FOURNISSEUR.AdresseDvNom = ' '		|	FOURNISSEUR.AdresseDvVil = ' '
	Case 2	|	FOURNISSEUR.AdresseCdNom = ' '		|	FOURNISSEUR.AdresseCdVil = ' '
	Case 3	|	FOURNISSEUR.AdresseBLNom = ' '		|	FOURNISSEUR.AdresseBLVil = ' '
	Case 4	|	FOURNISSEUR.AdresseFaNom = ' '		|	FOURNISSEUR.AdresseFaVil = ' '
	EndSwitch

	G3XZ.Tiers = Condition(	FOURNISSEUR.AdrTiers(Harmony.Dataind1) = ' ',	FOURNISSEUR.Tiers, \
																									FOURNISSEUR.AdrTiers(Harmony.Dataind1 ) )
	Recherche_Adresse(G3XZ.Tiers, FOURNISSEUR.AdrCod(Harmony.Dataind1), 2008)

	Switch Harmony.Dataind1
	Case 1	|	FOURNISSEUR.AdresseDvNom = T1.Nom	|	FOURNISSEUR.AdresseDvVil = T1.Vil
	Case 2	|	FOURNISSEUR.AdresseCdNom = T1.Nom	|	FOURNISSEUR.AdresseCdVil = T1.Vil
	Case 3	|	FOURNISSEUR.AdresseBLNom = T1.Nom	|	FOURNISSEUR.AdresseBLVil = T1.Vil
	Case 4	|	FOURNISSEUR.AdresseFaNom = T1.Nom	|	FOURNISSEUR.AdresseFaVil = T1.Vil
	EndSwitch
Endp

;*
Public Procedure Champ_Blmod_1_Ap			;controle mode de transport
Beginp
	FOURNISSEUR.ModeTransport_Lib = Condition(Lectab(26, FOURNISSEUR.BlMod, 1126) = 0, T026.Lib, ' ')
Endp

;*
Public Procedure Champ_BqcA_1_Ap				; Contrôle de la banque par défaut
Beginp
	FOURNISSEUR.CptBanqueDefaut_RibDom = Condition(Lectab_D2(FOURNISSEUR.BqcA, 2205) = 0, D2.RibDom, ' ')
Endp

;*
Public Procedure Champ_COECOD_1_Ap			; contrôle coefficient transport
Beginp
	Lectab(10, FOURNISSEUR.TvaBlCoe, 1110)
Endp

;*
Public Procedure Champ_Conf_1_Ap				; contrôle code de confidentialité
Beginp
	Controler_Confidentialite(FOURNISSEUR.Conf, 4000)
Endp

;*
Public Procedure Champ_Cpostal_1_Av			; pour appel de zoom
Beginp
	G3XZ.Pong = TRUE
Endp

;*
Public Procedure Champ_Cpostal_1_Ap			; recherche de la ville à partir du code postal
Beginp
	If Harmony.Key = K_CTRL_S
		FOURNISSEUR.Cpostal	= G3XZ.Cpostal
		FOURNISSEUR.Vil		= G3XZ.Vil
	EndIf
