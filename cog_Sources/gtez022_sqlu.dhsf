;*
Public Procedure Champ_TIERS_4_Ap			; Contrôle tiers adresse
1		tiers		>GTFDD.dhsd Tiers			; Tiers
1		x			2,0							; Code retour de fonction
1		nom		>GTFDD.dhsd Nom			; Nom
Beginp
	Switch Harmony.Dataind1
	Case 1	|	FOURNISSEUR.TiersAdresseDv_Nom	= ' '
	Case 2	|	FOURNISSEUR.TiersAdresseCde_Nom	= ' '
	Case 3	|	FOURNISSEUR.TiersAdresseBL_Nom	= ' '
	Case 4	|	FOURNISSEUR.TiersAdresseFac_Nom	= ' '
	EndSwitch

	Tiers = FOURNISSEUR.AdrTiers(Harmony.Dataind1)
	PingLocal('ZOOM_FOU', FOU)
	If ( x := Controle_Fournisseur(tiers, 0, 0) ) = 0	|	nom = FOURNISSEUR.Nom
	ElsIf x = 1														|	nom = ' '
		Mes_Alert(2004, tiers)
		Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
	ElsIf Controle_Client(tiers, 0, 0) = 0					|	nom = CLI.Nom
	ElsIf Controle_Tiers_Autre(tiers, 0, 0) = 0			|	nom = TIA.Nom
	Else																|	nom = ' '
		Mes_Alert(1900, tiers)
		Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
	EndIf
	
	Switch Harmony.Dataind1
	Case 1	|	FOURNISSEUR.TiersAdresseDv_Nom	= nom
	Case 2	|	FOURNISSEUR.TiersAdresseCde_Nom	= nom
	Case 3	|	FOURNISSEUR.TiersAdresseBL_Nom	= nom
	Case 4	|	FOURNISSEUR.TiersAdresseFac_Nom	= nom
	EndSwitch
	PingLocalReceive('ZOOM_FOU', FOU)
 	FOURNISSEUR.AdrTiers(Harmony.Dataind1) = Tiers 
Endp

;*
Public Procedure Champ_Tiers_5_Ap			; controle code tiers si création automatique a échouée
Beginp
	If Controle_Syntaxe_Tiers (FOURNISSEUR.Tiers, 2004) <> 0
		PReturn
	EndIf

	If Res_FOU(FOURNISSEUR.Tiers, 0, NoWait)  <> 0
		Mes_Alert(1901, FOURNISSEUR.Tiers)
		Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
		PReturn
	EndIf

	PingLocal('ZOOM_FOU', FOU)
	If Seek_Tiers(FOURNISSEUR.Tiers) = 0 	; recherche tout tiers confondu car code doit être unique
		PingLocalReceive('ZOOM_FOU', FOU)
		Lib_FOU(FOURNISSEUR.Tiers)
		Mes_Alert(1511, FOURNISSEUR.Tiers)
		Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
	EndIf
	PingLocalReceive('ZOOM_FOU', FOU)	
Endp

