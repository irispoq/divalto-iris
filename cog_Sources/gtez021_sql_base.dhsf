	If CLIENT.Tiers = ' '
		If Mes_Alert(2009,bouton = MB_OKCANCEL+MB_ICONQUESTION) <> IDOK
			Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
		EndIf
	Else
		Controle_Syntaxe_Tiers(CLIENT.Tiers, 2004) 
	
		If SOC.EntCodN(22) = 2 And SOC.EntCodN(24) = 2	; gère-t-on les établissements ?
			If CLIENT.Tiers < ETS.Tiers1(2) Or CLIENT.Tiers > ETS.Tiers2(2)
				Mes_Alert(2006, MZ.Etb, ETS.Tiers1(2), ETS.Tiers2(2))
				Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
			EndIf
		EndIf
	EndIf
Endp

