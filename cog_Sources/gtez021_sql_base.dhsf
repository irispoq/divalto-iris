;*
Public Procedure Champ_TIERS_4_Ap			; controle Tiers adresse
1	Tiers		>GTFDD.dhsd Tiers
Beginp
	Switch Harmony.Dataind1
	Case 1	|	CLIENT.TiersAdresseDv_Nom	= ' '
	Case 2	|	CLIENT.TiersAdresseCde_Nom	= ' '
	Case 3	|	CLIENT.TiersAdresseBL_Nom	= ' '
	Case 4	|	CLIENT.TiersAdresseFac_Nom	= ' '
	Case 5	|	CLIENT.TiersAdresseRlv_Nom	= ' '
	EndSwitch

	Tiers = CLIENT.AdrTiers(Harmony.Dataind1)		; enregistrement cli est écrasé par controle_CLIENT							
	PingLocal('ZOOM_CLI', CLI)
	If Controle_CLIENT(Tiers, 2000, 0) = 0
		Switch Harmony.Dataind1
		Case 1	|	CLIENT.TiersAdresseDv_Nom	= CLIENT.Nom
		Case 2	|	CLIENT.TiersAdresseCde_Nom	= CLIENT.Nom
		Case 3	|	CLIENT.TiersAdresseBL_Nom	= CLIENT.Nom
		Case 4	|	CLIENT.TiersAdresseFac_Nom	= CLIENT.Nom
		Case 5	|	CLIENT.TiersAdresseRlv_Nom	= CLIENT.Nom
		EndSwitch
		if Client.ADRCOD(Harmony.DataInd1) <> '' and Recherche_Adresse(TIERS,Client.ADRCOD(Harmony.DataInd1),0) <> 0
			
			Switch Harmony.Dataind1
			Case 1	|	CLIENT.AdresseDvNom = ' '	|	CLIENT.AdresseDvVil = ' '
			Case 2	|	CLIENT.AdresseCdNom = ' '	|	CLIENT.AdresseCdVil = ' '
			Case 3	|	CLIENT.AdresseBLNom = ' '	|	CLIENT.AdresseBLVil = ' '
			Case 4	|	CLIENT.AdresseFaNom = ' '	|	CLIENT.AdresseFaVil = ' '
			Case 5	|	CLIENT.AdresseRvNom = ' '	|	CLIENT.AdresseRvVil = ' '
			EndSwitch
		else
			Switch Harmony.Dataind1
			Case 1	|	CLIENT.AdresseDvNom = T1.Nom	|	CLIENT.AdresseDvVil = T1.Vil
			Case 2	|	CLIENT.AdresseCdNom = T1.Nom	|	CLIENT.AdresseCdVil = T1.Vil
			Case 3	|	CLIENT.AdresseBLNom = T1.Nom	|	CLIENT.AdresseBLVil = T1.Vil
			Case 4	|	CLIENT.AdresseFaNom = T1.Nom	|	CLIENT.AdresseFaVil = T1.Vil
			Case 5	|	CLIENT.AdresseRvNom = T1.Nom	|	CLIENT.AdresseRvVil = T1.Vil
			EndSwitch
	
		endif
	EndIf
	PingLocalReceive('ZOOM_CLI', CLI)
 	CLIENT.AdrTiers(Harmony.Dataind1) = Tiers 
	
	Tiers = Condition(	CLIENT.AdrTiers(Harmony.Dataind1) = ' ', 	CLIENT.Tiers, \
																							CLIENT.AdrTiers(Harmony.Dataind1 ) )
Endp

