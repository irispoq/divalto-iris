Public Procedure Champ_Bljr_1_Ap			;controle jour de livraison
1	i	2,0
Beginp
	If CLIENT.BlJr <> ' '
		For i = 1 to Length(CLIENT.BlJr)
			If Mid(CLIENT.BlJr, i, 1) not in (' ', 1..7)
				Mes_Alert(2015)
				Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
				PReturn
			EndIf
		Next
	EndIf
Endp

