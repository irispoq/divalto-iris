Public Procedure Champ_Tiers_5_Ap			; controle code Tiers si création automatique a échouée
Beginp
	If Controle_Syntaxe_Tiers (CLIENT.Tiers, 2004) <> 0
		PReturn
	EndIf

	If Res_Cli(CLIENT.Tiers, 0, NoWait)  <> 0
		Mes_Alert(1901, CLIENT.Tiers)
		Harmony.Retour = XMENEXT_TRAITEMENT_AVANT 
		PReturn
	EndIf

	PingLocal('ZOOM_CLI', CLI)
	If Seek_Tiers(CLIENT.Tiers) = 0 	; recherche tout Tiers confondu car code doit être unique
		PingLocalReceive('ZOOM_CLI', CLI)
		Lib_Cli(CLIENT.Tiers)
		Mes_Alert(2011, CLIENT.Tiers)
		Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
	EndIf
	PingLocalReceive('ZOOM_CLI', CLI)
	
Endp

