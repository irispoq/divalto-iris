;*
Public Procedure Champ_Cpt_1_Ap		; controle compte comptable
Beginp
	CLIENT.CompteClient_Lib = ' '
	If CLIENT.Tiers = ' ' And CLIENT.Cpt = '********'
		; pas de controle car affectation automatique
	Else
		If CLIENT.Cpt <> ' '
			If SOC.EntCodN(20) = 2
				If SOC.EntCod(14) not in (1, 3)
					; controle demandé et pas de création automatique
					If Controle_Compte(CLIENT.Cpt, conf=FALSE, cptnat=3) <= 1
						CLIENT.CompteClient_Lib = C3.Lib
					EndIf
				Elsif Controle_Compte(CLIENT.Cpt, existence=FALSE, conf=FALSE, cptnat=3) <= 1
					; création automatique uniquement contrôle syntaxique et classe
					CLIENT.CompteClient_Lib = C3.Lib
				EndIf
			Else
				Controle_Compte(CLIENT.Cpt, nonvide=FALSE, existence=FALSE)  ; alphanumérique + complétion
			EndIf
		else
			if CLIENT.Cpt =' '
				Harmony.Retour = XMENEXT_TRAITEMENT_AVANT	
				preturn
			endif
		endif
	EndIf

Endp

