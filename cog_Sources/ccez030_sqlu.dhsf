1			OLD_CPTCOL		>CCFDD.dhsd		CPTCOL


;*
;*		traitements sur les données du masque
;*


;*
public procedure Champ_AxeSais_1_Av

; avant autorisation axe

beginp	; Champ_AxeSais_1_Av

	if CA.AxeLib(Harmony.DataInd1)	= ' '	\
	or Compte.CptTyp							<> 1

		Compte.AxeSais(Harmony.DataInd1)	= 1
		Compte.Axe(Harmony.DataInd1)		= ' '
		X1.Lib(Harmony.DataInd1 + 2)	= ' '
		Harmony.Retour = XmeNext_Saut_Donnee

	endif

endp		; Champ_AxeSais_1_Av


;*
public procedure Champ_AxeSais_1_Ap

; après autorisation axe

1			axeno			>CCFDD.dhsd	AXENO

beginp	; Champ_AxeSais_1_Ap

	if Harmony.DataModif or Harmony.DataNotif
		axeno = Harmony.DataInd1

		if Compte.AxeSais(axeno) <> 2	|	XmeSetAttribut([ "axesais" axeno ], AN_STYLE, C_Champ_NSaisi)
		else								|	XmeResetAttribut([ "axesais" axeno ], AN_TOUS_ATTRIBUTS)
		endif

		if Harmony.DataNotif
			Harmony.Retour = XmeNext_Debut_Donnee
			preturn
		endif
	endif

	if Compte.AxeSais(Harmony.DataInd1)	<> 2
		Compte.Axe(Harmony.DataInd1)		= ' '
		X1.Lib(Harmony.DataInd1 + 2)	= ' '
	endif

endp		; Champ_AxeSais_1_Ap


;*
public procedure Champ_Axe_2_Av

; avant code axe

beginp	; Champ_Axe_2_Av

	if CA.AxeLib(Harmony.DataInd1)	= ' '	\
	or Compte.CptTyp							<> 1	\
	or Compte.AxeSais(Harmony.DataInd1)	<> 2

		Compte.Axe(Harmony.DataInd1)		= ' '
		X1.Lib(Harmony.DataInd1 + 2)	= ' '
		Harmony.Retour = XmeNext_Saut_Donnee
	
	endif

	C3XZ.AxeNo = Harmony.DataInd1

endp		; Champ_Axe_2_Av


;*
public procedure Champ_Axe_2_Ap

; après code axe

1			axe			>CCFDD.dhsd	AXE					; pour éviter l'écrasement si code commence par '='

beginp	; Champ_Axe_2_Ap

   X1.Lib(Harmony.DataInd1 + 2) = ' '
   if Compte.Axe(Harmony.DataInd1) <> ' '
	
		axe = Compte.Axe(Harmony.DataInd1)
		if Controle_Axe(Harmony.DataInd1, axe, nonvide = false, syntaxe = false, creation = true, conf = false, axetyp = 1, etablissement = false) < 2
     		X1.Lib(Harmony.DataInd1 + 2) = C5.Lib
		endif

   endif

endp		; Champ_Axe_2_Ap


;*
public procedure Champ_CentEdi_1_Av

; avant centralisation édition

beginp	; Champ_CentEdi_1_Av

	if Compte.CptTyp <> 1
		Compte.CentEdi = 1
		Harmony.Retour = XmeNext_Saut_Donnee
	endif

endp		; Champ_CentEdi_1_Av


;*
public procedure Champ_CentTyp_1_Av

; avant centralisation journal

beginp	; Champ_CentTyp_1_Av

	if Compte.CptTyp <> 1
		Compte.CentTyp = 1
		Harmony.Retour = XmeNext_Saut_Donnee
	endif

endp		; Champ_CentTyp_1_Av


;*
public procedure Champ_Cpt_1_Av

; avant le numéro de compte sur clé de création

beginp	; Champ_Cpt_1_Av

	if ZOOM.Action = ZOOM_DUPLICATION
		Compte_AvSaisie = Compte.C3
	endif

endp		; Champ_Cpt_1_Av


;*
public procedure Champ_Cpt_1_Ap

; après le numéro de compte sur clé de création

beginp	; Champ_Cpt_1_Ap

	if Controle_Compte(Compte.Cpt, nonvide = condition(ZOOM.Action = ZOOM_CREATION or ZOOM.Action = ZOOM_DUPLICATION, true, false), existence = false, etablissement = false) = 0	\
	and ZOOM.Action = ZOOM_DUPLICATION
	
		switch Compte_AvSaisie.CptNat
		case vsw = 1 and left(Compte_AvSaisie.Cpt, condition(CA.BalRupt(1) <> 0, CA.BalRupt(1), 3))	 = left(Compte.Cpt, condition(CA.BalRupt(1) <> 0, CA.BalRupt(1), 3))
		case vsw = 2 and left(Compte_AvSaisie.Cpt, condition(CA.BalRupt(2) <> 0, CA.BalRupt(2), 3))	 = left(Compte.Cpt, condition(CA.BalRupt(2) <> 0, CA.BalRupt(2), 3))
		case vsw = 3 and left(Compte_AvSaisie.Cpt, condition(CA.BalRupt1(1) <> 0, CA.BalRupt1(1), 1)) = left(Compte.Cpt, condition(CA.BalRupt1(1) <> 0, CA.BalRupt1(1), 1))
		case vsw = 4 and left(Compte_AvSaisie.Cpt, condition(CA.BalRupt1(2) <> 0, CA.BalRupt1(2), 1)) = left(Compte.Cpt, condition(CA.BalRupt1(2) <> 0, CA.BalRupt1(2), 1))
		case vsw = 5 and left(Compte_AvSaisie.Cpt, condition(CA.BalRupt1(3) <> 0, CA.BalRupt1(3), 1)) = left(Compte.Cpt, condition(CA.BalRupt1(3) <> 0, CA.BalRupt1(3), 1))
		default
			Mes_Alert(19035)									; la classe et le groupe doivent être identiques
			Compte.Cpt		= Compte_AvSaisie.Cpt
			Harmony.Retour = XmeNext_Traitement_Avant
		endswitch
	
	endif
	
	Compte_AvSaisie.Cpt = Compte.Cpt
	Compte.C3			  = Compte_AvSaisie

endp		; Champ_Cpt_1_Ap


;*
public procedure Champ_CptCent_1_Av

; avant compte centralisateur

beginp	; Champ_CptCent_1_Av

	if Compte.CptTyp <> 1
		Compte.CptCent = ' '
		Harmony.Retour = XmeNext_Saut_Donnee
	endif

endp		; Champ_CptCent_1_Av


;*
public procedure Champ_CptCent_1_Ap

; après compte centralisateur

1			cpt			>CCFDD.dhsd	CPT
1			cptcent		>CCFDD.dhsd	CPTCENT

beginp	; Champ_CptCent_1_Ap

	Compte.Cpt_Centraliseur_Lib = ' '
	
	if Compte.CptCent <> ' '

		cpt		= Compte.Cpt
		cptcent	= Compte.CptCent

		if Controle_Compte(cptcent, creation = true, conf = false, cptnat = Compte.CptNat, etablissement = false) < 2			; attention, Controle_Compte retourne un C3 et non un recordSQL "Compte"
			Compte.Cpt_Centraliseur_Lib = C3.Lib
			if C3.CptNat < 3 and left(C3.Cpt, 2) <> left(cpt, 2)
				Mes_Alert(19036)							; compte pas de même groupe
				Harmony.Retour = XmeNext_Traitement_Avant
			endif
		endif

		Compte.CptCent = cptcent

	endif

endp		; Champ_CptCent_1_Ap


;*
public procedure Champ_CptCol_1_Av

; avant compte collectif

beginp	; Champ_CptCol_1_Av

	if Compte.CptNat < 3
		Compte.CptCol = ' '
		Harmony.Retour = XmeNext_Saut_Donnee
		preturn
	endif

;	if  Zoom.Action = ZOOM_MODIFICATION									\
;	and hread(CCFECR, C8, [ 'L' MZ.Dos Compte.Cpt ], 'F') = 0	\
;	and C8.Dos = MZ.Dos														\
;	and C8.Cpt = Compte.Cpt
;		Harmony.Retour = XmeNext_Saut_Donnee
;	endif

	OLD_CPTCOL = Compte.CptCol

endp		; Champ_CptCol_1_Av


;*
public procedure Champ_CptCol_1_Ap

; après compte collectif

1			cptcol		>CCFDD.dhsd	CPTCOL

beginp	; Champ_CptCol_1_Ap

	cptcol = Compte.CptCol
	Compte.Cpt_Collectif_Lib = ' '

	if Controle_Compte(cptcol, creation = true, conf = false, cptnat = 1, collectif = true, etablissement = false) < 2 and C3.CptTyp <> 2
		Mes_Alert(19034, cptcol)														; pas collectif
		Harmony.Retour = XmeNext_Traitement_Avant
	endif

	Compte.Cpt_Collectif_Lib = C3.Lib												; attention, Controle_Compte retourne un C3 et non un recordSQL "Compte"

	if  left(OLD_CPTCOL, 2) <> left(cptcol, 2)							\		; pas le même groupe (attention, on se limite volontairement à 2 caractères pour permettre le passage de "clients" à "clients douteux" par exemple)
	and hread(CCFECR, C8, [ 'L' MZ.Dos Compte.Cpt ], 'F') = 0		\		; il y a déjà au moins une écriture sur ce compte
	and C8.Dos = MZ.Dos															\
	and C8.Cpt = Compte.Cpt
		Mes_Alert(19801, OLD_CPTCOL, cptcol)										; modification interdite
		Harmony.Retour = XmeNext_Traitement_Avant
		Compte.CptCol = OLD_CPTCOL
	else
		Compte.CptCol = cptcol
	endif

endp		; Champ_CptCol_1_Ap


;*
public procedure Champ_CptCol_2_Ap

; avant collectif sur onglet "à partir de"

1			cpt			>CCFDD.dhsd	CPT

beginp	; Champ_CptCol_2_Ap

	cpt = Compte_Sel.CptCol
	Controle_Compte(cpt, nonvide = false, syntaxe = false, existence = false, etablissement = false)
	Compte_Sel.CptCol = cpt

endp		; Champ_CptCol_2_Ap


;*
public procedure Champ_CptNat_1_Av

; avant nature de compte

beginp	; Champ_CptNat_1_Av

	if string(left(Compte.Cpt, 1), "012345")
		Compte.CptNat = 1
		Compte.CptCol = ' '
		XmeSetAttribut("cptnat", AN_STYLE, C_Champ_NSaisi)
		Harmony.Retour = XmeNext_Saut_Donnee
		preturn

	elsif string(left(Compte.Cpt, 1), "6789")
		Compte.CptNat = 2
		Compte.CptCol = ' '
		XmeSetAttribut("cptnat", AN_STYLE, C_Champ_NSaisi)
		Harmony.Retour = XmeNext_Saut_Donnee
		preturn

	endif

	XmeResetAttribut("cptnat", AN_TOUS_ATTRIBUTS)

endp		; Champ_CptNat_1_Av


;*
public procedure Champ_CptNat_1_Ap

; après nature de compte

beginp	; Champ_CptNat_1_Ap

	if Harmony.DataModif or Harmony.DataNotif

		XmeResetAttribut("cptnataux",	 AN_TOUS_ATTRIBUTS)

		if Compte.CptNat > 2
			XmeSetAttribut("cptnataux", AN_STYLE, C_Champ_NSaisi)
		endif

		if Harmony.DataNotif
			Harmony.Retour = XmeNext_Debut_Donnee
			preturn
		endif

	endif

	if Compte.CptNat < 3

		Mes_Alert(19032)									; nature interdite
		Harmony.Retour = XmeNext_Traitement_Avant

	elsif Compte.Cpt < CA.AuxCpt1(Compte.CptNat - 2) or Compte.Cpt > CA.AuxCpt2(Compte.CptNat - 2)

		Mes_Alert(19033, CA.AuxCpt1(Compte.CptNat - 2), CA.AuxCpt2(Compte.CptNat - 2))
		Harmony.Retour = XmeNext_Traitement_Avant

	endif

endp		; Champ_CptNat_1_Ap


;*
public procedure Champ_CptTyp_1_Av

; avant type de compte

beginp	; Champ_CptTyp_1_Av

	if ZOOM.Action = ZOOM_MODIFICATION

		if		Compte.CptTyp = 2														\	; compte collectif
		and	hread(CCFJCA, XXC3, [ 'C' MZ.Dos Compte.Cpt ], 'F') = 0	\	; il y a un compte rattaché
		and	Compte.Dos = MZ.Dos													\
		and	Compte.Cpt = XXC3.CptCol

			Harmony.Retour = XmeNext_Saut_Donnee

		elsif Compte.CptTyp <> 2													\	; autre compte
		and	hread(CCFECR, C8, [ 'L' MZ.Dos Compte.Cpt ], 'F') = 0		\	; il y a une écriture
		and	C8.Dos = MZ.Dos														\
		and	C8.Cpt = Compte.Cpt

			Harmony.Retour = XmeNext_Saut_Donnee

		endif

	endif

endp		; Champ_CptTyp_1_Av


;*
public procedure Champ_CptTyp_1_Ap

; après type de compte

1			axeno			>CCFDD.dhsd	AXENO

beginp	; Champ_CptTyp_1_Ap

	if Harmony.DataModif or Harmony.DataNotif

		XmeResetAttribut("cpttyp2fstd", AN_TOUS_ATTRIBUTS)
		XmeResetAttribut("cpttyp2vgrise", AN_TOUS_ATTRIBUTS)
		XmeResetAttribut("cpttyp2vcache", AN_TOUS_ATTRIBUTS)
		XmeResetAttribut("cpttyp3fstd", AN_TOUS_ATTRIBUTS)
		XmeResetAttribut("cpttyp3vgrise", AN_TOUS_ATTRIBUTS)
		XmeResetAttribut("cpttyp3vcache", AN_TOUS_ATTRIBUTS)
		if CA.EtbCod = 2			; gestion multi-établissements
			XmeResetAttribut("etb_cpttyp2vgrise",	AN_TOUS_ATTRIBUTS)
			XmeResetAttribut("etb_cpttyp3vgrise",	AN_TOUS_ATTRIBUTS)
		endif

		switch Compte.CptTyp
		case 1
		case 2
			XmeSetAttribut("cpttyp2fstd", AN_STYLE, C_Champ_NSaisi)
			XmeSetAttribut("cpttyp2vgrise", AN_VISIBILITE, AV_GRISE)
			XmeSetAttribut("cpttyp2vcache", AN_VISIBILITE, AV_CACHE)
			if CA.EtbCod = 2			; gestion multi-établissements
				XmeSetAttribut("etb_cpttyp2vgrise",	AN_VISIBILITE, AV_GRISE)
			endif
		case 3
			XmeSetAttribut("cpttyp3fstd", AN_STYLE, C_Champ_NSaisi)
			XmeSetAttribut("cpttyp3vgrise", AN_VISIBILITE, AV_GRISE)
			XmeSetAttribut("cpttyp3vcache", AN_VISIBILITE, AV_CACHE)
			if CA.EtbCod = 2			; gestion multi-établissements
				XmeSetAttribut("etb_cpttyp3vgrise",	AN_VISIBILITE, AV_GRISE)
			endif
		endswitch

		if Harmony.DataNotif
			Harmony.Retour = XmeNext_Debut_Donnee
			preturn
		endif
	endif

	if Compte.CptTyp <> 1

		Compte.Sens							= 3
		Compte.TblNo						= 0
		Compte.LettTyp						= 1
		Compte.PointCod					= 1
		Compte.CentTyp						= 1
		Compte.CentEdi						= 1
		Compte.EpuCod						= 1
		init	Compte.JnlSais()			= 1
		Compte.Reg							= ' '
		Compte.Table_Mode_Regl_Lib		= ' '
		Compte.TvaTie						= ' '
		Compte.TvaArt						= ' '
		Compte.Mt2Nat						= 0
		X1.Mt2Lib							= ' '
		Compte.CptCent						= ' '
		Compte.Cpt_Centraliseur_Lib	= ' '
		init Compte.AxeSais()			= 1
		Compte.AxeTb(*)					= ' '
		X1.Lib(3)							= ' '
		X1.Lib(4)							= ' '
		X1.Lib(5)							= ' '
		X1.Lib(6)							= ' '
		Compte.EtbSais						= 3
		Compte.Etb							= ' '
		X1.Lib(7)							= ' '
		Compte.Cpt_Collectif_Lib		= ' '

		if Compte.CptTyp	= 3
			Compte.DevCod	= 1
			Compte.Dev		= ' '
			Compte.Table_Devise_Lib	= ' '
			Compte.DevConv	= 3
			Compte.GrpCptCod= 1
		endif
	endif

	if Compte.DevCod < 2	|	XmeSetAttribut("DevCodfstd", AN_STYLE, C_Champ_NSaisi)
								XmeSetAttribut("DevCodvgrise", AN_VISIBILITE, AV_GRISE)
								XmeSetAttribut("DevCodvcache", AN_VISIBILITE, AV_CACHE)
	else					|	XmeResetAttribut("DevCodfstd", AN_TOUS_ATTRIBUTS)
								XmeResetAttribut("DevCodvgrise", AN_TOUS_ATTRIBUTS)
								XmeResetAttribut("DevCodvcache", AN_TOUS_ATTRIBUTS)
	endif

	if CA.EtbCod = 2			; gestion multi-établissements
		if Compte.EtbSais	<>  1
			XmeSetAttribut("etbunique",	AN_VISIBILITE, AV_GRISE)
			XmeSetAttribut("etbunique",	AN_STYLE, C_Champ_NSaisi)
		endif
	endif


	for axeno = 1 to index(Compte.Axe(), 1)
		if Compte.AxeSais(axeno) <> 2	|	XmeSetAttribut([ "axesais" axeno ], AN_STYLE, C_Champ_NSaisi)
		else								|	XmeResetAttribut([ "axesais" axeno ], AN_TOUS_ATTRIBUTS)
		endif
	next

endp		; Champ_CptTyp_1_Ap


;*
public procedure Champ_Dev_1_Av

; avant code devise

beginp	; Champ_Dev_1_Av

	if Compte.DevCod < 2 or Compte.CptTyp = 3
		Compte.Dev						= ' '
		Compte.Table_Devise_Lib		= ' '
		Harmony.Retour 				= XmeNext_Saut_Donnee
	endif

endp		; Champ_Dev_1_Ap


;*
public procedure Champ_Dev_1_Ap

; après code devise

beginp	; Champ_Dev_1_Ap

	Compte.Table_Devise_Lib = ' '
	if Compte.Dev = ' '					|	Compte.Dev = CA.Dev	|	endif
