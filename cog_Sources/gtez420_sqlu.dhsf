SetModuleInfo('$Id: gtez420_sql.dhsf 33569 2013-09-02 14:50:30Z sn $')


include 'GTTC000.dhsp'

public hfile	gtfdd.dhsd	GTFTAB	GTFTAB
public hfile	gtfdd.dhsd	GTFPCF	GTFPCF	
public hfile	gtfdd.dhsd	GTFAT		GTFAT
public hfile	gtfdd.dhsd	GTFPROJ 	GTFPROJ	

public record 	gtfdd.dhsd	G7	 
public record  gtfdd.dhsd	CLI
public record  gtfdd.dhsd	FOU
public record  gtfdd.dhsd	PRO
public record  gtfdd.dhsd	TIA

public record	gtfdd.dhsd 	ART

public record 	gtfdd.dhsd 	PRJAP
public record 	gtfdd.dhsd 	PRJAV

public record	gtfdd.dhsd	piece_mouv

public record 	gtfdd.dhsd	T034
public record	gtfdd.dhsd	SOC

public record	gtfdd.dhsd	XZ		G3XZ
public record	a5dd.dhsd	M4
public record	a5dd.dhsd	MZ 	
Public Record "gtfdd.dhsd" ent ent
Public Record "ccfdd.dhsd" ca ca
Public Record "gtfdd.dhsd" ets ets
Public Record "ddsys.dhsd" rowinfo zoomselection
Public Record "gtfdd.dhsd" piece_mouv piece_mouv_sel
Public Record "gtfdd.dhsd" gtpceprod gtpceprod

Const	C_Pilote_ParTiersDate	= 1
Const	C_Pilote_ParArticle		= 2
Const	C_Pilote_ParEnrno		   = 3
Const	C_Pilote_ParProjet	   = 4
Const	C_Pilote_ParMarche	   = 5
Const	C_Pilote_ParPiece			= 8

;*
;* séquences liées au masque
;*

;*
Public Procedure Champ_Choix_1_Av
;
Beginp
	if  G7.ZoomPar  = C_Pilote_ParPiece
		Harmony.Retour = Xmenext_Saut_Donnee
	endif
Endp


;*
Public Procedure Champ_DatF_1_Ap
BeginP
	Controle_DonneeFin(G7.DATD, G7.DATF,3,7914)
EndP

;*
Public Procedure Champ_EnrNo_1_Ap	; pour conserver la lecture inverse par no enreg si on tape return
BeginP
	if piece_mouv.ENRNO = 0
		piece_mouv.ENRNO = ' '
	endif
EndP

;*
Public Procedure Champ_ENRNO_2_Ap	; pour conserver la lecture inverse par no enreg si on tape return
BeginP
	if piece_mouv.CDENRNO = 0
		piece_mouv.CDENRNO = ' '
	endif
EndP

;*
public procedure Champ_Etb_1_Av ; avant établissement
beginp
	if G7.Zoompar in (C_Pilote_ParTiersDate,C_Pilote_ParArticle)
		Harmony.Retour = Xmenext_Saut_Donnee
	elsif SOC.ENTCODN(22)= 1
		G7.ETB = ' '
		Harmony.Retour = Xmenext_Saut_Donnee
	endif		 
endp


;*
public procedure Champ_Etb_1_Ap	 ; controle établissement
beginp
	if Controle_Ets (G7.ETB,1400) = 0
		if G7.ETB <> ' '
			MZ.ETB = G7.ETB
		endif
	endif
endp

;*
public procedure Champ_Marche_1_Av						; avant le marché
beginp
	G3XZ.TIERS = G7.TIERS
	G3XZ.TICOD = G7.TICOD
EndP

;*
public procedure Champ_Ref_1_Ap			; controle référence article par tiers
beginp
	Controle_Article(Piece_Mouv_Sel.Ref)
endp


;*
public procedure Champ_TiCod_1_Ap	; après donnée sélection clé paramètre = ' ' ou 3 saisie autorisée
beginp
	if G7.TICOD = 'I'
		G7.Tiers = Determiner_TiersInterne()
		G7.LIB(26) = ' '
	endif
endp

;*
Public Procedure Champ_Tiers_1_Av ; avant tiers
Beginp
	If G7.TiCod = 'I'
		If SOC.EntCodN(22) <> 2 And SOC.EntCodN(44) <> 2
			Harmony.Retour = XMENEXT_SAUT_DONNEE
		Endif
		G7.Tiers = Determiner_TiersInterne()
		G7.Lib(26) = ' '
	Endif
Endp

;*
Public Procedure Champ_Tiers_1_Ap ; controle tiers
Beginp
	If Harmony.Key = K_F8
		Switch G7.TiCod
		Case 'C'	|	Zoom_Call(9021)
		Case 'F'	|	Zoom_Call(9022)
		Case 'P'	|	Zoom_Call(9023)
		Case 'I'	|	Zoom_Call(9025)
		Endswitch
	Else
		If (G7.Etb <> ' ' And Controle_Tiers(G7.TiCod,G7.Tiers,0) = 0) Or \
			(G7.Etb = ' ' And Controle_Tiers(G7.TiCod,G7.Tiers,0, ctretb=FALSE) = 0)
			Switch G7.TiCod
			Case 'C'	|	G7.Lib(26) = CLI.Nom
			Case 'P'	|	G7.Lib(26) = PRO.Nom
			Case 'F'	|	G7.Lib(26) = FOU.Nom
			Case 'I'	|	G7.Lib(26) = TIA.Nom
			Endswitch
		Endif
	Endif
Endp
[/diva]
