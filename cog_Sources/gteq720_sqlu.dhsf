	Controle_DonneeFin(G3XQ.LieuD	, G3XQ.LieuF	, 1, 7936)
	
	For cpt = 1 to 3
		Controle_DonneeFin(G3XQ.FamD(cpt), G3XQ.FamF(cpt), 1, 7908)
	Next
Endp 

