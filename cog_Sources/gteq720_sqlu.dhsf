Public Procedure Page_Boutons_1_Ap		; Après page 1
1		cpt			3,0		; Compteur

Beginp
	Controle_DonneeFin(G3XQ.RefD	, G3XQ.RefF		, 1, 7903)
	Controle_DonneeFin(G3XQ.Sref1D, G3XQ.Sref1F	, 1, 7904)
	Controle_DonneeFin(G3XQ.Sref2D, G3XQ.Sref2F	, 1, 7905)
	Controle_DonneeFin(G3XQ.DepoD	, G3XQ.DepoF	, 1, 7909)
