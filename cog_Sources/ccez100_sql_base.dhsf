;*
Public Procedure Champ_FiltreEtb_1_Ap

; après saisie du filtre établissements

Beginp	; Champ_FiltreEtb_1_Ap

	if Harmony.ClicOnglet		|		preturn		|		endif

	if	Harmony.Key	= K_CTRL_S
	 	Receptionner_Selection_ZoomEtablissement(X1.FiltreEtb)
		Harmony.Retour = XmeNext_Traitement_Avant
	else
		if Filtre_Verifier_Syntaxe(X1.FiltreEtb) <> 0
			Mes_Alert(3033)
			Harmony.Retour = XmeNext_Traitement_Avant
		endif
	endif

Endp		; Champ_FiltreEtb_1_Ap


