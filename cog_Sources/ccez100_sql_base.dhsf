; includes
include	"cctc000.dhsp"

; record SQL
public	recordSQL	'CCRSECR.dhoq'			ECRITURE

; enregistrements
public	record	CCFDD.dhsd		C3								; intitulé de compte
public	record	CCFDD.dhsd		C4								; paramètres de journal
public	record	CCFDD.dhsd		C8			C8_SEL			; ligne d'écriture (sélection)
public	record	CCFDD.dhsd		CA								; dossier, paramètres généraux
public	record	CCFDD.dhsd		H8								; types de règlement
public	record	CCFDD.dhsd		X1								; work zoom

public	record	GTFDD.dhsd 		T006							; modes de règlement

public	record	DDSYS.dhsd		ZOOM							; variables de contrôle du zoom
public	record	DDSYS.dhsd 		ROWINFO ZOOMSELECTION

; modules
module	"gttm000.dhop"


;*
;*		procédures sur données des onglets
;*

;*
public procedure Champ_Cpt_1_Ap

; après saisie du compte sur l'onglet (compte de sélection)

beginp	; Champ_Cpt_1_Ap

	X1.Lib(9) = ' '
	if Controle_Compte(C8_SEL.Cpt, nonvide = false, syntaxe = false, collectif = condition(X1.Choix(4) = 3, true, false), err = 0, etablissement = false) = 0
		X1.Lib(9) = C3.Lib
		if X1.Choix(4) = 3 and C3.CptTyp <> 2		; type du compte de sélection est "collectif" mais le compte saisi n'est pas un collectif
			X1.Lib(9) = ' '
			Mes_Alert(3035, C3.Cpt)
			Harmony.Retour = XmeNext_Traitement_Avant
		endif
	endif

endp		; Champ_Cpt_1_Ap


;*
public procedure Champ_Jnl_1_Ap

; après saisie du code journal sur l'onglet (journal de sélection)

beginp	; Champ_Jnl_1_Ap

	X1.Lib(8) = ' '
	if Controle_Journal(C8_SEL.Jnl, err = 0) = 0
		X1.Lib(8) = C4.Lib
		if Jnl_Lie_A_Reglement(MZ.EtbC3, C4.Jnl) = 'O'              
			XmeSetAttribut("r3cod", AN_STYLE, "CHAMP_NON_SAISI")
		else
			XmeResetAttribut("r3cod", AN_TOUS_ATTRIBUTS)
		endif
	endif

endp		; Champ_Jnl_1_Ap


