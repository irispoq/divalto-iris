;*
Public Procedure Champ_TiersTyp_1_Ap	; Changement de sélection de tiers : met à jour le multichoix de type d'affichage
Beginp
	Charger_AffTyp(GR001.TiersTypTb(), Util.ReprUser)			; Rechargement du multi-choix

	If GR001.Choix(1) = 2														; Si 'Tous établissements' est coché => le choix ne peut être que nom, téléphone, email, siret
		If GR001.AffTyp not in (C_AFF_NOM, C_AFF_TEL, C_AFF_EMAIL, C_AFF_SIRET)
			GR001.AffTyp = C_AFF_NOM
		EndIf

		If GR001.TiersTyp(6) = 2 And GR001.AffTyp in (C_AFF_SIRET)	; Pas de mode affichage par siret, code naf ou projet si type de tiers individu
			GR001.AffTyp = C_AFF_NOM
		EndIf
	Else																				; 'Tous établissements' est décoché
		; Pas de mode affichage Commercial si utilisateur = Commercial ou type de tiers Fournisseur Commercial ou autre tiers ou individu
		If Util.ReprUser = 2 Or GR001.TiersTyp(3) = 2 Or GR001.TiersTyp(4) = 2 Or GR001.TiersTyp(5) = 2 Or GR001.TiersTyp(6) = 2
			If GR001.AffTyp in (	C_AFF_REPR1, C_AFF_REPR1_NAF, C_AFF_REPR1_PAYS, C_AFF_REPR1_STAT1_NOM, C_AFF_REPR1_STAT1_PAYS, \
										C_AFF_REPR1_STAT2_NOM, C_AFF_REPR1_STAT2_PAYS, C_AFF_REPR1_STAT3_NOM, C_AFF_REPR1_STAT3_PAYS, \
										C_AFF_REPR2_NOM, C_AFF_REPR3_NOM )
				GR001.AffTyp = C_AFF_NOM
			EndIf
		EndIf
	
		; Pas de mode affichage Das ou statistique si type de tiers Commercial ou autretiers ou individu
		If GR001.TiersTyp(4) = 2 Or GR001.TiersTyp(5) = 2 Or GR001.TiersTyp(6) = 2
			If GR001.AffTyp in ( C_AFF_DAS_NOM, C_AFF_DAS_PAYS_CPOSTAL, C_AFF_REPR1_STAT1_NOM, C_AFF_REPR1_STAT1_PAYS, \
										C_AFF_REPR1_STAT2_NOM, C_AFF_REPR1_STAT2_PAYS, C_AFF_REPR1_STAT3_NOM, C_AFF_REPR1_STAT3_PAYS, \
										C_AFF_STAT1_NOM, C_AFF_STAT1_PAYS, C_AFF_STAT2_NOM, C_AFF_STAT2_PAYS, \
										C_AFF_STAT3_NOM, C_AFF_STAT3_PAYS )
				GR001.AffTyp = C_AFF_NOM
			EndIf
		EndIf
	
		; Pas de mode affichage code qualification si tiers autre que prospect
		If GR001.TiersTyp(2) = 2 Or GR001.TiersTyp(3) = 2 Or GR001.TiersTyp(4) = 2 Or GR001.TiersTyp(5) = 2 Or GR001.TiersTyp(6) = 2
			If GR001.AffTyp = C_AFF_QUALIFICATION
				GR001.AffTyp = C_AFF_NOM
			EndIf
		EndIf
	
		; Pas de mode affichage groupement centrale si tiers autre que prospect	client fournisseur
		If GR001.TiersTyp(4) = 2 Or GR001.TiersTyp(5) = 2 Or GR001.TiersTyp(6) = 2
			If GR001.AffTyp = C_Aff_Centrale
				GR001.AffTyp = C_Aff_Nom
			EndIf
		EndIf
	EndIf

	; Pas de mode affichage par contact si type de tiers commercial ou individu
	If GR001.TiersTyp(4) = 2 Or GR001.TiersTyp(6) = 2
		If GR001.AffTyp = C_AFF_NOM_CONTACT
			GR001.AffTyp = C_AFF_NOM
		EndIf
	EndIf

	; Pas de mode affichage par siret, code naf ou projet si type de tiers individu
	If GR001.TiersTyp(6) = 2
		If GR001.AffTyp in ( C_AFF_SIRET, C_AFF_NAF, C_AFF_PROJET_NOM)
			GR001.AffTyp = C_AFF_NOM
		EndIf
	EndIf
	
	; Repositionnement de GR1.choix(1)
	GR1.Choix(1) = GR001.AffTyp

	; Rechargement du multichoix des types de tiers pour les filtres sur rubriques
	Charger_Types_Tiers

	; Initialisation de l'entité en fonction des types de tiers chargés
	If GR001.TiersTyp(6) = 2	|	GR001_RUB.Choix(1) = C_ENTITE_INDIVIDU		|	EndIf
	If GR001.TiersTyp(5) = 2	|	GR001_RUB.Choix(1) = C_ENTITE_AUTRE_TIERS	|	EndIf
	If GR001.TiersTyp(4) = 2	|	GR001_RUB.Choix(1) = C_ENTITE_COMMERCIAL	|	EndIf
	If GR001.TiersTyp(3) = 2	|	GR001_RUB.Choix(1) = C_ENTITE_FOURNISSEUR	|	EndIf
	If GR001.TiersTyp(2) = 2	|	GR001_RUB.Choix(1) = C_ENTITE_CLIENT		|	EndIf
	If GR001.TiersTyp(1) = 2	|	GR001_RUB.Choix(1) = C_ENTITE_PROSPECT		|	EndIf	
Endp

;*
Public Procedure Champ_Tiers_1_Ap		; Complète code tiers en création
1		x			1,0
1		rep		1
RecordSql 'GTRSPPTIERS.dhoq'	VueTiers Tiers_Loc

Beginp
	Switch GR1.MulticTicodVTb(GR001V1.Ce2N)
	Case '2'
		If Length(GR001V1.TiersIndividu) > DataSize(PRO.Tiers)
			Mes_Alert(2051, ToString(DataSize(PRO.Tiers)), 'prospect')
			Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
			PReturn
		Else
			If GR001V1.Tiers <> ' ' And Left(GR001V1.Tiers, 1) <> '-'
				Controle_Syntaxe_Tiers(GR001V1.Tiers, 2004)
				If Soc_Gerer_Tiers_Etablissement
					If GR001V1.Tiers not in (Etablissement.Tiers1(1)..Etablissement.Tiers2(1))
						Mes_Alert(2017, MZ.Etb, Etablissement.Tiers1(1), Etablissement.Tiers2(1))
						Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
					EndIf
				EndIf
			EndIf
		EndIf

	Case '3'
		If Length(GR001V1.TiersIndividu) > DataSize(CLI.Tiers)
			Mes_Alert(2051, ToString(DataSize(CLI.Tiers)), 'client')
			Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
			PReturn
		Else
			If GR001V1.Tiers <> ' ' And Left(GR001V1.Tiers, 1) <> '-'
				Controle_Syntaxe_Tiers(GR001V1.Tiers, 2004)
				If Soc_Gerer_Tiers_Etablissement
					If GR001V1.Tiers not in (Etablissement.Tiers1(2)..Etablissement.Tiers2(2))
						Mes_Alert(2015, MZ.Etb, Etablissement.Tiers1(2), Etablissement.Tiers2(2))
						Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
					EndIf
				EndIf
			EndIf
		EndIf

	Case '4'
		If Length(GR001V1.TiersIndividu) > DataSize(FOU.Tiers)
			Mes_Alert(2051, ToString(DataSize(FOU.Tiers)), 'fournisseur')
			Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
			PReturn
		Else
			If GR001V1.Tiers <> ' ' And Left(GR001V1.Tiers, 1) <> '-'
				Controle_Syntaxe_Tiers(GR001V1.Tiers, 2004)
				If Soc_Gerer_Tiers_Etablissement
					If GR001V1.Tiers not in (Etablissement.Tiers1(3)..Etablissement.Tiers2(3))
						Mes_Alert(2019, MZ.Etb, Etablissement.Tiers1(3), Etablissement.Tiers2(3))
						Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
					EndIf
				EndIf
			EndIf
		EndIf

	Case '5'
		If Length(GR001V1.TiersIndividu) > DataSize(VRP.Tiers)
			Mes_Alert(2051, ToString(DataSize(VRP.Tiers)), 'commercial')
			Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
			PReturn
		Else
			If GR001V1.Tiers = ' '
				Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
				PReturn
			EndIf
			Controle_Syntaxe_Tiers(GR001V1.Tiers, 2004)
		EndIf

	Case '6'
		If Length(GR001V1.TiersIndividu) > DataSize(TIA.Tiers)
			Mes_Alert(2051, ToString(DataSize(TIA.Tiers)), 'autre tiers')
			Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
			PReturn
		Else
			If GR001V1.Tiers = ' '
				Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
				PReturn
			EndIf
			Controle_Syntaxe_Tiers(GR001V1.Tiers, 2004)
		EndIf

	Case '7'
		If Length(GR001V1.TiersIndividu) > DataSize(PINDIVIDU.Individu)
			Mes_Alert(2051, ToString(DataSize(PINDIVIDU.Individu)), 'individu')
			Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
			PReturn
		Else
			If GR001V1.TiersIndividu = ' '
				Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
				PReturn
			EndIf
		EndIf
			
	EndSwitch

	If GR001V1.TiersIndividu <> ' '
		If Seek_VueTiers(Tiers_Loc, GR001V1.TiersIndividu) = 0	; Le code tiers existe déjà
			Mes_Alert(2011, GR001V1.TiersIndividu)
			Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
		EndIf
	EndIf
Endp

