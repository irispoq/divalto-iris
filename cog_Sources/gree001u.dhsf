;*
Public Procedure Champ_Tiersgrp_1_Ap	; Contrôle code centrale
1		tiers		>GTFDD.dhsd Tiers
1		x			2,0							; Code retour de fonction

Beginp
	GR001V1.Lib(10)	= ' '
	tiers					= GR001V1.TiersGrp
	If GR001V1.Ce2 = '2'	
		PingLocal('GR001_PRO', PRO)
		
		Switch ( x := Controle_Prospect(tiers, 0, 0) )
		Case 0															|	GR001V1.Lib(10) = PRO.Nom									; On accepte client ou prospect
		Case 1															|	Mes_Alert(2004, tiers)		|	Harmony.Retour = 3
		Case Controle_Client(tiers, 0, 0) = 0					|	GR001V1.Lib(10) = CLI.Nom									; On accepte un code client
		Case Vsw = 6 Or Controle_Client(tiers, 0, 0) = 6	|	Mes_Alert(2006, tiers)		|	Harmony.Retour = 3	; Client ou prospect non visé
		Default															|	Mes_Alert(2000, tiers)		|	Harmony.Retour = 3
		EndSwitch

		PingLocalReceive('GR001_PRO', PRO)
	ElsIf GR001V1.Ce2 = '3'
		PingLocal('GR001_CLI', CLI)

		Switch (x := Controle_Client(tiers, 0, 0))
		Case 0	|	GR001V1.Lib(10) = CLI.Nom
		Case 1	|	Mes_Alert(2004, tiers)		|	Harmony.Retour = 3
		Case 6	|	Mes_Alert(2006, tiers)		|	Harmony.Retour = 3
		Default	|	Mes_Alert(2007, tiers)		|	Harmony.Retour = 3
		EndSwitch

		PingLocalReceive('GR001_CLI', CLI)
	Else
		PingLocal('GR001_FOU', FOU)

		Switch ( x := Controle_Fournisseur(tiers, 0, 0))
		Case 0	|	GR001V1.Lib(10) = FOU.Nom
		Case 1	|	Mes_Alert(2004, tiers)		|	Harmony.Retour = 3
		Case 6	|	Mes_Alert(2006, tiers)		|	Harmony.Retour = 3
		Default	|	Mes_Alert(2008, tiers)		|	Harmony.Retour = 3
		EndSwitch

		PingLocalReceive  ('GR001_FOU',FOU)
	EndIf
	GR001V1.TiersGrp = tiers
Endp
	
