Public Procedure Effacer_Filtres_Rubriques
1		i			2,0

Beginp
	For i = 1 to NbFiltresRubriquesMax + 10 ; Attention si on ajoute des lignes de filtres, à ne pas dépasser la taille limite des tableaux
		GR001_RUB.FiltreChamp(i)		= ' '
		GR001_RUB.FiltreChampTyp(i)	= ' '
		GR001_RUB.FiltreChampOdbc(i)	= ' '
		GR001_RUB.FiltreOp(i)			= ' '
		GR001_RUB.FiltreExp(i)			= ' '
		GR001_RUB.FiltreQuestion(i)	= ' '
	Next
	
	For i = 1 to NbFiltresValRubriquesMax
		GR001_RUB.FiltreValeur(i) = ' '
	Next
	
	GR001_RUB.FiltreFormule(1) = ' '
	
	Xmt_Call('Effacer_Filtre_Ap')
Endp

