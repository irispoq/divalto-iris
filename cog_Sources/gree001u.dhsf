Public Procedure Champ_Tiers_3_Ap				; Contrôle code tiers si création automatique a échoué
Beginp
	If GR001V1.Ce2N <> 7 And Controle_Syntaxe_Tiers(GR001V1.Tiers, 2004) <> 0
		PReturn
	EndIf

	Switch GR001V1.Ce2
	Case '2'
		If Length(GR001V1.TiersIndividu) > DataSize(PRO.Tiers)
			Mes_Alert(2051, ToString(DataSize(PRO.Tiers)), 'prospect')
			Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
			PReturn
		Else
			If Res_Pro(GR001V1.Tiers, 0, NoWait) <> 0
				Mes_Alert(2001, PRO.Tiers)
				Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
				PReturn
			EndIf
			PingLocal('ZOOM_PRO', PRO)
			If Seek_Tiers(GR001V1.Tiers) = 0 	; recherche tous tiers confondus car code doit être unique
				PingLocalReceive('ZOOM_PRO', PRO)
				Lib_Pro(GR001V1.Tiers)
				Mes_Alert(2022, PRO.Tiers)
				Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
			EndIf
			PingLocalReceive('ZOOM_PRO', PRO)
		EndIf

	Case '3'
		If Length(GR001V1.TiersIndividu) > DataSize(CLI.Tiers)
			Mes_Alert(2051, ToString(DataSize(CLI.Tiers)), 'client')
			Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
			PReturn
		Else
			If Res_Cli(GR001V1.Tiers, 0, NoWait) <> 0
				Mes_Alert(2001, GR001V1.Tiers)
				Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
				PReturn
			EndIf
			PingLocal('ZOOM_CLI', CLI)
			If Seek_Tiers(GR001V1.Tiers) = 0 	; recherche tous tiers confondus car code doit être unique
				PingLocalReceive('ZOOM_CLI',CLI)
				Lib_Cli(GR001V1.Tiers)
				Mes_Alert(2023, GR001V1.Tiers)
				Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
			EndIf
			PingLocalReceive('ZOOM_CLI',CLI)
		EndIf

	Case '4'
		If Length(GR001V1.TiersIndividu) > DataSize(FOU.Tiers)
			Mes_Alert(2051, ToString(DataSize(FOU.Tiers)), 'fournisseur')
			Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
			PReturn
		Else			
			If Res_Fou(GR001V1.Tiers, 0, NoWait) <> 0
				Mes_Alert(2001, GR001V1.Tiers)
				Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
				PReturn
			EndIf
			PingLocal('ZOOM_FOU', FOU)
			If Seek_Tiers(GR001V1.Tiers) = 0 	; recherche tous tiers confondus car code doit être unique
				PingLocalReceive('ZOOM_FOU', FOU)
				Lib_Fou(GR001V1.Tiers)
				Mes_Alert(2024, GR001V1.Tiers)
				Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
			EndIf
			PingLocalReceive('ZOOM_FOU', FOU)
		EndIf

	EndSwitch
Endp

