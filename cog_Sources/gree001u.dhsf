;*
Public Procedure Calculer_SECU
1		secu		15
	2	secusexe	1
	2	secuan	2
	2	secumois	2
	2 	secudep	2
	2	secuvil	3
	2	securan	3
	2	secucle	2

Beginp
	secu = GR001V1.Secu
	
	If GR001V1.Sexe		<> ' '									|	secusexe = GR001V1.Sexe					|	EndIf

	If GR001V1.NaisDt		<> ' '
		secuan	= Fstring ('0', Right(HYEAR(GR001V1.NaisDt), 2))
		secumois	= Fstring ('0', HMONTH(GR001V1.NaisDt))
	EndIf

	If GR001V1.DeparNais	<> ' '									|	secudep = GR001V1.DeparNais			|	EndIf
	If GR001V1.Comnais	<> ' ' And T057.VilInsee <> ' '	|	secuvil = Right(T057.VilInsee, 3)	|	EndIf

	GR001V1.Secu = secu
Endp

