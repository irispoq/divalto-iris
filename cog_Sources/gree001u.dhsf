Public Procedure Champ_CFctCod_1_Ap		; Après saisie code tiers
1		NbFctCod		4,0
1		i				4,0
1		fctCod		>GRFDD.dhsd FctCod

Beginp	
	If Harmony.Key = K_CTRL_S
		If PongReceive('T069_NBRE', NbFctCod) = 0
			For i = 1 to NbFctCod
				PongReceive([ 'T069_' ToString(i) ], fctCod)
				If Length(GR1.CFctCod) = 0 Or Right(Left(GR1.CFctCod), 1) in (';','-')
					GR1.CFctCod &= Left(fctCod)	
				Else
					GR1.CFctCod &= [ ';' Left(fctCod) ]
				EndIf
			Next
		EndIf
	EndIf

	If A5_Filtre_Verifier_Syntaxe(GR1.CFctCod) <> 0	
		Mes_Alert(7950)
      Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
	EndIf
