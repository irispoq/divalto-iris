		GR001.FiltreValeur(Harmony.Dataind1)= ' '
		Switch Harmony.Dataind1	
		Case Vsw in (11..15)	|	GR001.FiltreDtUn(Harmony.Dataind1 - 10) = ' '	|	GR001.FiltreDtNb(Harmony.Dataind1 - 10) = ' '	|	GR001.FiltreDt(Harmony.Dataind1 - 10) = ' '
		Case Vsw in (26..30)	|	GR001.FiltreDtUn(Harmony.Dataind1 - 20) = ' '	|	GR001.FiltreDtNb(Harmony.Dataind1 - 20) = ' '	|	GR001.FiltreDt(Harmony.Dataind1 - 20) = ' '
		Case Vsw in (41..45)	|	GR001.FiltreDtUn(Harmony.Dataind1 - 30) = ' '	|	GR001.FiltreDtNb(Harmony.Dataind1 - 30) = ' '	|	GR001.FiltreDt(Harmony.Dataind1 - 30) = ' '
		Case Vsw in (56..60)	|	GR001.FiltreDtUn(Harmony.Dataind1 - 40) = ' '	|	GR001.FiltreDtNb(Harmony.Dataind1 - 40) = ' '	|	GR001.FiltreDt(Harmony.Dataind1 - 40) = ' '
		Case Vsw in (71..75)	|	GR001.FiltreDtUn(Harmony.Dataind1 - 50) = ' '	|	GR001.FiltreDtNb(Harmony.Dataind1 - 50) = ' '	|	GR001.FiltreDt(Harmony.Dataind1 - 50) = ' '
		EndSwitch
	Else
		If Left(GR001.FiltreChampOdbc(Harmony.Dataind1), 1) = '-'
			libabr = Convertir_Nom_Nomabr(Mid(GR001.FiltreChampOdbc(Harmony.Dataind1), 2, 31))
			A5FRUB.Key = [ 'M' MZ.DOS libabr ]
			If Next_MRBQ(A5FRUB.Key) <> 0
				Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
			Else
				GR001.FiltreChampOdbc(Harmony.Dataind1) = MRBQ.Rubrique
				Switch Harmony.Dataind1
				Case Vsw in (26..30)	|	Controler_Rubrique(GR001.FiltreChampOdbc(Harmony.Dataind1), 3001, entite=7)	; Contacts
				Case Vsw in (41..45)	|	Controler_Rubrique(GR001.FiltreChampOdbc(Harmony.Dataind1), 3001, entite=11)	; Evénements
				Case Vsw in (56..60)	|	Controler_Rubrique(GR001.FiltreChampOdbc(Harmony.Dataind1), 3001, entite=9)	; Projets
				Case Vsw in (71..75)	|	Controler_Rubrique(GR001.FiltreChampOdbc(Harmony.Dataind1), 3001, entite=10)	; Affaires
				Default					|	Controler_Rubrique(GR001.FiltreChampOdbc(Harmony.Dataind1), 3001)
				EndSwitch
			EndIf
		Else	
			Switch Harmony.Dataind1
			Case Vsw in (26..30)	|	Controler_Rubrique(GR001.FiltreChampOdbc(Harmony.Dataind1), 3001, entite=7)		; Contacts
			Case Vsw in (41..45)	|	Controler_Rubrique(GR001.FiltreChampOdbc(Harmony.Dataind1), 3001,	entite=11)		; Evénements
			Case Vsw in (56..60)	|	Controler_Rubrique(GR001.FiltreChampOdbc(Harmony.Dataind1), 3001, entite=9)		; Projets
			Case Vsw in (71..75)	|	Controler_Rubrique(GR001.FiltreChampOdbc(Harmony.Dataind1), 3001, entite=10)		; Affaires
			Default					|	Controler_Rubrique(GR001.FiltreChampOdbc(Harmony.Dataind1), 3001)
			Endswitch
		EndIf
	EndIf
	GR001.FiltreChamp(Harmony.Dataind1) = GR001.FiltreChampOdbc(Harmony.Dataind1)
Endp

