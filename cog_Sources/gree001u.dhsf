;*
Public Procedure Champ_FiltreChampOdbc_1_Ap
1		Res				1000
1		Enreg				32
1		Champ				60
1		ChampOdbc		60 = ' '
1		ChampOdbcLu		60
1		Trouve			1,0
1		StChamp			L
1		pos				2,0
1		Dico				20
1		ptdico			L	        	
1		buffer			8096		; Buffer du flux
1		NbDim				2,0
1		Dim1				5,0
1		Dim2				5,0
1		Dim3				5,0
1		Dim4				5,0
1		niveaudico  	X = 2		; Niveau 1 , 2 ou 3
1		Ind	   		20 = ' '
1		TypeChamp		2,0

Beginp
	If GR001.FiltreChampOdbc(Harmony.Dataind1) = ' '
		GR001.FiltreOp		(Harmony.Dataind1) = ' '
		GR001.FiltreValeur(Harmony.Dataind1) = ' '
		GR001.FiltreChamp	(Harmony.Dataind1) = ' '
	Else																	; Extraction du nom du champ ODBC
		pos = String('.', GR001.FiltreChampOdbc(Harmony.Dataind1))
		If pos <> 0 And Length(GR001.FiltreChampOdbc(Harmony.Dataind1)) > pos
			ChampOdbc = Mid(GR001.FiltreChampOdbc(Harmony.Dataind1), pos+1, Length(GR001.FiltreChampOdbc(Harmony.Dataind1)) - pos)
			If ( Pos := String('(', ChampOdbc) ) <> 0
				Ind			= Mid(Champodbc, Pos, Length(ChampOdbc) - Pos + 1)
				ChampOdbc	= Left(ChampOdbc, Pos - 1)
			EndIf
		EndIf	

		Switch Harmony.dataind1
		Case Vsw in (1..10)											; Champs des tiers
			If Left(GR001.FiltreChampOdbc(Harmony.Dataind1), 6)	<> 'CLIENT'			And \
				Left(GR001.FiltreChampOdbc(Harmony.Dataind1), 11)	<> 'FOURNISSEUR'	And \
				Left(GR001.FiltreChampOdbc(Harmony.Dataind1), 8)	<> 'PROSPECT'		And \
				Left(GR001.FiltreChampOdbc(Harmony.Dataind1), 11)	<> 'AUTRE_TIERS'	And \
				Left(GR001.FiltreChampOdbc(Harmony.Dataind1), 10)	<> 'COMMERCIAL'	And \
				Left(GR001.FiltreChampOdbc(Harmony.Dataind1), 8)	<> 'INDIVIDU'		And \
				Left(GR001.FiltreChampOdbc(Harmony.Dataind1), 10)	<> 'TIERS_VIEW'
				Mes_Alert(1107, GR001.FiltreChampOdbc(Harmony.Dataind1))
				Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
				Preturn	
			ElsIf Left(GR001.FiltreChampOdbc(Harmony.Dataind1), 6)	= 'CLIENT'			|	Enreg = 'CLI'
			ElsIf Left(GR001.FiltreChampOdbc(Harmony.Dataind1), 11)	= 'FOURNISSEUR'	|	Enreg = 'FOU'
			ElsIf Left(GR001.FiltreChampOdbc(Harmony.Dataind1), 8)	= 'PROSPECT'		|	Enreg = 'PRO'
			ElsIf Left(GR001.FiltreChampOdbc(Harmony.Dataind1), 11)	= 'AUTRE_TIERS'	|	Enreg = 'TIA'
			ElsIf Left(GR001.FiltreChampOdbc(Harmony.Dataind1), 10)	= 'COMMERCIAL'		|	Enreg = 'VRP'
			ElsIf Left(GR001.FiltreChampOdbc(Harmony.Dataind1), 8)	= 'INDIVIDU'		|	Enreg = 'PINDIVIDU'
			ElsIf Left(GR001.FiltreChampOdbc(Harmony.Dataind1), 10)	= 'TIERS_VIEW'		|	Enreg = 'TIERS_VIEW'
			EndIf

		Case Vsw in (16..25)											; Champs des contacts
			If Left(GR001.FiltreChampOdbc(Harmony.Dataind1), 7) <> 'CONTACT'
				Mes_Alert(1107, GR001.FiltreChampOdbc(Harmony.Dataind1))
				Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
				Preturn
			Else
				Enreg = 'T2'		
			EndIf
		
		Case Vsw in (31..40)											; Champs des événements
			If Left(GR001.FiltreChampOdbc(Harmony.Dataind1), 9) <> 'EVENEMENT'
				Mes_Alert(1107, GR001.FiltreChampOdbc(Harmony.Dataind1))
				Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
				Preturn	
			Else
				Enreg = 'EVTTIERS'		
			EndIf
		
		Case Vsw in (46..55)											; Champs des projets
			If Left(GR001.FiltreChampOdbc(Harmony.Dataind1), 6) <> 'PROJET'
				Mes_Alert(1107, GR001.FiltreChampOdbc(Harmony.Dataind1))	
				Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
				Preturn	
			Else
				Enreg='PRJAV'		
			EndIf
		
		Case Vsw in (61..70)											; Champs des affaires
			If Left(GR001.FiltreChampOdbc(Harmony.Dataind1), 7) <> 'AFFAIRE'
				Mes_Alert(1107, GR001.FiltreChampOdbc(Harmony.Dataind1))
				Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
				Preturn	
			Else
				Enreg='PRJAP'		
			EndIf
		
		Case Vsw in (76..85, 91..100, 106..115, 121..130)	; Champs des pièces
			If Left(GR001.FiltreChampOdbc(Harmony.Dataind1), 9) <> 'MOUVEMENT'
				Mes_Alert(1107, GR001.FiltreChampOdbc(Harmony.Dataind1))
				Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
				Preturn	
			Else
				Enreg='MOUV'		
			EndIf
		
		Case Vsw in (86..90, 101..105, 116..120, 131..135)	; Champs des ventilations
			If Left(GR001.FiltreChampOdbc(Harmony.Dataind1), 11) <> 'VENTILATION'
				Mes_Alert(1107, GR001.FiltreChampOdbc(Harmony.Dataind1))
				Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
				Preturn	
			Else
				Enreg='MVTL'		
			EndIf

		EndSwitch
		
		Switch Enreg													; Recherche du nom du champ
		Case 'EVTTIERS'	|	Dico = 'GRFDD.dhsd'
		Case 'PINDIVIDU'	|	Dico = 'PPFDD.dhsd'
		Default				|	Dico = 'GTFDD.dhsd'
		EndSwitch

		If DDInterface(DD_OPEN, ptdico, Dico, buffer, niveaudico) <> 0				; Ouverture de dictionnaire
			DDInterface(DD_GETERROR, ptdico, ' ', buffer, niveaudico)
			Mes_Alert(9063, Left(buffer), dico)
			Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
			Preturn
		EndIf
	
		If DDInterface(DD_TABLEFIND_DD, ptdico, Enreg, buffer, niveaudico) <> 0	; Recherche de la description de la table
			DDInterface(DD_GETERROR, ptdico, ' ', buffer, niveaudico)
			Mes_Alert(9064, Left(buffer), Enreg)
			Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
			Preturn
		EndIf

		stchamp = DDInterface(DD_DATABEGIN, ptdico, ' ', buffer, niveaudico)		; Boucle sur la table pour vérifier si le champ odbc existe
	
		Trouve = FALSE
	  	Do while stchamp = 0																		; On récupère le nom de la donnée
			Champ			= Hmpseek(buffer, 'NOM')
			ChampOdbclu	= HmpSeek(buffer, 'NOMODBC')
			TypeChamp	= HmpSeek(buffer, 'NATURE')
			If FString('U', ChampOdbc) = FString('U', ChampOdbcLu)
				stchamp	= 1
				Trouve	= TRUE
			Else
				stchamp	= DDInterface(DD_DATANEXT, ptdico, ' ', buffer, niveaudico)
			EndIf	
		WEnd

		If trouve = FALSE
			Mes_Alert(1107, GR001.FiltreChampOdbc(Harmony.Dataind1))
			Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
		Else
			GR001.FiltreChampTyp(Harmony.Dataind1)	= TypeChamp
			GR001.FiltreChamp(Harmony.Dataind1)		= [ Left(Enreg) '.' Left(Champ) Left(Ind) ]
			If DataCompute(GR001.FiltreChamp(Harmony.Dataind1), Res, 1) <> 0
				If DataCompute(GR001.FiltreChamp(Harmony.Dataind1), Res) <> 0
					Mes_Alert(1107, GR001.FiltreChampOdbc(Harmony.Dataind1))
					Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
				EndIf
			EndIf	
		EndIf
	EndIf
Endp

