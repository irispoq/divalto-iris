		If GR001.TiersTyp(2) = 2	|	GR001_RUB.Choix(1) = C_ENTITE_CLIENT		|	EndIf	
		If GR001.TiersTyp(1) = 2	|	GR001_RUB.Choix(1) = C_ENTITE_PROSPECT		|	EndIf		
	EndIf
