																														XmeSetAttribut('cCPOSTAL'	, AN_STYLE, 'CHAMP_SAISI_CLE')	|	XmeSetAttribut('cNom'		, AN_STYLE, 'CHAMP_SAISI_CLE')
	Case Vsw in (C_AFF_REPR1, C_AFF_REPR2_NOM, C_AFF_REPR3_NOM)									|	XmeSetAttribut('cRepr'		, AN_STYLE, 'CHAMP_SAISI_CLE')	|	XmeSetAttribut('cNom'		, AN_STYLE, 'CHAMP_SAISI_CLE')
	Case C_AFF_REPR1_PAYS																					|	XmeSetAttribut('cRepr'		, AN_STYLE, 'CHAMP_SAISI_CLE')	|	XmeSetAttribut('cPAYS'		, AN_STYLE, 'CHAMP_SAISI_CLE')
																														XmeSetAttribut('cCPOSTAL'	, AN_STYLE, 'CHAMP_SAISI_CLE')	|	XmeSetAttribut('cNom'		, AN_STYLE, 'CHAMP_SAISI_CLE')
	Case C_AFF_PAYS_CPOSTAL																					|	XmeSetAttribut('cPAYS'		, AN_STYLE, 'CHAMP_SAISI_CLE')	|	XmeSetAttribut('cCPOSTAL'	, AN_STYLE, 'CHAMP_SAISI_CLE')	
																														XmeSetAttribut('cNom'		, AN_STYLE, 'CHAMP_SAISI_CLE')
	Case C_AFF_VILLE																							|	XmeSetAttribut('cVille'		, AN_STYLE, 'CHAMP_SAISI_CLE')
	Case C_AFF_NAF																								|	XmeSetAttribut('cNAf'		, AN_STYLE, 'CHAMP_SAISI_CLE')
	Case C_AFF_CENTRALE																						|	XmeSetAttribut('cCentrale'	, AN_STYLE, 'CHAMP_SAISI_CLE')
	Case C_AFF_QUALIFICATION																				|	XmeSetAttribut('cQualcod'	, AN_STYLE, 'CHAMP_SAISI_CLE')
	Case Vsw in (C_AFF_REPR1_STAT1_NOM, C_AFF_REPR1_STAT2_NOM, C_AFF_REPR1_STAT3_NOM)	|	XmeSetAttribut('cRepr'		, AN_STYLE, 'CHAMP_SAISI_CLE')	|	XmeSetAttribut('cStat'		, AN_STYLE, 'CHAMP_SAISI_CLE')
																														XmeSetAttribut('cNom'		, AN_STYLE, 'CHAMP_SAISI_CLE')
	Case Vsw in (C_AFF_REPR1_STAT1_PAYS, C_AFF_REPR1_STAT2_PAYS, C_AFF_REPR1_STAT3_PAYS)|	XmeSetAttribut('cRepr'		, AN_STYLE, 'CHAMP_SAISI_CLE')	|	XmeSetAttribut('cStat'		, AN_STYLE, 'CHAMP_SAISI_CLE')
																														XmeSetAttribut('cPAYS'		, AN_STYLE, 'CHAMP_SAISI_CLE')	|	XmeSetAttribut('cCPOSTAL'	, AN_STYLE, 'CHAMP_SAISI_CLE')	
																														XmeSetAttribut('cNom'		, AN_STYLE, 'CHAMP_SAISI_CLE')
	Case C_AFF_REPR1_NAF																						|	XmeSetAttribut('cRepr'		, AN_STYLE, 'CHAMP_SAISI_CLE')	|	XmeSetAttribut('cNAf'		, AN_STYLE, 'CHAMP_SAISI_CLE')
	Case C_AFF_DAS_NOM																						|	XmeSetAttribut('cDAS'		, AN_STYLE, 'CHAMP_SAISI_CLE')	|	XmeSetAttribut('cNom'		, AN_STYLE, 'CHAMP_SAISI_CLE')
	Case C_AFF_DAS_PAYS_CPOSTAL																			|	XmeSetAttribut('cDAS'		, AN_STYLE, 'CHAMP_SAISI_CLE')	|	XmeSetAttribut('cPAYS'		, AN_STYLE, 'CHAMP_SAISI_CLE')
																														XmeSetAttribut('cCPOSTAL'	, AN_STYLE, 'CHAMP_SAISI_CLE')	|	XmeSetAttribut('cNom'		, AN_STYLE, 'CHAMP_SAISI_CLE')
	EndSwitch
	
	If Util.ReprUser = 2	|	XmeSetAttribut('cRepr', AN_VISIBILITE, AV_GRISE)	|	EndIf		
Endp

;*
Public Procedure Champ_Choix_2_Ap
Beginp
	; Si GR1_PARAM_ENTETE.Choix(1) vaut true, on le laisse à true (signifie qu'on a modifié un autre champ avant de saisie le
	; champ courant), sinon on lui donne la valeur de Harmony.Datamodif
	GR1_PARAM_ENTETE.Choix(1) = Condition(GR1_PARAM_ENTETE.Choix(1) = TRUE, TRUE, Harmony.Datamodif)
	If Harmony.Datanotif = TRUE
		If Harmony.Datamodif	= TRUE
			If GR1_PARAM_ENTETE.Choix(3) <> 1	; Types d'événements à afficher est coché => on dégrise les Cases à cocher
				XmeSetAttribut('EVTTYPE', AN_VISIBILITE, AV_VISIBLE) 				
			Else											; 'Tous les événements' est coché => on grise les cases à cocher et on recharge les événements
				XmeSetAttribut('EVTTYPE', AN_VISIBILITE, AV_GRISE)
				Harmony.Retour		= XMENEXT_SIMULATION_TOUCHE
				Harmony.Cplretour	= K_F10
			EndIf			
		EndIf		
	EndIf
	If GR1_PARAM_ENTETE.Choix(3) <> 1
		GR1_PARAM_ENTETE.Choix(1) = TRUE
	EndIf
	
Endp

