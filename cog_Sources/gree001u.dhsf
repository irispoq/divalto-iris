Public Procedure Champ_FiltreValeur_1_Ap		; Après la saisie d'une valeur de rubrique
1		RbqVal			>A5DD.dhsd RbqVal
1		RbqValN0 		14,0
1		RbqValN1 		15,1
1		RbqValN2 		16,2
1		RbqValN3 		17,3
1		RbqValN4 		18,4
1		RbqValN5 		19,5
1		RbqValN6 		20,6
1		RbqValN7 		21,7
1		RbqValN8			22,8
1		RbqValN9			23,9
1		res				1,0

Beginp
	If GR001.FiltreChampOdbc(Harmony.Dataind1) <> ' ' And GR001.FiltreValeur(Harmony.Dataind1) <> ' ' And GR001.FiltreOp(Harmony.Dataind1) not in (8, 9, 10)
		If Rechercher_Rubrique(GR001.FiltreChampOdbc(Harmony.Dataind1)) = 0
			RbqVal = GR001.FiltreValeur(Harmony.Dataind1)
			Switch MRBQ.RubValTyp								; Cadrage en fonction du type de la rubrique
			Case 2													; Si la rubrique est de type numérique
				If IsNumer(RbqVal) = 0
					Mes_Alert(3101, MRBQ.Lib80)
					Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
					Preturn
				EndIf

				Switch MRBQ.Dec
				Case 0	|	RbqValN0 = Val(RbqVal)	|	RbqVal = RbqValN0
				Case 1	|	RbqValN1 = Val(RbqVal)	|	RbqVal = RbqValN1
				Case 2	|	RbqValN2 = Val(RbqVal)	|	RbqVal = RbqValN2
				Case 3	|	RbqValN3 = Val(RbqVal)	|	RbqVal = RbqValN3
				Case 4	|	RbqValN4 = Val(RbqVal)	|	RbqVal = RbqValN4
				Case 5	|	RbqValN5 = Val(RbqVal)	|	RbqVal = RbqValN5
				Case 6	|	RbqValN6 = Val(RbqVal)	|	RbqVal = RbqValN6
				Case 7	|	RbqValN7 = Val(RbqVal)	|	RbqVal = RbqValN7
				Case 8	|	RbqValN8 = Val(RbqVal)	|	RbqVal = RbqValN8
				Case 9	|	RbqValN9 = Val(RbqVal)	|	RbqVal = RbqValN9
				EndSwitch
			
			Case 3													; Si la rubrique est de type date, la mettre au format YYYYMMDD
				If ( res := FDate(RbqVal, RbqVal) ) <> 1
					Mes_Alert(3102, MRBQ.Lib80)
					Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
					Preturn
				EndIf

			EndSwitch
			GR001.FiltreValeur(Harmony.Dataind1) = RbqVal
		EndIf
	EndIf
Endp

