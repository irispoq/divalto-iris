public procedure Champ_Statdc_1_Av				; avant découpage code stat tiers
beginp
	if Dossier.STATLIB(Harmony.DataInd1) = ' '
		Harmony.Retour = Xmenext_Saut_Donnee
	elsif Harmony.DataInd2 > 1
		if Dossier.STATDC(Harmony.DataInd1,Harmony.DataInd2 - 1) = 0
         Harmony.Retour = Xmenext_Saut_Donnee
		endif
	endif
endp

;*
public procedure Champ_Statdc_1_Ap				; controle decoupage code stat tiers
1	i		1,0	
beginp
	if Dossier.STATDC(Harmony.DataInd1,Harmony.DataInd2) > 8
		Mes_Alert (1004)
      Harmony.Retour = 3
	elsif Dossier.STATDC(Harmony.DataInd1,Harmony.DataInd2) = 0
      for i = Harmony.DataInd2 to 4
         Dossier.STATDC(Harmony.DataInd1,i) = ' '
         Dossier.STATLIBDC(Harmony.DataInd1,i) = ' '
		next
	endif
endp

;*
public procedure Champ_STATDC_2_Av				; avant libellé associé au découpage tiers
beginp
   if Dossier.STATDC(Harmony.DataInd1,Harmony.DataInd2) = 0
      HARMONY.RETOUR = Xmenext_Saut_Donnee
	endif
endp

;*
public procedure Champ_STATDC_3_Ap				; après libellé code statistique	tiers
1	i	1,0
beginp
	
	;MAJ combo pour dasstatno
	soc = dossier.Soc

   if Dossier.STATLIB(Harmony.DataInd1) = ' '
	   for i = 1 to 4
	      Dossier.STATDC(Harmony.DataInd1,i) = ' '
	      Dossier.STATLIBDC(Harmony.DataInd1,i) = ' '
		next
	endif
	if Dossier.STATLIB(Harmony.DataInd1) <> ' ' and Dossier.STATLIBDC(Harmony.DataInd1,1) = ' '
   	Dossier.STATDC(Harmony.DataInd1,1) = 8
	   Dossier.STATLIBDC(Harmony.DataInd1,1) = Dossier.STATLIB(Harmony.DataInd1)
	endif
endp

