   if Dossier.FAMLIB(Harmony.DataInd1) = ' '
	   for i = 1 to 4
	      Dossier.FAMDC(Harmony.DataInd1,i) = ' '
	      Dossier.FAMLIBDC(Harmony.DataInd1,i) = ' '
