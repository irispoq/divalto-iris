SetModuleInfo('$Id: gteq721_sql.dhsf 51516 2015-11-19 12:34:13Z als $')
;*
;*   Inventaire comptable différé
;*

;*
;*		Déclarations
;*

Include	'GTTC000.dhsp'

Public RecordSql	'gtrsdos.dhoq'	Dossier		Dossier
Public RecordSql	'GTRSHQINV.dhoq'	InvArtMouv	_ART

Public Record 		'GTFDD.dhsd'		XQ 		G3XQ
Public Record		'A5DD.dhsd'			MZ									; Sauvegarde MZ pour double xmiload.
Public Record 		'GTFDD.dhsd'		ART
Public Record 		'GTFDD.dhsd'		GT721

;*
;*			Séquences appelées par les données du masque
;*

;*
Public Procedure Champ_Cptsf_1_Ap
Beginp
	Controle_DonneeFin(G3XQ.CptSD, G3XQ.CptSF, 1, 7937)
Endp 

;*
Public Procedure Champ_Cptsmsk_1_Ap
Beginp
	Controle_Masquage(G3XQ.CptSMsk, 7916, 1)   
Endp 

;*
Public Procedure Champ_DepoF_1_Ap
Beginp
	Controle_DonneeFin(G3XQ.DepoD, G3XQ.DepoF, 1, 7909)
Endp 

;*
Public Procedure Champ_Etbd_1_Av
Beginp
	If Dossier.EntCodN(22) = 1
 		Harmony.Retour = XMENEXT_SAUT_DONNEE 
	EndIf
Endp 

;*
Public Procedure Champ_Etbf_1_Ap
Beginp
	Controle_DonneeFin(G3XQ.EtbD, G3XQ.EtbF, 1, 7910)
Endp 

;*
Public Procedure Champ_FamD_1_Av
Beginp
	If Dossier.FamLib(Harmony.DataInd1) = ' '
 		Harmony.Retour = XMENEXT_SAUT_DONNEE
	EndIf
Endp 

;*
Public Procedure Champ_FamF_1_Ap
Beginp
	Controle_DonneeFin(G3XQ.FamD(Harmony.DataInd1), G3XQ.FamF(Harmony.DataInd1), 1, 7908)
Endp 

;*
Public Procedure Champ_FamMsk_1_Ap
Beginp
	Controle_Masquage(G3XQ.FamMsk(Harmony.DataInd1), 7916, 1)  
Endp 

;*
Public Procedure Champ_NstFin_1_Ap
Beginp
	Controle_DonneeFin(G3XQ.NstDeb, G3XQ.NstFin, 1, 7935)
Endp 

;*
Public Procedure Champ_Nstmsk_1_Ap
Beginp
	Controle_Masquage(G3XQ.NstMsk, 7916, 1)   
Endp 

;*
Public Procedure Champ_RefF_1_Ap
Beginp
	Controle_DonneeFin(G3XQ.RefD, G3XQ.RefF, 1, 7903)
Endp 

;*
Public Procedure Champ_Refmsk_1_Ap
Beginp
	Controle_Masquage(G3XQ.RefMsk, 7916, 1)    
Endp 

;*
Public Procedure Champ_SREF_1_Av
Beginp
	If Dossier.EntCodN(17) = 1
 		Harmony.Retour = XMENEXT_SAUT_DONNEE
	EndIf
Endp 

;*
Public Procedure Champ_SREF_1_Ap
Beginp
	Controle_DonneeFin(G3XQ.Sref1D, G3XQ.Sref1F, 1, 7904)
Endp 

;*
Public Procedure Champ_SREF_2_Ap
Beginp
	Controle_DonneeFin(G3XQ.Sref2D, G3XQ.Sref2F, 1, 7905)
Endp 

;*
Public Procedure Champ_SREFMsk_1_Ap
