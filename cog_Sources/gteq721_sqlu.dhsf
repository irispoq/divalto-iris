	Controle_Masquage(G3XQ.Sref1Msk, 7916, 1)
Endp 

;*
Public Procedure Champ_SREFMsk_2_Ap
Beginp
	Controle_Masquage(G3XQ.Sref2Msk, 7916, 1)
Endp 

;*
;*
;*

;*
Public Procedure    Page_Selection_1_Ap ; après page sélection
1		cpt			3,0	; Compteur de parcours

Beginp
	Controle_DonneeFin(G3XQ.RefD	, G3XQ.RefF		, 1, 7903)
	Controle_DonneeFin(G3XQ.Sref1D, G3XQ.Sref1F	, 1, 7904)
	Controle_DonneeFin(G3XQ.Sref2D, G3XQ.Sref2F	, 1, 7905)
	Controle_DonneeFin(G3XQ.DepoD	, G3XQ.DepoF	, 1, 7909)
	Controle_DonneeFin(G3XQ.EtbD	, G3XQ.EtbF		, 1, 7910)
	Controle_DonneeFin(G3XQ.NstDeb, G3XQ.NstFin	, 1, 7935)
	Controle_DonneeFin(G3XQ.CptSD	, G3XQ.CptSF	, 1, 7937)

	For cpt = 1 to 3
		Controle_DonneeFin(G3XQ.FamD(cpt), G3XQ.FamF(cpt), 1, 7908)
	Next
Endp

