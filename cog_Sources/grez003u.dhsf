Public Procedure Champ_Tiers_1_Av
;	

Beginp

	; Interdiction de modifier le tiers si piloté par tiers, et non piloté par no d'événement ou no d'événement père
	If GR1.EvtNo = ' ' and GR1.EvtNoPere = ' '
		GRXZ.Pong = FALSE
		If GR1.TiersIndividuPilote <> ' '	
			EVTTIERS.TiersIndividu = GR1.TiersIndividuPilote
			Controler_Tiers (EVTTIERS.TiersIndividu)
			GR1.Lib(1)		= PCFIDX.Nom
			Harmony.Retour	= XMENEXT_SAUT_DONNEE
		ElsIf GR1.TiersIndividu <> ' '
			EVTTIERS.TiersIndividu = GR1.TiersIndividu
			Controler_Tiers (EVTTIERS.TiersIndividu)
			GR1.Lib(1)		= PCFIDX.Nom
			Harmony.Retour	= XMENEXT_SAUT_DONNEE
		EndIf
	EndIf

Endp

;*
Public Procedure Champ_Tiers_1_Ap
;

Beginp

	If EVTTIERS.TiersIndividu = ' '
		CLI			= ' '
		PINDIVIDU	= ' '
		PCFIDX		= ' '
		T2				= ' '
	EndIf
	
	If Harmony.Datamodif = TRUE

		EVTTIERS.Contact	= ' '
		GR1.Lib(3)			= ' '
		GR1.Lib(1)			= ' '
		If GR1.PiloteSt = 2 and HARMONY.Cliconglet = 2
			HARMONY.Retour = XMENEXT_TRAITEMENT_AVANT
			PReturn
		EndIf
	
		If EVTTIERS.TiersIndividu <>' '
			If Controler_Tiers (EVTTIERS.TiersIndividu, 2000, Today, TRUE) <> 0
				Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
			Else
				If MUSER.ReprUser = 2 and PCFIDX.Repr(1) <> GR1.UserRepr	and PCFIDX.Ce2 <> '7'	; on ne vérifie pas les commerciaux pour les individus
					; L'utilisateur n'a pas accès à ce tiers
					Mes_Alert (3005, EVTTIERS.Tiers)
					Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
				ElsIf ( PCFIDX.Ce2 = '2' and MUSER.TiersTyp(1) <> 2 ) \
				Or ( PCFIDX.Ce2 = '3' and MUSER.TiersTyp(2) <> 2 ) \
				Or	( PCFIDX.Ce2 = '4' and MUSER.TiersTyp(3) <> 2 ) \
				Or	( PCFIDX.Ce2 = '5' and MUSER.TiersTyp(4) <> 2 ) \
				Or	( PCFIDX.Ce2 = '6' and MUSER.TiersTyp(5) <> 2 ) \
				Or	( PCFIDX.Ce2 = '7' and MUSER.TiersTyp(6) <> 2 )
					; le type de tiers n'est pas coché
					Mes_Alert (3006, EVTTIERS.TiersIndividu)
					Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
				Else
					GR1.Lib(1)		= PCFIDX.Nom
					EVTTIERS.Ce2	= PCFIDX.Ce2
					Rechercher_Tiers_Individu (EVTTIERS.Ce2, EVTTIERS.TiersIndividu)
					XmeMenuEnableItem (ZOOM.Idmenu, 'TIERS', TRUE)
					XmeToolbarEnableButton (IdOutilZoom, 'TIERS', TRUE)
					; Si le tiers est de type VRP ou individu, grisage des boutons et zones contact
					If EVTTIERS.Ce2 in ('5', '7')
						XmeSetAttribut ('evt_contact_ppal', AN_VISIBILITE, AV_GRISE)
					Else
						XmeSetAttribut ('evt_contact_ppal', AN_VISIBILITE, AV_VISIBLE)
					EndIf
				EndIf
			EndIf
		Else
			XmeMenuEnableItem (ZOOM.Idmenu, 'TIERS', FALSE)
			XmeToolbarEnableButton (IdOutilZoom, 'TIERS', FALSE)
		EndIf
	EndIf

Endp	

;*
Public Procedure Champ_Tiers_2_Ap
;	

Beginp

	GR1_ENTETE.Lib(1)				= ' '
	GR1_ENTETE.TiersIndividu	= ' '
	If GR1.Tiers <> ' '
		If Controler_Tiers (GR1.TiersIndividu, 2000) <> 0
			Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
		Else
			If MUSER.ReprUser = 2 and PCFIDX.Repr(1) <> GR1.UserRepr	and PCFIDX.Ce2 <> '7'	; on ne vérifie pas les commerciaux pour les individus
				; L'utilisateur n'a pas accès à ce tiers
				Mes_Alert (3005, GR1.Tiers)
				Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
			ElsIf ( PCFIDX.Ce2 = '2' and MUSER.TiersTyp(1) <> 2 ) \
			Or ( PCFIDX.Ce2 = '3' and MUSER.TiersTyp(2) <> 2 ) \
			Or ( PCFIDX.Ce2 = '4' and MUSER.TiersTyp(3) <> 2 ) \
			Or ( PCFIDX.Ce2 = '5' and MUSER.TiersTyp(4) <> 2 ) \
			Or	( PCFIDX.Ce2 = '6' and MUSER.TiersTyp(5) <> 2 ) \
			Or	( PCFIDX.Ce2 = '7' and MUSER.TiersTyp(6) <> 2 )
				; Le type de tiers n'est pas sélectionné
				Mes_Alert (3006, GR1.TiersIndividu)
				Harmony.Retour = XMENEXT_TRAITEMENT_AVANT				
			Else
				GR1_ENTETE.Lib(1)				= PCFIDX.Nom
				; Sauvegarde du tiers sélectionné pour le restaurer si on revient sur cet onglet
				GR1_ENTETE.TiersIndividu	= GR1.TiersIndividu
			EndIf										
		EndIf
	EndIf

Endp


