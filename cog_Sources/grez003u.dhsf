Public Procedure Champ_AdrCod_1_Ap
;
Beginp

	G3XZ.Tiers = EVTTIERS.Tiers
	Recherche_Adresse (EVTTIERS.Tiers, EVTTIERS.AdrCod, 2008)

Endp

;*
Public Procedure Champ_AdrCod_1_Av
; initialise g3xz.tiers pour appel du zoom
Beginp
	If EVTTIERS.Tiers = ' '
		Harmony.Retour = XMENEXT_SAUT_DONNEE
	Else
		G3XZ.Tiers = EVTTIERS.Tiers
	Endif
Endp

;*
Public Procedure Champ_Affaire_1_Ap
;
Beginp

	GR1.Lib(8) = ' '
	If Controle_Affaire (EVTTIERS.Affaire, 5000) = 0
		If EVTTIERS.Tiers <> ' ' and PRJAP.Tiers <> ' ' and EVTTIERS.Tiers <> PRJAP.Tiers
			Mes_Alert (3014, EVTTIERS.Tiers, PRJAP.TIERS, EVTTIERS.Affaire)
		EndIf
		; On contrôle si le statut de l'affaire est égal à confirmé ou démarré
		If EVTTIERS.Affaire <> ' ' and PRJAP.Ce2 not in (2,3)
			Mes_Alert (3018, EVTTIERS.Affaire)
			Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
		EndIf
		GR1.Lib(8) = PRJAP.Lib80
	EndIf

Endp

;*
Public Procedure Champ_Affaire_1_Av
;
Beginp

	; Piloté par affaire
	If GR1.Affaire <> ' '
		Harmony.Retour = XMENEXT_SAUT_DONNEE
	EndIf

Endp

;*
Public Procedure Champ_Affaire_2_Ap
;
Beginp

	GR1_ENTETE.Lib(8) = ' '
	If Recherche_Affaire (EVTTIERS.Affaire) = 0
		GR1_ENTETE.Lib(8) = PRJAP.Lib80
	EndIf

Endp


;*
Public Procedure Champ_AlarmeDuree_1_Av
;
Beginp
	
	If EVTTIERS.Alarme = 3
		Harmony.Retour			= XMENEXT_SAUT_DONNEE
		EVTTIERS.AlarmeDuree	= 1
	EndIf

Endp

;*
Public Procedure Champ_Alarme_1_Ap
;
Beginp

	If EVTTIERS.Alarme = 3
		; Pas d'alarme
		XmeSetAttribut ('ALARME', AN_STYLE, 'CHAMP_NON_SAISI')
		EVTTIERS.AlarmeDuree = 1
	Else
		XmeResetAttribut ('ALARME', AN_STYLE)
	EndIf

Endp

;*
Public Procedure Champ_Choix_1_Ap
;

Beginp

	Initialiser_Type_Evt (GR1.Choix(10), EVTTIERS.RdvOk, EVTTIERS.RealiseOk)

	; Tâche à réaliser
	If EVTTIERS.RealiseOk = 1
		EVTTIERS.RealDt = ' '
		If EVTTIERS.RealUser = ' '
			EVTTIERS.RealUser = EVTTIERS.EvtUser
		EndIf
		XmeSetAttribut ('REAL', AN_POSITION_X, 140)
		XmeSetAttribut ('REAL', AN_VISIBILITE, AV_CACHE)
		XmeSetAttribut ('AREAL', AN_VISIBILITE, AV_VISIBLE)
	EndIf
	
	; Tâche réalisée
	If EVTTIERS.RealiseOk = 2
		XmeSetAttribut ('REAL', AN_VISIBILITE, AV_VISIBLE)
		XmeSetAttribut ('REAL', AN_POSITION_X, 88)
		XmeSetAttribut ('AREAL', AN_VISIBILITE, AV_CACHE)
		If EVTTIERS.RealDt = ' '
			EVTTIERS.RealDt = Today
		EndIf
	EndIf
	
	; Pas une tâche
	If EVTTIERS.RealiseOk = 3
		EVTTIERS.RealDt	= ' '
		EVTTIERS.EvtEchDh	= ' '
		XmeSetAttribut ('REAL', AN_POSITION_X, 140)
		XmeSetAttribut ('REAL', AN_VISIBILITE, AV_CACHE)
		XmeSetAttribut ('AREAL', AN_VISIBILITE, AV_VISIBLE)
		XmeSetAttribut ('TACHE', AN_STYLE, 'CHAMP_NON_SAISI')
	Else
		XmeResetAttribut ('TACHE', AN_STYLE)
	EndIf

	; Pas un rdvs
	If EVTTIERS.RdvOk = 3
		XmeSetAttribut ('RDV', AN_STYLE, 'CHAMP_NON_SAISI')
	Else
		XmeResetAttribut ('RDV', AN_STYLE)
	EndIf

Endp

;*
Public Procedure Champ_Choix_2_Ap
; Après choix des types d'evts à afficher

Beginp

	If Harmony.Datanotif = TRUE
		If GR1.SelAffSt <> 1
			; Types d'événements à afficher est coché => on dégrise les cases à cocher
			XmeSetAttribut ("EvtType", AN_VISIBILITE, AV_VISIBLE) 				
		Else
			; Tous les événements est coché => on grise les cases à cocher et on recharge les événements				
			XmeSetAttribut ("EvtType", AN_VISIBILITE, AV_GRISE)
			Harmony.Retour = XMENEXT_SIMULATION_TOUCHE
			Harmony.Cplretour = K_F10
		EndIf		
	EndIf
	
Endp

;*
Public Procedure Champ_Choix_3_Ap
; Après choix autres que types d'evts à afficher dans l'entête des onglets

Beginp

	If Harmony.Datanotif = TRUE
		Harmony.Retour = XMENEXT_SIMULATION_TOUCHE
		Harmony.Cplretour = K_F10
	EndIf

Endp

;*
Public Procedure Champ_Couleur_1_Ap
;
1 style 32		

Beginp

	If EVTTIERS.Couleur <> 0
		style = Left ('CHAMP_SAISI_' && ToString (EVTTIERS.Couleur))
		XmeSetAttribut ('COULEUR', AN_STYLE, style)
	Else
		XmeResetAttribut ('COULEUR', AN_STYLE)
	EndIf

Endp

;*
Public Procedure Champ_Evenement_1_Ap	
;

1 style 32

Beginp

	If Controler_evenement (EVTTIERS.Evenement, 3000) = 0
		GR1.Lib(2) = GRTEVT.Lib
		Rechercher_Famille (GRTEVT.EvtFam)
		GR1.Lib(14) = GRTEFA.Lib

		; Condidentialité proposée par défaut
		EVTTIERS.Conf = Condition (GRTEVT.Conf <> ' ', GRTEVT.Conf, GRTEFA.Conf)

		; Test si modification et famille d'événement différente
		; Réinitialisation de la couleur et de la bitmap
		If ZOOM.Action = ZOOM_MODIFICATION
			If EVTTIERS.EvtFam <> GRTEVT.EvtFam
				EVTTIERS.Bitmap		= Condition (GRTEVT.Bitmap=' ', 'CRMBASE', GRTEVT.Bitmap)
				EVTTIERS.Couleur		= GRTEVT.Couleur
				EVTTIERS.CouleurFond	= GRTEVT.CouleurFond
				EVTTIERS.EvtFam		= GRTEVT.EvtFam
				If EVTTIERS.Couleur <> 0
					style = Left('CHAMP_SAISI_' && ToString (EVTTIERS.Couleur) )
					XmeSetAttribut('COULEUR', AN_STYLE, style)
				Else
					XmeSetAttribut ('COULEUR', AN_STYLE, 'CHAMP_SAISI')
				EndIf
			EndIf	
		ElsIf Zoom.Action = ZOOM_CREATION
			If GRTEVT.HsDt <> ' ' and Today > GRTEVT.HsDt
				Mes_Alert (3022, GRTEVT.Evenement)
				Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
			EndIf
		EndIf
	EndIf

Endp

;*
Public Procedure Champ_CalDhAff_1_Av
; Avant et après saisie de la date
; Si la date est vide, la positionne à la date du jour
; Recalcule la date calculée
Beginp

	If GR1.CalDhAff = ' '	  	
		GR1.CalDhAff = Time (1)	
	EndIf
	If GR1.CalHrAff = '000000'	and ZOOM.Sens = 1 
		GR1.CalHrAff = '240000'
	EndIf

	DHSECOND (GR1.CalDhAff)		= '00' ; met le nb de secondes à 00
	DHYEAR (EVTTIERS.CalDh)		= 9999 - DHYEAR (GR1.CalDhAff)
	DHMONTH (EVTTIERS.CalDh)	= 99 - DHMONTH (GR1.CalDhAff)
	DHDAY (EVTTIERS.CalDh)		= 99 - DHDAY (GR1.CalDhAff)
	DHHOUR (EVTTIERS.CalDh)		= 99 - DHHOUR (GR1.CalDhAff)
	DHMINUTE (EVTTIERS.CalDh)	= 99 - DHMINUTE (GR1.CalDhAff)
	DHSECOND (EVTTIERS.CalDh)	= 99

Endp

