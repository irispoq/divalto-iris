SetModuleInfo('$Id: gtez027_sql.dhsf 35431 2013-11-15 11:50:30Z jf $')
;*
;*      Zoom sur Adresse	Enregistrement Adresse de GTRSTIERS
;*

Include 'GTTC000.dhsp'

; TDF 
Public HFile		'GTFDD.dhsd'		GTFTAB	GTFTAB

; Enregistrement
Public RecordSql	'GTRSTIERS.dhoq'	Adresse 	
Public Record  	'DDSYS.dhsd'		ZOOM
Public Record  	'GTFDD.dhsd'		SOC
Public Record  	'GTFDD.dhsd'		T013
Public Record  	'GTFDD.dhsd'		T057
Public Record  	'GTFDD.dhsd'		XZ			G3XZ
Public Record 		'GTFDD.dhsd'		G7
Public Record 		'DDSYS.dhsd'		ROWINFO	zoomselection

;*
;*		Séquences liées au masque
;*

;*
Public Procedure Champ_Bljr_1_Ap				; Contrôle jour de livraison
1		cpt		2,0
Beginp
	If Adresse.BlJr <> ' '
		For cpt = 1 to Length(Adresse.BlJr)
			If Mid(Adresse.BlJr, cpt, 1) not in (' ', 1..7)
				Mes_Alert(2015)
				Harmony.Retour = 3
				PReturn
			EndIf
		Next
	EndIf
Endp

;*
Public Procedure Champ_COECOD_1_Ap			; Contrôle coefficient transport
Beginp
	Lectab(10, Adresse.TvaBlCoe, 1110)
Endp

;*
Public Procedure Champ_Cpostal_1_Av			; Pour appel de zoom
Beginp
	G3XZ.Pong = TRUE
Endp

;*
Public Procedure Champ_Cpostal_1_Ap			; Recherche de la ville à partir du code postal
Beginp
	If Harmony.Key = K_CTRL_S
		Adresse.Cpostal	= G3XZ.Cpostal
		Adresse.Vil			= G3XZ.Vil
	EndIf
	G3XZ = ' '

	If Adresse.Vil = ' '							; Uniquement si ville non renseignée
		If Lectab(57, Adresse.Cpostal) = 0
			Adresse.Vil = T057.Vil
		EndIf
	EndIf
Endp

;*
Public Procedure Champ_Pay_1_Ap				; Contrôle pays
Beginp
	Lectab(13, Adresse.Pay, 1113)
Endp

;*
Public Procedure Champ_Vil_1_Av				; Avant ville pour pilotage du code postal
Beginp
	G3XZ.Pong = TRUE
Endp

;*
Public Procedure Champ_Vil_1_Ap				; Après ville pour pilotage du code postal
Beginp
	If Harmony.Key = K_CTRL_S
		Adresse.Cpostal	= G3XZ.Cpostal
		Adresse.Vil			= G3XZ.Vil
	EndIf
	G3XZ = ' '
Endp
[/diva]
