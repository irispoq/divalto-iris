[diva]
Include Gttc000.Dhsp

Public Hfile 	gtfdd.dhsd 	GTFPCF GTFPCF2	

Public RecordSql "gtrstiers.dhoq" adresse adresse
Public Record "gtfdd.dhsd" g7 g7
Public Record "ddsys.dhsd" rowinfo zoomselection
Public Record "Gtfdd.Dhsd"	Cli
public record 	gtfdd.dhsd 	T1 	T1_Bis
Public Record "gtfdd.dhsd" t1 adr_sel

;*
Private Procedure Champ_AdrCod_1_Ap
;
Beginp
	If G7.ZoomPar = 5

		GTFPCF2.KeyValue = MZ.DOS & adresse.AdrCod 
		Do While Iread(GTFPCF2, T1_Bis, "COG_ADRCOD",GTFPCF2.KeyValue)= 0 And MZ.DOS = T1_Bis.DOS And adresse.AdrCod  = T1_Bis.AdrCod
			If T1_Bis.Hsdt <= Today | Redodo | Endif
			If T1_Bis.Tiers <> " " And  Recherche_client(T1_Bis.Tiers) = 0
				ExitDo
			Endif
		Wend
		If T1_Bis.Dos = MZ.dos And adresse.AdrCod = T1_Bis.AdrCod
			Messagebox("Site déjà éxistant","Erreur",MB_ICONEXCLAMATION)
			harmony.retour = xmenext_ressaisie
		Endif  
	Endif	
Endp

;*
Private Procedure Champ_Tiers_2_Ap
Beginp
	Controle_Client (adresse.TIERS, 2000,true,today,true)
   G7.Lib(1) = CLI.Nom
Endp


;*
Private Procedure Champ_Tiers_2_Av
Beginp
	If Not nospaces(G7.TIERS) = system.esp
		adresse.Tiers = G7.TIERS
		harmony.retour = xmenext_saut_donnee
	Endif
Endp
[/diva]
