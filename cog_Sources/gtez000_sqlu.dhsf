Public Procedure Champ_SrefCod_1_Ap			; Après gestion des sous-références
Beginp
	If ARTICLE.SrefCod = 1
		ARTICLE.GriCod		= ' '
		ARTICLE.GriSais	= 1
	EndIf
Endp

;*
Public Procedure Champ_StCod_1_Ap			; Après suivi stock
Beginp
	If Zoom.Action = ZOOM_MODIFICATION And Harmony.DataModif And ARTICLE.StCod <> 2
		
		If Controler_NiStock_NiReservation(ARTICLE.Ref) <> 0
			Mes_Alert(2131)
			Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
			PReturn
		EndIf

	EndIf

	If ARTICLE.StCod = 3	|	ARTICLE.GriSais = 1	|	EndIf
Endp

