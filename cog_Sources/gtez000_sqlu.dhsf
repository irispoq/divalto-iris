;*
Public Procedure Champ_REF_1_Ap				; Après référence de remplacement
1		ref		>GTFDD.dhsd Ref
Beginp
	ARTICLE.ArticleRemplacement_Des = ' '
	ref = ARTICLE.RefRpl
	PingLocal('ZOOM_ART', ART)
	If Controle_Article(ref, 2100, conf=FALSE) = 0	|	ARTICLE.ArticleRemplacement_Des = ARTICLE.Des	|	EndIf
	PingLocalReceive('ZOOM_ART', ART)
	ARTICLE.RefRpl = ref
Endp

;*
Public Procedure Champ_REF_2_Ap				; Après référence de tarification
1		ref		>GTFDD.dhsd Ref
Beginp
	ARTICLE.ArticleMemeTarif_Des = ' '
	ref = ARTICLE.TaRef
	PingLocal('ZOOM_ART', ART)
	If Controle_Article(ref, 2100, conf=FALSE) = 0	|	ARTICLE.ArticleMemeTarif_Des = ARTICLE.Des	|	EndIf
	PingLocalReceive('ZOOM_ART', ART)
	ARTICLE.TaRef = ref
Endp

;*
Public Procedure Champ_Ref_3_Ap				; référence en création
Beginp
	If ARTICLE.Ref = ' ' And Mes_Alert(2109, bouton=MB_OKCANCEL+MB_ICONQUESTION) <> IDOK
		Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
	EndIf 
Endp

;*
Public Procedure Champ_Ref_4_Ap			; controle référence si création automatique a échouée
Beginp
	If Res_ART(ARTICLE.Ref, 0, NoWait)  <> 0
		Mes_Alert(2101, ARTICLE.Ref)
		Harmony.Retour = XMENEXT_TRAITEMENT_AVANT 
		PReturn
	EndIf

	PingLocal('ZOOM_ART', ART)
	if Seek_ART(ARTICLE.Ref) = 0 	; recherche article existe ?
		PingLocalReceive('ZOOM_ART', ART)
		Lib_ART(ARTICLE.Ref)
		Mes_Alert(2111, ARTICLE.Ref)
		Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
	EndIf
	PingLocalReceive('ZOOM_ART', ART)
Endp

