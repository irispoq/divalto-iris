;*
Private Procedure Champ_Cog_CptIA_1_Ap
;
Beginp

	ARTICLE.CompteImmoAch_Lib = ' '

	If ARTICLE.Cog_CptIA = ' '
		PReturn
	EndIf

	If SOC.EntCodN(20) = 2 
		If Controle_Compte(ARTICLE.Cog_CptIA, conf=FALSE) <= 1
			ARTICLE.CompteImmoAch_Lib = C3.Lib
		EndIf
	Else
		Controle_Compte(ARTICLE.Cog_CptIA, FALSE, existence=FALSE)
	EndIf

Endp


;*
Private Procedure Champ_Cog_CptIS_1_Ap
;
Beginp

	ARTICLE.CompteImmoSt_Lib = ' '

	If ARTICLE.Cog_CptIS = ' '
		PReturn
	EndIf

	If SOC.EntCodN(20) = 2 	 	; controle des données Divalto compta à la saisie
		If Controle_Compte(ARTICLE.Cog_CptIS, conf=FALSE) <= 1
			ARTICLE.CompteImmoSt_Lib = C3.Lib
		EndIf
	Else
		Controle_Compte(ARTICLE.Cog_CptIS, FALSE, existence=FALSE)
	EndIf

Endp


;*
Private Procedure Champ_Cog_Generique_1_Av
;
Beginp

	CogRs_DAR.Init()
	CogRs_DAR.Join.Deactivate("$ALL")
	CogRs_DAR.Where.Equal_Ref(Article.Ref)
	If CogRs_DAR.GetCount() > 0  OR Article.StRes = 2
		Harmony.Retour = XMENEXT_SAUT_DONNEE
	EndIf

Endp


;*
Private Procedure Champ_NatImmo_1_Ap
;
Beginp

	ARTICLE.NATI_Lib = ' '
	If Lectab_HD(ARTICLE.NatImmo, 9163) = 0
		ARTICLE.NATI_Lib = HD.Lib
	EndIf

Endp


;*
Private Procedure Champ_StRes_1_Ap
;
Beginp

	If Article.StRes = 2
		Article.Cog_Generique = 1
	EndIF

Endp
