;*
public Procedure Champ_Un_1_Ap				; après l'unité de vente recalcul de refqte

1	x			1,0				; compte-rendu fonction
1	qte		13,0
BeginP
	; modification autorisée
	G1T1.LIB(11) = ' '

	if G1T1.VENUN = ' '
		G1T1.VENUN = G1T1.REFUN
	endif
			
	if ART.GICOD = 2 and G1T1.VENUN <> G1T1.REFUN
		G1T1.VENUN = G1T1.REFUN
		Mes_alert(2223)
		Harmony.Retour = XmeNext_Traitement_Avant 
	
	elsif Lectab_Unite(G1T1.REF,G1T1.SREF1,G1T1.SREF2,G1T1.VENUN,1123) = 0

		if (x:=Convertir_Qte_Refqte(G1T1.REF,G1T1.SREF1,G1T1.SREF2,G1T1.DstQte,G1T1.REFQTE,G1T1.VENUN,G1T1.REFUN)) > 1
	
		; recherche du coefficient venun vers refun  et conversion refqte
		; dépassement de capacité

			Mes_Alert(2222,G1T1.DstQte,G1T1.REFUN)
			Harmony.Retour = XmeNext_Traitement_Avant
		elsif x = 1
			; table de coefficient inexistante
			Mes_Alert(1105,G1T1.VenUn,G1T1.RefUn)
			Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
		Else
			G1T1.Lib(11) = T023.Lib
			If	Harmony.Datamodif And G1T1.Pcod(1) <> C_Prix_Manuel
				; si prix n'est pas manuel recherche du code tarif
				Recherche_Famille_Tarif_G1T1(G1T1.DstQte,Piece_Ligne_LireSlg(G1T1.MOUV,ENT.PiCod),2225)			
				Maj_Promotyp
				Piece_Calculer_HT_G1T1
			endif
			; convertir les ventilations
			Convertir_Qte_Refqte_Tous_Vtl

			if Harmony.DataModif 
				; emballage
				if Lectab_EmbCoef(G1T1.REF,G1T1.SREF1,G1T1.SREF2,G1T1.VENUN, G1T1.DstQte) = 0 
					G1T1.EMBUN	= T033.EMBUN
					if T033.EMBQTE <> 0
						if modulo(G1T1.DstQte,T033.EMBQTE) = 0
							G1T1.EMBQTE = G1T1.DstQte / T033.EMBQTE
						else
							truncate
							qte = G1T1.DstQte / T033.EMBQTE 
							G1T1.EMBQTE = qte + 1
							round
						endif
					else
						G1T1.EMBQTE = 0
					endif
				else
					G1T1.EMBUN = ' '
					G1T1.EMBQTE = 0
				endif 		
			endif 		

		endif
	endif
EndP

