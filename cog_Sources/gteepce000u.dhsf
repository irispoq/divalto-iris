	if ENT.ADRTIERS(Harmony.DataInd1) = ' '
	
		if ENT.ADRCOD(Harmony.DataInd1) = ' '
			; adresse du tiers

			GTENT.ADR(Harmony.DataInd1)= Piece_Tiers_Adr(Harmony.Dataind1)
			if Harmony.DataInd1 = 3	and ENT.TiCod = 'C'
				ENT.TOUR		= CLI.TOUR
				ENT.TOURRG	= CLI.TOURRG
				ENT.TRANSJRNB= CLI.TRANSJRNB
			endif

		elsif (flag:= Recherche_Adresse(ENT.TIERS,ENT.ADRCOD(Harmony.DataInd1),2008,ENT.PIDT,Harmony.DataInd1)) = 0
				; recherche du code adresse 

				GTENT.ADR(Harmony.DataInd1)= T1.ADR
				if Harmony.DataInd1 = 3 and ENT.TiCod = 'C'
					if T1.TOUR <> ' '
						ENT.TOUR = T1.TOUR
					else
						ENT.TOUR	= CLI.TOUR
					endif
					if T1.TOURRG <> ' '
						ENT.TOURRG = T1.TOURRG
					else
						ENT.TOURRG = CLI.TOURRG
					endif
					if T1.TRANSJRNB <> 0
						ENT.TRANSJRNB= T1.TRANSJRNB
					else
						ENT.TRANSJRNB= CLI.TRANSJRNB
					endif
				endif

		else
				ENT.ADRCOD(Harmony.DataInd1) = ' '
				GTENT.ADR(Harmony.DataInd1)= CLI.ADR
				if Harmony.DataInd1 = 3
					ENT.TOUR		= CLI.TOUR
					ENT.TOURRG	= CLI.TOURRG
					ENT.TRANSJRNB= CLI.TRANSJRNB
				endif
		endif
	else
		; controle du client
		; enregistrement cli est écrasé par controle_client							
		
		Piece_Sauver_Tiers
		if GTPCE.TicodDst='F' and ((flag:= Controle_Fournisseur (ENT.ADRTIERS(Harmony.DataInd1), 0,0,ENT.PIDT,0))  = 0)
				GTENT.ADR(Harmony.DataInd1) = FOU.ADR
 		elsif GTPCE.TicodDst='F' and flag <5
				switch flag
				case	1
					Mes_Alert(2004,ENT.ADRTIERS(Harmony.DataInd1))
				default
					Mes_Alert(1902,ENT.ADRTIERS(Harmony.DataInd1))
				endswitch
				Harmony.Retour = Xmenext_Traitement_Avant
		elsif ( GTPCE.TicodDst<>'F' and (flag:=Controle_Client (ENT.ADRTIERS(Harmony.DataInd1), 2000,0,ENT.PIDT,0))  = 0) or \
			( GTPCE.TicodDst='F' and (flag:=Controle_Client (ENT.ADRTIERS(Harmony.DataInd1), 0,0,ENT.PIDT,0))  = 0	)
			if ENT.ADRCOD(Harmony.DataInd1) = ' '
				GTENT.ADR(Harmony.DataInd1) = CLI.ADR
				if Harmony.DataInd1 = 3
					ENT.TOUR		= CLI.TOUR
					ENT.TOURRG	= CLI.TOURRG
					ENT.TRANSJRNB= CLI.TRANSJRNB
				endif

			elsif	(flag := Recherche_Adresse(ENT.ADRTIERS(Harmony.DataInd1),ENT.ADRCOD(Harmony.DataInd1),2008,ENT.PIDT,Harmony.DataInd1)) = 0
				GTENT.ADR(Harmony.DataInd1)= T1.ADR
				if Harmony.DataInd1 = 3 
					if T1.TOUR <> ' '
						ENT.TOUR = T1.TOUR
					else
						ENT.TOUR	= CLI.TOUR
					endif
					if T1.TOURRG <> ' '
						ENT.TOURRG = T1.TOURRG
					else
						ENT.TOURRG = CLI.TOURRG
					endif
					if T1.TRANSJRNB <> 0
						ENT.TRANSJRNB= T1.TRANSJRNB
					else
						ENT.TRANSJRNB= CLI.TRANSJRNB
					endif
				endif

			else
				ENT.ADRCOD(Harmony.DataInd1) = ' '
				GTENT.ADR(Harmony.DataInd1)= CLI.ADR
				if Harmony.DataInd1 = 3
					ENT.TOUR		= CLI.TOUR
					ENT.TOURRG	= CLI.TOURRG
					ENT.TRANSJRNB= CLI.TRANSJRNB
				endif
			endif
		elsif GTPCE.TicodDst='F' and flag <5
			switch flag
			case	1
				Mes_Alert(2004,ENT.ADRTIERS(Harmony.DataInd1))
			default
				Mes_Alert(1902,ENT.ADRTIERS(Harmony.DataInd1))
			endswitch
			Harmony.Retour = Xmenext_Traitement_Avant
		elsif (flag:= Controle_Tiers_Autre(ENT.ADRTIERS(Harmony.DataInd1), 1900,0,ENT.PIDT)) = 0
				GTENT.ADR(Harmony.DataInd1) = TIA.ADR
		endif
	
			; erreur traitée par controle_client
		if flag = 0 and ENT.ADRCOD(Harmony.DataInd1) <> ' '
			if	(flag := Recherche_Adresse(ENT.ADRTIERS(Harmony.DataInd1),ENT.ADRCOD(Harmony.DataInd1),2008,ENT.PIDT,Harmony.DataInd1)) = 0
				GTENT.ADR(Harmony.DataInd1)= T1.ADR
			else
				ENT.ADRCOD(Harmony.DataInd1) = ' '
			endif
		endif
		Piece_Restaurer_Tiers
	endif
	if flag = 0					
		; erreur corrigée
		GTENT.ERRFLAG(Harmony.DataInd1) = 0				
	endif

	if ENT.TRANSJRNB <> TransJtNb_Save
		Maj_DelaiCalcule_Tous
	endif

	if Harmony.DataInd1 =3 and TiersLivreAvantSaisie <> ENT.ADRTIERS(Harmony.DataInd1)
		Maj_TiersStock_Tous
	endif

endp

