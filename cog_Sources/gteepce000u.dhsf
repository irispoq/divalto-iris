			Maj_Promotyp

			; Gestion du tarif forfaitaire
			If G1T1.PaForf = 2
				G1T1.Ppar = G1T1.FaQte
			Endif

			; calcul HT d'une ligne
			Calculer_HT_G1T1(G1T1.FAQTE)
		endif
	endif
  	if	GTPCE.PiCodOri = 3 and G1T1.CLE <> ' ' and G1T1.FaQte >= G1T1.StQte
		XmeSetAttribut('BLQTEREL',AN_STYLE,'CHAMP_NON_SAISI')
