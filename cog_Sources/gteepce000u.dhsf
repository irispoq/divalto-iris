Procedure Maj_Champs_Tarifs
Beginp
	
	; Tarifs
	XmeResetAttribut('TAFAM',AN_STYLE)
	XmeResetAttribut('PROMOTACOD',AN_STYLE)
	XmeResetAttribut('TACOD',AN_STYLE)
	If G1T1.Pcod(1) = C_Prix_Manuel
		XmeSetAttribut('TAFAM',AN_STYLE,'CHAMP_NON_SAISI')
		XmeSetAttribut('PROMOTACOD',AN_STYLE,'CHAMP_NON_SAISI')
		XmeSetAttribut('TACOD',AN_STYLE,'CHAMP_NON_SAISI')
	Elsif G1T1.Pcod(1) = C_Prix_Standard_Saisi
		XmeSetAttribut('TAFAM',AN_STYLE,'CHAMP_NON_SAISI')
		XmeSetAttribut('PROMOTACOD',AN_STYLE,'CHAMP_NON_SAISI')
	Elsif G1T1.Pcod(1) = C_Prix_Promotion_Saisi
		XmeSetAttribut('TAFAM',AN_STYLE,'CHAMP_NON_SAISI')
	Endif

	; Remises
	XmeResetAttribut('REFAM',AN_STYLE)
	XmeResetAttribut('PROMOREMCOD',AN_STYLE)
	XmeResetAttribut('REMCOD',AN_STYLE)
	If G1T1.Pcod(2) = C_Prix_Manuel
		XmeSetAttribut('REFAM',AN_STYLE,'CHAMP_NON_SAISI')
		XmeSetAttribut('PROMOREMCOD',AN_STYLE,'CHAMP_NON_SAISI')
		XmeSetAttribut('REMCOD',AN_STYLE,'CHAMP_NON_SAISI')
	Elsif G1T1.Pcod(2) = C_Prix_Standard_Saisi 
		XmeSetAttribut('REFAM',AN_STYLE,'CHAMP_NON_SAISI')
		XmeSetAttribut('PROMOREMCOD',AN_STYLE,'CHAMP_NON_SAISI')
	Elsif G1T1.Pcod(2) = C_Prix_Promotion_Saisi
		XmeSetAttribut('REFAM',AN_STYLE,'CHAMP_NON_SAISI')
	Endif

	; Pas de remises si PubTyp = 2
	XmeResetAttribut('REMCOD2',AN_STYLE)
	If G1T1.PubTyp = 2
		XmeSetAttribut('REMCOD2',AN_STYLE,'CHAMP_NON_SAISI')
		XmeSetAttribut('REFAM',AN_STYLE,'CHAMP_NON_SAISI')
		XmeSetAttribut('REMCOD',AN_STYLE,'CHAMP_NON_SAISI')
		XmeSetAttribut('PROMOREMCOD',AN_STYLE,'CHAMP_NON_SAISI')
	Endif
Endp

