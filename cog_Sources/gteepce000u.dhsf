;*
public procedure Champ_Projet_1_Ap						; controle du projet de l'en-tête
1		i	2,0
1		Tiers	>gtfdd.dhsd Tiers
1		rz			L			; Résultat zoom
1		TypeProjetAffaire	1,0		; retour de fonction pour identifier code projet ou affaire
beginp
	If Harmony.Key = K_F8
		Switch GTPCE.PiCodDst
		case GTPCE.TicodDst = 'F' 	|rz = Zoom_Call(9500)
		Case 1	|	rz = Zoom_Call(9843)
		Default	|	rz = Zoom_Call(9868)
 		EndSwitch
		If rz <> E_ZOOM_OK	|	PReturn	|	EndIf
	EndIf
	If Harmony.Key = K_SF8 and  GTPCE.PiCodDst = 1
		rz = Zoom_Call(9868)
		If rz <> E_ZOOM_OK	|	PReturn	|	EndIf
	endif
	GTENT.LIB(4) = ' '
	if GTPCE.TiCodDst in ('P','C')
		tiers = ENT.Tiers
	else
		tiers=  ' '
	endif
	TypeProjetAffaire = 2
	if ((GTPCE.PicodDst = 1) and (Controle_Projet_Ou_Affaire(ENT.PROJET,5005,tiers,conf=false,niv=1,TypeTable=TypeProjetAffaire) = 0))	 \
		or ((GTPCE.PicodDst <> 1) and (Controle_Affaire(ENT.PROJET, 5000, tiers, conf=false,niv=1) = 0))
		if TypeProjetAffaire = 1
			GTENT.LIB(4) = PRJAV.LIB80
		else	
			GTENT.LIB(4) = PRJAP.LIB80
		endif
		
		; recherche si axe par défaut = projet
		for i = 1 to 4
			if SOC.AXEMAJTYP(i) = 8 and GTENT.PROJET = ENT.AXE(i)
				ENT.AXE(i) = ENT.PROJET
				
				; controle axe dans C3
				if Soc_Controle_Donnees_comptables and Recherche_Axe(i, ENT.AXE(i),false) = 0
					GTENT.LIB(7+i) = C5.LIB
				else
					ENT.AXE(i) = ' '
