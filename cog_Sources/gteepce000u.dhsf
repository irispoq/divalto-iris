;*
public procedure Champ_Repr_4_Ap			;controle représentant ligne
1		irepr		1,0		; obligatoirement en 1,0
beginp
	if Harmony.DataModif
		
		; on repasse en commissionnement standard
		irepr	= Harmony.DataInd1
		G1T1.PCOD(2+Harmony.DataInd1) = 1
		G1T1.LIB(12+Harmony.Dataind1) = ' '
		XmeResetAttribut(['COFAM' irepr],AN_STYLE)

		if G1T1.REPR(Harmony.DataInd1) = ' '
			G1T1.COFAMV(Harmony.Dataind1) = ' '
			G1T1.COMP(Harmony.Dataind1) = ' '
			G1T1.COMMT(Harmony.Dataind1) = ' ' 

			switch  Harmony.DataInd1
			case 1
				XmeSetAttribut('REPRL1COMP',AN_STYLE,'CHAMP_NON_SAISI')
			case 2
				XmeSetAttribut('REPRL2COMP',AN_STYLE,'CHAMP_NON_SAISI')
				XmeSetAttribut('REPRL3',AN_STYLE,'CHAMP_NON_SAISI')
				XmeSetAttribut('REPRL3COMP',AN_STYLE,'CHAMP_NON_SAISI')
				G1T1.REPR(3) = ' '
				G1T1.COFAMV(3) = ' '
				G1T1.COMP(3) = ' '
				G1T1.COMMT(3) = ' '
				G1T1.LIB(15) = ' '
			case 3
				XmeSetAttribut('REPRL3COMP',AN_STYLE,'CHAMP_NON_SAISI')
			endswitch

		else
				XmeResetAttribut(['REPRL' irepr 'COMP'],AN_STYLE)
				if irepr = 2
					XmeResetAttribut('REPRL3',AN_STYLE)
				endif

				if G1T1.REPR(Harmony.DataInd1) = ENT.REPR(Harmony.DataInd1)
				
					; on reprend les valeurs de l'en-tête
					G1T1.COFAMV(Harmony.Dataind1) = ENT.COFAMV(Harmony.DataInd1)
					G1T1.COMP(Harmony.Dataind1) 	= ENT.COMP(Harmony.DataInd1)
					G1T1.LIB(12+Harmony.DataInd1) = GTENT.LIB(17+Harmony.DataInd1)
					Recherche_Commission(G1T1.COMP(Harmony.Dataind1),G1T1.COFAMV(Harmony.Dataind1),ENT.COFAM,G1T1.COFAMR, \
												G1T1.COMP(Harmony.Dataind1))
					
				elsif Controle_VRP ( G1T1.REPR(Harmony.DataInd1),1700) = 0
					if Harmony.DataInd1 = 1 and G1T1.REPR(1) <> ' 'and VRP.REPRTYP <> 2
						Mes_Alert (1706)
						Harmony.Retour = XmeNext_Traitement_Avant
					else
						G1T1.LIB(12+Harmony.Dataind1) = VRP.NOM
						G1T1.COFAMV(Harmony.Dataind1) = VRP.COFAMV
						G1T1.COMP(Harmony.Dataind1) = VRP.COMP
						Recherche_Commission(G1T1.COMP(Harmony.Dataind1),G1T1.COFAMV(Harmony.Dataind1),ENT.COFAM,G1T1.COFAMR, \
													G1T1.COMP(Harmony.Dataind1))
					endif
				endif
		endif
	endif
endp

