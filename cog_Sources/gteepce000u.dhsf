;*
public procedure Champ_Op_1_Ap						; contrôle du code opération de la ligne
1		op			>gtfdd.dhsd	OP
1		sens		>gtfdd.dhsd	SENS
1		saip		>gtfdd.dhsd	SAIP
1		nst		>gtfdd.dhsd	NST
1		majStRes 1,0 =	false

beginp
	if Harmony.DataModif
		op		= G1T1.OP
		sens 	= G1T1.SENS
		saip	= G1T1.SAIP
		nst	= G1T1.NST
		pinglocal ('GTTE220_T020',T020) 
		if Recherche_OP_G1T1(1120) = 0
			if OP_Evolution(T020) > OP_Evolution(T020_ENT)
				Mes_Alert(1373)
				G1T1.SENS	= sens
				G1T1.SAIP	= saip
				G1T1.NST		= nst
				pinglocalreceive ('GTTE220_T020',T020) 				
				Harmony.Retour = XmeNext_Traitement_Avant
				preturn
			endif

			if sens <> G1T1.SENS 
				if LireProgrammePhase() <> C_Phase_LigneCreat
					; modification du sens interdite en dehors de la création de ligne
					Mes_Alert(1368)
					G1T1.SENS	= sens
					G1T1.SAIP	= saip
					G1T1.NST		= nst
					pinglocalreceive ('GTTE220_T020',T020) 				
					Harmony.Retour = XmeNext_Traitement_Avant	
					preturn
				endif

				; changement de sens recalculer les coûts
				Cout_Saisie_Sortie_G1T1		
				Cout_Saisie_Entree_G1T1
				if G1T1.DECCOD = oui
					G1T1.CR	= 0 - G1T1.CR 
					G1T1.CMP	= 0 - G1T1.CMP
					G1T1.CRTOTMT	= 0 - G1T1.CRTOTMT
					G1T1.CMPTOTMT	= 0 - G1T1.CMPTOTMT
				endif
				if G1T1.SENS = 1 or G1T1.PiCod = 1
					XmeSetAttribut('RESERVER2',AN_VISIBILITE,AV_CACHE)
					G1T1.STRES = Non
					majStRes =	true
				else
					XmeResetAttribut('RESERVER2',AN_VISIBILITE)
					G1T1.STRES = Init_Reservation(3)
					majStRes =	true
				endif
			endif

			; recherche emplacement implicite car dépend de la nature de stock 
			if nst <> G1T1.NST
				if Soc_Gerer_Emplacements and T017_Ori.LIEUCOD = Oui
					; recherche ArtDepo pour initialiser emplacement même si saisie en grille
					; sera pris en compte pour les prochaines créations de ventilation pas de màj automatique des emplacements déjà saisis
					Recherche_ArtDepo(G1T1.REF, G1T1.DEPO,G1T1.SREF1, G1T1.SREF2,G1T1.NST )
				endif	
				majStRes = true
			endif
			if majStRes
				Maj_Vtl_Stres
			endif

			G1T1.LIB(1) = T020.LIB	
		endif
	endif
endp

;*
public procedure Champ_Op_2_Ap						; contrôle du code opération de l'en-tête
1		sens		>gtfdd.dhsd	SENS
beginp
	sens 	= GTENT.SENS
	if ENT.OP = T020_Menu.OP
		T020_ENT = T020_Menu
	elsif Recherche_OP_ENT(1120) <> 0
			preturn
	else
		if OP_Evolution(T020) < OP_Evolution(T020_ENT) and OP_Evolution(T020) < OP_Evolution_ToutesLignes(T020.OP)
				Mes_Alert(1364)
				Harmony.retour = XmeNext_Traitement_Avant
				preturn
		endif
		T020_ENT = T020
	endif
	GTENT.SENS = Condition(ENT.ASCOD = Non, T020_ENT.SENS, Condition(T020_ENT.SENS=1,2,1))
	ENT.STRES = Init_Reservation(1)

	if sens <> GTENT.SENS
		; changement de sens recalculer les postes et chapitres et décompositions
		Inverser_Totaux_Chapitre_Poste
	endif

endp




