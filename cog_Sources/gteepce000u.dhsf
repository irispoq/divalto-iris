				G1T1.LIB(7) = T014.LIB
	
				; code tarif existe
				If Harmony.Datamodif	|	G1T1.Pcod(1) 	= C_Prix_Standard_Saisi	|	Endif
	
				If G1T1.PromoTaCod = ' ' Or G1T1.Pcod(1) = C_Prix_Standard_Saisi
					Recherche_Prix_G1T1(G1T1.DstQte,Piece_Ligne_LireSlg(G1T1.MOUV,ENT.PiCod),2225)
					If G1T1.Pcod(2) < C_Prix_Manuel 
						Recherche_Remise_G1T1(G1T1.DstQte,Piece_Ligne_LireSlg(G1T1.MOUV,ENT.PiCod))
						
					Endif
					Maj_Promotyp
					
					; calcul HT d'une ligne
					Piece_Calculer_HT_G1T1
				Endif
			Endif
		Endif
		Maj_Champs_Tarifs
	EndIf
	
Endp

