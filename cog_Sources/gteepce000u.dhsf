;*
public procedure Champ_Projet_2_Ap						; controle du projet de la ligne
1		i	2,0
1		Tiers	>gtfdd.dhsd Tiers
1		rz			L			; Résultat zoom
1		TypeProjetAffaire	1,0		; retour de fonction pour identifier code projet ou affaire
beginp
	If Harmony.Key = K_F8
			Switch GTPCE.PiCodDst
			case GTPCE.TicodDst = 'F'     |rz = Zoom_Call(9500)
			Case 1								|	rz = Zoom_Call(9843)
			Default								|	rz = Zoom_Call(9868)
	 		EndSwitch
			If rz <> E_ZOOM_OK	|	PReturn	|	EndIf
	EndIf
	If Harmony.Key = K_SF8 and  GTPCE.PiCodDst = 1
		rz = Zoom_Call(9868)
		If rz <> E_ZOOM_OK	|	PReturn	|	EndIf
	endif

	G1T1.LIB(4) = ' '
	if GTPCE.TiCodDst in ('P','C')
		tiers = ENT.Tiers
	else
		tiers=  ' '
	endif
	TypeProjetAffaire = 2
	if ((GTPCE.PicodDst = 1) and (Controle_Projet_Ou_Affaire(G1T1.PROJET,5005,tiers,conf=false,niv=1,TypeTable=TypeProjetAffaire) = 0))	 \
		or ((GTPCE.PicodDst <> 1) and (Controle_Affaire(G1T1.PROJET, 5000, tiers, conf=false,niv=1) = 0))
		if TypeProjetAffaire = 1
			G1T1.LIB(4) = PRJAV.LIB80
		else	
			G1T1.LIB(4) = PRJAP.LIB80
		endif
				
		; recherche si axe par défaut = projet
		for i = 1 to 4
			if SOC.AXEMAJTYP(i) = 8 and GTENT.PROJET = G1T1.AXE(i)
				G1T1.AXE(i) = G1T1.PROJET

				; controle axe dans C3
				if Soc_Controle_Donnees_comptables and Recherche_Axe(i, G1T1.AXE(i),false) = 0
					G1T1.LIB(15+i) = C5.LIB
				else
					G1T1.AXE(i) = ' '
					G1T1.LIB(15+i) = ' '
				endif
			endif
		next
		; contrôle avenant valide
		G1T1.LIB(21)	= ' '
		if Recherche_Avenant(G1T1.Projet,G1T1.Avenant) = 0
			G1T1.LIB(21) = PrjAvt.Lib80
		else
			G1T1.Avenant = ' '
		endif

	endif
EndP

