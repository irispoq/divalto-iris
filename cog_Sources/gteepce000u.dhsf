;*
Public Procedure Champ_Choix_2_Ap			;pour invalider les options de modification globale
1		ichoix		1,0

Beginp
	ichoix = Harmony.Dataind1

	If Harmony.Datamodif = TRUE
		If GTPCE.Choix(Harmony.Dataind1) = Oui
			XmeSetAttribut(['CHOIX' ichoix], AN_STYLE, 'CHAMP_SAISI')
			XmeResetAttribut(['CHOIX' ichoix], AN_VISIBILITE)
		Else
			XmeSetAttribut(['CHOIX' ichoix], AN_VISIBILITE, AV_GRISE)
			XmeResetAttribut(['CHOIX' ichoix], AN_STYLE)
		Endif

		; Pré-remplissage des valeurs de l'en-tête
		Switch ichoix
		Case 6	|	GTENT.Axe(1) 	= Condition(GTPCE.Choix(6) = Oui, ENT.Axe(1)		, ' ') 	; Axe analytique 1
		Case 7	|	GTENT.Axe(2) 	= Condition(GTPCE.Choix(7) = Oui, ENT.Axe(2)		, ' ') 	; Axe analytique 2
		Case 8	|	GTENT.Axe(3) 	= Condition(GTPCE.Choix(8) = Oui, ENT.Axe(3)		, ' ') 	; Axe analytique 3
		Case 9	|	GTENT.Axe(4) 	= Condition(GTPCE.Choix(9) = Oui, ENT.Axe(4)		, ' ') 	; Axe analytique 4
		Case 5	|	GTENT.Marche 	= Condition(GTPCE.Choix(5) = Oui, ENT.Marche		, ' ')	; Marché
		Case 4	|	GTENT.Projet	= Condition(GTPCE.Choix(4) = Oui, ENT.Projet		, ' ')	; Projet
		Endswitch

		If Harmony.Datanotif = TRUE	; message notifié on reste en input
		   Harmony.retour = XMENEXT_DEBUT_DONNEE
		Endif	
	Endif
Endp

