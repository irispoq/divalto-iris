;*
Public Procedure Champ_VolTot_1_Ap						; après volume total
1	Indice	1,0
Beginp
	If Harmony.Datamodif
		; si le volume a été saisi
		switch GTPCE.TicodDst
		case vsw in ('C','P')
			Indice = SOC.PIEDPCIND
		case 'F'
			Indice = SOC.PIEDPFIND
		default
			Indice = 0
		endswitch

		ENT.VOLCOD = 2
		if ENT.PORCOD = 4 and Indice in (1..3) and ENT.PIEDNO(Indice) <> 0
			;port / volume
			Calculer_Port(ENT.PIEDMT(Indice),ENT.PORTHEOMT,GTENT.PAY(3),GTENT.DEPAR(3))
		endif
	endif
endp
