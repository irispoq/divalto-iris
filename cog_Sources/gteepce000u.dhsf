;*
public procedure Champ_Depo_2_Ap						;controle dépôt de la ligne
1		lieu	>gtfdd.dhsd	LIEU
beginp
	
	if G1T1.DEPO = ' '
		G1T1.DEPO = ENT.DEPO
	endif

	if Controle_Depo_G1T1(1117) = 0
		Piece_Ligne_EnvironnementInit(true, 0)
		; les attributs doivent être gérés avant le traitement des ventilations
		if Soc_Gerer_Emplacements and T017_Ori.LIEUCOD = Oui
			XmeResetAttribut('LIEU',AN_VISIBILITE)
		else
			XmeSetAttribut('LIEU',AN_VISIBILITE,AV_CACHE)
		endif
		if Soc_Gerer_Reservation and T017_Ori.STRES = oui and G1T1.SENS = 2 and G1T1.PiCod <> 1
			XmeResetAttribut('RESERVER2',AN_VISIBILITE)
		else
			XmeSetAttribut('RESERVER2',AN_VISIBILITE,AV_CACHE)
		endif

		G1T1.LIB(2) = T017_Ori.LIB

		If Harmony.Datamodif And Soc_Gerer_Tarifs_Par_Depot And G1T1.Pcod(1) <> C_Prix_Manuel
			Recherche_Prix_G1T1(G1T1.DstQte,Piece_Ligne_LireSlg(G1T1.MOUV,ENT.PiCod),2225)
			
			Maj_Promotyp
			Piece_Calculer_HT_G1T1
		endif

		if Harmony.DataModif
			G1T1.STRES = Init_Reservation(3)

			; recherche emplacement implicite du dépôt 
			if Recherche_EmplacementImplicite (G1T1.REF,G1T1.DEPO,G1T1.SREF1,G1T1.SREF2,G1T1.NST) = 0
				lieu = T018.LIEU
			else
				lieu = ' '
			endif

			Maj_Vtl_Stres(true,lieu)
		endif

	endif
EndP

