;*
public procedure Champ_Sref_2_Ap						; après sous-référence 2
1	qte		13,0
beginp
	if Harmony.key = K_CTRL_S
		G1T1.SREF = G3XZ.SREF
	endif
	G3XZ = ' '

	if Lectab(19,G1T1.SREF2, 1119) = 0
		
		if not (LireProgrammePhase() = C_Phase_LigneCreat and ART.GRISAIS = 2 and G1T1.SREF = ' ' and G1T1.GICOD = 1)

			; pas de saisie en grille

			if Recherche_Sous_ref(G1T1.REF,G1T1.SREF1,G1T1.SREF2,ART.SREFCOD,2114) = 0
				if Piece_Ligne_Controler_Referencement <> 0
					preturn
				endif
		
				; emballage
				if Lectab_EmbCoef(G1T1.REF,G1T1.SREF1,G1T1.SREF2,G1T1.VENUN, G1T1.DstQte) = 0 
					G1T1.EMBUN	= T033.EMBUN
					if T033.EMBQTE <> 0
						if modulo(G1T1.DstQte,T033.EMBQTE) = 0
							G1T1.EMBQTE = G1T1.DstQte / T033.EMBQTE
						else
							truncate
							qte = G1T1.DstQte / T033.EMBQTE 
							G1T1.EMBQTE = qte + 1
							round
						endif
					else
						G1T1.EMBQTE = 0
					endif
				else
					G1T1.EMBUN = ' '
					G1T1.EMBQTE = 0
				endif 		

				Cout_Saisie_Sortie_G1T1		
				switch G1T1.PCOD(1)
				Case C_Prix_Auto
					Recherche_Famille_Tarif_G1T1(G1T1.DstQte,Piece_Ligne_LireSlg(G1T1.MOUV,ENT.PiCod))
					
				Case C_Prix_Promotion_Saisi
					Actualiser_Famille_Tarif_G1T1(G1T1.DstQte,Piece_Ligne_LireSlg(G1T1.MOUV,ENT.PiCod))
					
				Case C_Prix_Standard_Saisi
					Recherche_Prix_G1T1(G1T1.DstQte,Piece_Ligne_LireSlg(G1T1.MOUV,ENT.PiCod))
					
				Endswitch

				Switch G1T1.Pcod(2)
				Case C_Prix_Auto
					Recherche_Classe_Remise_G1T1(G1T1.DstQte,Piece_Ligne_LireSlg(G1T1.MOUV,ENT.PiCod))

				Case C_Prix_Promotion_Saisi
					Actualiser_Classe_Remise_G1T1(G1T1.DstQte,Piece_Ligne_LireSlg(G1T1.MOUV,ENT.PiCod))

				Case C_Prix_Standard_Saisi
					Recherche_Remise_G1T1(G1T1.DstQte,Piece_Ligne_LireSlg(G1T1.MOUV,ENT.PiCod))
					
				Endswitch

				Maj_Promotyp
				; calcul HT d'une ligne en cas de modification
				Piece_Calculer_HT_G1T1
	
				if SART.POIB = 0
					G1T1.POIB	= ART.POIB
				else
					G1T1.POIB	= SART.POIB
				endif
				if SART.POIN = 0
					G1T1.POIN	= ART.POIN
				else
					G1T1.POIN	= SART.POIN
				endif
				if SART.VOL = 0
					G1T1.VOl		= ART.VOL
				else
					G1T1.VOl		= SART.VOL	
				endif
			endif
		endif

		if Soc_Gerer_Emplacements and T017_Dst.LIEUCOD = Oui
			; recherche ArtDepo pour initialiser emplacement même si saisie en grille
			; sera pris en compte pour les prochaines créations de ventilation
			Recherche_ArtDepo(G1T1.REF, T017_Dst.DEPO,G1T1.SREF1, G1T1.SREF2,G1T1.NST )
			G1T1.BLASLIEU = ARTDEPO.LIEU
		endif	

		if Soc_Gerer_Emplacements and T017_Ori.LIEUCOD = Oui
			; recherche ArtDepo pour initialiser emplacement même si saisie en grille
			; sera pris en compte pour les prochaines créations de ventilation
			Recherche_ArtDepo(G1T1.REF, T017_Ori.DEPO,G1T1.SREF1, G1T1.SREF2,G1T1.NST )
			G1T1.LIEU = ARTDEPO.LIEU
		endif	

	endif
	
endp	

