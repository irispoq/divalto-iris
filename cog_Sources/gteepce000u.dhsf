procedure Traitement_Champ_Qte_4_Ap
; Controle de la quantité facturée
beginp
	if Controle_Quantite_G1T1(G1T1.FAQTE,2220) = 0
		if G1T1.DstQte = 0
			Mes_Alert(2287)
		endif
		if G1T1.StQte <> G1T1.DstQte
			Mes_Alert(2286)
		endif
	
		if Harmony.DataModif
			; recherche du tarif par quantité
			If G1T1.Pcod(1) <> C_Prix_Manuel	|	Recherche_Famille_Tarif_G1T1(G1T1.FaQte, G1T1.FaSlg, 2225)	|	Endif
