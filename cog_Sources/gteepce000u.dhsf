;*
public procedure Champ_AdrCod_1_Ap			;controle code adresse

1		flag			1,0					; = 0 si ok
												; <> 0 sinon
1		TransJtNb_Save		>gtfdd.dhsd TRANSJRNB

beginp
	if Harmony.DataModif
	TransJtNb_Save = ENT.TRANSJRNB
	flag = 0
	if ENT.ADRTIERS(Harmony.DataInd1) = ' '
		if ENT.ADRCOD(Harmony.DataInd1) = ' '
			; adresse du tiers
			GTENT.ADR(Harmony.DataInd1)= Piece_Tiers_AdrInd(Harmony.Dataind1)
			switch ENT.Ticod
			case 'C'
				if Harmony.DataInd1 = 3
					ENT.TOUR		= CLI.TOUR
					ENT.TOURRG	= CLI.TOURRG
					ENT.TRANSJRNB= CLI.TRANSJRNB
				endif
			endswitch
		elsif	(flag:=Recherche_Adresse(ENT.TIERS,ENT.ADRCOD(Harmony.DataInd1),2008,ENT.PIDT,Harmony.DataInd1)) =0
				; recherche du code adresse 
				GTENT.ADR(Harmony.DataInd1)= T1.ADR
				if Harmony.DataInd1 = 3 and ENT.TiCod = 'C'
					if T1.TOUR <> ' '
						ENT.TOUR = T1.TOUR
					else
						ENT.TOUR	= CLI.TOUR
					endif
					if T1.TOURRG <> ' '
						ENT.TOURRG = T1.TOURRG
					else
						ENT.TOURRG = CLI.TOURRG
					endif
					if T1.TRANSJRNB <> 0
						ENT.TRANSJRNB= T1.TRANSJRNB
					else
						ENT.TRANSJRNB= CLI.TRANSJRNB
					endif
				endif
		endif
	else
		; controle du client
		; enregistrement cli est écrasé par recherche_client							
		
		Piece_Sauver_Tiers
		if (flag:=Seek_Tiers (ENT.ADRTIERS(Harmony.DataInd1),TiCod_ce1()))  = 0
			
			if ENT.ADRCOD(Harmony.DataInd1) = ' '
				GTENT.ADR(Harmony.DataInd1)= Piece_Tiers_Adr(Harmony.Dataind1)
				if Harmony.DataInd1 = 3 and ENT.TiCod = 'C'
					ENT.TOUR		= CLI.TOUR
					ENT.TOURRG	= CLI.TOURRG
					ENT.TRANSJRNB= CLI.TRANSJRNB
				endif
			elsif	(flag:=Recherche_Adresse(ENT.ADRTIERS(Harmony.DataInd1),ENT.ADRCOD(Harmony.DataInd1),2008,ENT.PIDT,Harmony.DataInd1)) = 0
				GTENT.Adr(Harmony.DataInd1)= T1.ADR
				if Harmony.DataInd1 = 3 and ENT.TiCod = 'C'
					if T1.TOUR <> ' '
						ENT.TOUR = T1.TOUR
					else
						ENT.TOUR	= CLI.TOUR
					endif
					if T1.TOURRG <> ' '
						ENT.TOURRG = T1.TOURRG
					else
						ENT.TOURRG = CLI.TOURRG
					endif
					if T1.TRANSJRNB <> 0
						ENT.TRANSJRNB= T1.TRANSJRNB
					else
						ENT.TRANSJRNB= CLI.TRANSJRNB
					endif
				endif
			else
				ENT.ADRCOD(Harmony.DataInd1) = ' '
				GTENT.ADR(Harmony.DataInd1)= Piece_Tiers_Adr(Harmony.Dataind1)
				if Harmony.DataInd1 = 3 and ENT.Ticod = 'C'
					ENT.TOUR		= CLI.TOUR
					ENT.TOURRG	= CLI.TOURRG
					ENT.TRANSJRNB= CLI.TRANSJRNB
				endif
			endif
		else	
			; erreur traitée par controle_client en amont
		endif
		Piece_Restaurer_Tiers
		if ENT.TRANSJRNB <> TransJtNb_Save
			Maj_DelaiCalcule_Tous
		endif

	endif
	if flag = 0					
		; erreur corrigée
		GTENT.ERRFLag(Harmony.DataInd1) = 0				
	endif
	endif

endp

;*
public procedure Champ_AdrTyp_1_Ap				;initialisation en fonction du type d'adresse
1		iadr	1,0		; obligatoirement en 1,0
1		i		1,0		; obligatoirement en 1,0
1		TransJtNb_Save		>gtfdd.dhsd TRANSJRNB
beginp

	if Harmony.DataModif
		TransJtNb_Save = ENT.TRANSJRNB

	iadr = Harmony.DataInd1

	Switch	ENT.ADRTYP(Harmony.DataInd1)
	Case	1
		; on passe de manuelle à codifiée
		ENT.ADRTIERS(Harmony.DataInd1) = Piece_Tiers_AdrInd(Harmony.Dataind1) 
		ENT.ADRCOD(Harmony.DataInd1) = Piece_Tiers_AdrCod(Harmony.Dataind1)
		if Harmony.DataInd1 = 3
			ENT.TOUR		= CLI.TOUR
			ENT.TOURRG	= CLI.TOURRG
			ENT.TRANSJRNB= CLI.TRANSJRNB
		endif

		;recherche_Adresse
		if ENT.ADRTIERS(Harmony.DataInd1) = ' '
			if ENT.ADRCOD(Harmony.DataInd1) = ' '
			; adresse du tiers

				GTENT.Adr(Harmony.DataInd1)= Piece_Tiers_Adr(Harmony.DataInd1)
			elsif	Recherche_Adresse(ENT.TIERS,ENT.ADRCOD(Harmony.DataInd1),2008,ENT.PIDT,Harmony.DataInd1) =0
				; recherche du code adresse pour le tiers de la pièce
				
				GTENT.Adr(Harmony.DataInd1)= T1.ADR
				if Harmony.DataInd1 = 3 
					if T1.TOUR <> ' '
						ENT.TOUR = T1.TOUR
					elsif GTPCE.TiCodDst = 'C'
						ENT.TOUR	= CLI.TOUR
					endif
					if T1.TOURRG <> ' '
						ENT.TOURRG = T1.TOURRG
					elsif GTPCE.TiCodDst = 'C'
						ENT.TOURRG = CLI.TOURRG
					endif
					if T1.TRANSJRNB <> 0
						ENT.TRANSJRNB= T1.TRANSJRNB
					elsif GTPCE.TiCodDst = 'C'
						ENT.TRANSJRNB= CLI.TRANSJRNB
					endif
				endif

	 		else
				GTENT.Adr(Harmony.DataInd1)= Piece_Tiers_Adr(Harmony.DataInd1)
				ENT.ADRCOD(Harmony.DataInd1) = ' '
			endif
		else
			; controle du client
			; enregistrement cli est écrasé par recherche_client							
			
			Piece_Sauver_Tiers
			if Recherche_Client (ENT.ADRTIERS(Harmony.DataInd1),2000)  = 0
				
				if ENT.ADRCOD(Harmony.DataInd1) = ' '
					GTENT.Adr(Harmony.DataInd1) = Piece_Tiers_Adr(Harmony.DataInd1)
					if Harmony.DataInd1 = 3
						ENT.TOUR		= CLI.TOUR
						ENT.TOURRG	= CLI.TOURRG
						ENT.TRANSJRNB= CLI.TRANSJRNB
					endif
				elsif	Recherche_Adresse(ENT.ADRTIERS(Harmony.DataInd1),ENT.ADRCOD(Harmony.DataInd1),2008,ENT.PIDT,Harmony.DataInd1) = 0
					GTENT.Adr(Harmony.DataInd1)= T1.ADR
					if Harmony.DataInd1 = 3 
						if T1.TOUR <> ' '
							ENT.TOUR = T1.TOUR
						elsif GTPCE.TiCodDst = 'C'
							ENT.TOUR	= CLI.TOUR
						endif
						if T1.TOURRG <> ' '
							ENT.TOURRG = T1.TOURRG
						elsif GTPCE.TiCodDst = 'C'
							ENT.TOURRG = CLI.TOURRG
						endif
						if T1.TRANSJRNB <> 0
							ENT.TRANSJRNB= T1.TRANSJRNB
						elsif GTPCE.TiCodDst = 'C'
							ENT.TRANSJRNB= CLI.TRANSJRNB
						endif
					endif

				else
					ENT.ADRCOD(Harmony.DataInd1) = ' '
					GTENT.Adr(Harmony.DataInd1)= Piece_Tiers_Adr(Harmony.DataInd1)
					if Harmony.DataInd1 = 3	and GTPCE.TiCodDst = 'C'
						ENT.TOUR		= CLI.TOUR
						ENT.TOURRG	= CLI.TOURRG
						ENT.TRANSJRNB= CLI.TRANSJRNB
					endif
					Harmony.Retour = 0
				endif
			else	
				; client adresse inexistant
					ENT.ADRTIERS(Harmony.DataInd1) = ' '
					Piece_Restaurer_Tiers
					GTENT.Adr(Harmony.DataInd1)= CLI.ADR
					if Harmony.DataInd1 = 3	 and GTPCE.TiCodDst = 'C'
						ENT.TOUR		= CLI.TOUR
						ENT.TOURRG	= CLI.TOURRG
						ENT.TRANSJRNB= CLI.TRANSJRNB
					endif

					Harmony.Retour = 0
			endif
			Piece_Restaurer_Tiers
			GTENT.ERRFLag(Harmony.DataInd1) = 0				; l'adresse est obligatoirement ok
		endif

		; modification des attributs
		XmeResetAttribut(['ADRTIERS' iadr],AN_STYLE)
		XmeSetAttribut(['ADR' iadr],AN_STYLE,'CHAMP_NON_SAISI')

		; vérifier si autre adresse pointait sur celle-ci
		for i = 1 to 5
			if ENT.ADRTYP(i) = 2 + Harmony.DataInd1
				ENT.ADRTYP(i) = 1
				ENT.ADRTIERS(i) = ' '
				ENT.ADRCOD(i) = ' '
				GTENT.Adr(i)= Piece_Tiers_Adr(Harmony.DataInd1)
				XmeResetAttribut(['ADRTIERS' i],AN_STYLE)
				XmeSetAttribut(['ADR' i],AN_STYLE,'CHAMP_NON_SAISI')
			endif
		next


	Case 	2
		ENT.ADRTIERS(Harmony.DataInd1) = ' '
		ENT.ADRCOD(Harmony.DataInd1) = ' '
		GTENT.Adr(Harmony.DataInd1) = ' '
		GTENT.ERRFLag(Harmony.DataInd1) = 0				; l'adresse est obligatoirement ok
		ENT.TRANSJRNB	= 0								; on n'impute pas de temps de transport au délai

		; modification des attributs
		XmeSetAttribut(['ADRTIERS' iadr],AN_STYLE,'CHAMP_NON_SAISI')
		XmeResetAttribut(['ADR' iadr],AN_STYLE)

	Default
		ENT.ADRTIERS(Harmony.DataInd1) = ' '
