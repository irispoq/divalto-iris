;*
Public Procedure Page_ModificationDesLignesPointees_1_Av
1			i		2,0
BeginP
	GTPCE.DELDEMDT	= ' '
	GTPCE.DELACCDT	= ' '
	GTPCE.DELREPDT	= ' '
	GTENT.AXETB(*)	= ' '
	GTENT.MARCHE	= ' '
	GTENT.PROJET	= ' '
	for i = 30 to index(GTENT.LIB(),1)
		GTENT.LIB(i) = ' '
	next
	init GTPCE.Choix() = 1
	for i = 1 to index(GTPCE.Choix(),1)

		XmeResetAttribut(['CHOIX' right(i)],AN_VISIBILITE)				
		XmeResetAttribut(['CHOIX' right(i)],AN_STYLE)
	Next			 

EndP

;*
Public Procedure Page_ModificationDesLignesPointees_1_Ap
1			i		2,0
1	indice 2,0
BeginP
	switch ENT.TiCod
	case 'C'
		indice= 1
	case 'P'
		indice= 1
	case 'F'
		indice= 2
	case 'I'
		indice= 0
	endswitch
	if indice <> 0
		for i = 1 to 4
			if GTPCE.Choix(5+i) = Oui
				if SOC.AXESAIS(indice,i) = 3 and GTENT.AXE(i) = ' '	; axe obligatoire non renseigné
					Mes_Alert(8005,i)
					Harmony.Retour = XMENEXT_Traitement_Avant
				endif
			endif
		next
	endif
EndP

