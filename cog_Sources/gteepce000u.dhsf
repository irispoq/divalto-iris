;*
Public Procedure Champ_TaCod_3_Ap
;
Beginp
	If Harmony.DataModif
		G1T1.Lib(24) = ' '
		
		If G1T1.PromoTaCod <> ' '
			If G1T1.PromoTaCod = '99' And not Soc_Gerer_Prix_Specifiques_Client
				; prix promotionnel particulier non géré
				Mes_Alert(2224)
				Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
			Elsif Lectab(72,Piece_Ticod_Tarif  &  G1T1.PromoTaCod,1172) = 0
					G1T1.Lib(24) = T072.Lib
		
				; code tarif promotion existe
				If Harmony.Datamodif	|	G1T1.Pcod(1) 	= C_Prix_Promotion_Saisi	|	Endif
			Endif
		Endif
		Recherche_Prix_G1T1(G1T1.DstQte,Piece_Ligne_LireSlg(G1T1.MOUV,ENT.PiCod),2225)
		
		If G1T1.Pcod(2) < C_Prix_Manuel
			Recherche_Remise_G1T1(G1T1.DstQte,Piece_Ligne_LireSlg(G1T1.MOUV,ENT.PiCod))		
		Endif
		Maj_Promotyp
		
		; calcul HT d'une ligne
		Piece_Calculer_HT_G1T1
		Maj_Champs_Tarifs
	EndIf
	
Endp					 

