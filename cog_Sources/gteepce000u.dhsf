;*
Public Procedure Champ_Ref_1_Ap
1		i				2,0
1		slg   		>GTFDD.dhsd LiLg
1		RfcCtrCod	>GTFDD.dhsd RfcCtrCod
1		Niv			1,0
1		TvaArt		>GTFDD.dhsd TvaArt
RecordSql 'GTRSPPTAB.dhoq'	TauxTvaN	TauxTva_Loc

beginp
	If Harmony.Key = K_SF8
		Switch GTPCE.TiCodDst
		Case 'C'	|	If Zoom_Call(9874) <> E_ZOOM_OK	|	PReturn	|	EndIf
		Case 'F'	|	If Zoom_Call(9877) <> E_ZOOM_OK	|	PReturn	|	EndIf
		EndSwitch
	EndIf
	
	If Harmony.Key = K_CTRL_S
		G1T1.Ref		= G3XZ.Ref
		G1T1.Sref	= G3XZ.Sref
	EndIf
	G3XZ = ' '

	slg = Piece_Ligne_LireSlg(G1T1.MOUV, ENT.PiCod)
	Switch TRUE
	Case slg <> 0			|	RfcCtrCod = 1					; Pas de contrôle dans le cas des sous-lignes
	Case ENT.Ticod = 'C'	|	RfcCtrCod = CLI.RfcCtrCod
	Case ENT.Ticod = 'F'	|	RfcCtrCod = FOU.RfcCtrCod
	Default					|	RfcCtrCod = 1
	EndSwitch

	Switch GTPCE.TiCodDst
	Case 'C'	|	Niv = 1
	Case 'F'	|	Niv = 2
	Case 'I'	|	Niv = 3
	Default 	|	Niv = 1
	EndSwitch

	If Controle_Article(G1T1.Ref, 2100, Niv, TRUE, ENT.PiDt, ENT.Tiers, TRUE, rfcctrcod=RfcCtrCod) = 0
		TvaArt = Condition(GTPCE.TicodDst <> 'F', ART.TvaArt, ART.TvaArtA)

		If ENT.Ticod = 'I'
			If OP_TrtType_CessionInterEtb(T020_ENT)
				If OP_TrtType_TransfertDepot(T020_ENT) And ART.StCod = 1
					Mes_Alert(2133, G1T1.Ref)
					Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
				ElsIf not OP_TrtType_TransfertDepot(T020_ENT) And ART.StCod = 2
					Mes_Alert(2142)
					Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
				EndIf
			EndIf
		ElsIf Seek_TauxTvaN(TauxTva_Loc, TvaArt, ENT.TvaTie, ENT.PiDt) = 0		; Contrôle code TVA
			If Soc_Utiliser_TPF																	; Pas de contrôle pour les pièces internes
				For i = 1 to 3																		; Contrôle code TPF
					If ART.TpfR(i) <> 0  and GTPCE.TicodDst = 'C'
						If Lectab(4, [ ART.TpfR(i) ENT.TpfT ], 1104) <> 0 
							i = 3
						EndIf
					EndIf
				Next
			EndIf
		Else
			Mes_Alert(1185, [ TvaArt *1 ENT.TvaTie *1 ENT.PiDt ])
			Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
		EndIf

		If ART.Pdp = 2 And ENT.PrgCdeFlg = 2
			Mes_Alert(2145)
			Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
		EndIf
	Else
		G1T1.Ref	= G1T1.RefD
	EndIf

	If Harmony.Retour = XMENEXT_TRAITEMENT_AVANT
		G1T1.Ref	= G1T1.RefD
	EndIf
Endp

