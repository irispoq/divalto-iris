public procedure Champ_Jnl_1_Ap

;	Après saisie du journal d'amortissement

beginp	; Champ_Jnl_1_Ap

	X1.Lib(1) = ' '
	If Controle_Journal(DOS_IMMO.Jnl)=0
		X1.Lib(1) = C4.Lib
	endif

