;*
;*		Module écran du zoom sur le dossier Immobilisation															scctz000
;*

;*		Log	 Version	  N°modif		Date		Modifications
;*
;*		YLEF		P205a						10/2014	DAVFIN_FE001 	Code confidentialité pour saisie de la date de début amo d'un bien

;
Module	"A5Tm000.DHOP"
Module 	"CCTM000.DHOP"
;
Public RecordSql "ccrsdos.dhoq" 	dosimmo 	DOS_IMMO
;
Public Record 	"ccfdd.dhsd" 		x1 		x1
Public Record	"CCfdd.dhsd"		C3
Public Record	"CCfdd.dhsd"		C4									; Journaux



;*
Private Procedure Champ_Cog_CesJnl_1_Ap
;
Beginp
	DOS_IMMO.Libelle_CesJnl = ' '
	If Controle_Journal(DOS_IMMO.Cog_CesJnl)=0
		DOS_IMMO.Libelle_CesJnl = C4.Lib	
	endif
Endp


;*
Private Procedure Champ_Cog_ConfSvcDt_1_Ap
;
Beginp
	Controler_Confidentialite(DOS_IMMO.Cog_ConfSvcDt , 4000)
Endp


;*
Private Procedure Champ_Cog_CptCesAmo_1_Ap
;
Beginp
	DOS_IMMO.CptCesAmo_Lib = ' '
	if Controle_Compte(DOS_IMMO.Cog_CPTCesAmo, etablissement = false) < 2
		DOS_IMMO.CptCesAmo_Lib = C3.Lib
	endif
Endp


;*
Private Procedure Champ_Cog_CptCesVal_1_Ap
;
Beginp
	DOS_IMMO.CptCesVal_Lib = ' '
	if Controle_Compte(DOS_IMMO.Cog_CptCesVal, etablissement = FALSE) < 2
		DOS_IMMO.CptCesVal_Lib = C3.Lib
	endif
Endp


;*
Private Procedure Champ_Cog_CptCes_1_Ap
;
Beginp
 	DOS_IMMO.CptCes_Lib = ' '
	if Controle_Compte(DOS_IMMO.Cog_CPTCes, etablissement = FALSE) < 2
		DOS_IMMO.CptCes_Lib = C3.Lib
	endif
Endp
