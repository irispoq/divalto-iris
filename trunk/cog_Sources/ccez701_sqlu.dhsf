;*
public procedure Champ_FisTyp_1_Ap

; après méthode d'amortissement fiscale

beginp	; Champ_FisTyp_1_Ap

	if Harmony.DataModif

		if IMMOBILISATION.FisTyp = 5 and CD.IfrsFl = 1
			Mes_Alert(7137)
			Harmony.Retour = XMENEXT_DEBUT_DONNEE
			preturn
		endif

		Regler_Acces_Fistyp

		Regler_Acces_Cumuls

		if Harmony.DataNotif
			Harmony.Retour = XMENEXT_DEBUT_DONNEE
			preturn
		endif
	endif

	X1.Choix(7) = true
	if AMOTYP = IMMOBILISATION.FisTyp	|	preturn								|	endif

	if IMMOBILISATION.FisTyp <> 3			|	IMMOBILISATION.DotFis = ' '	|	endif
	if IMMOBILISATION.FisTyp = 4			|	IMMOBILISATION.FisDur = ' '	|	endif

	if	Reglage_Parametres(C_Plan_Fiscal, IMMOBILISATION.FisTyp, IMMOBILISATION.FisDur, IMMOBILISATION.FisCoe, IMMOBILISATION.FisTau, IMMOBILISATION.DotFis, IMMOBILISATION.UoCode, IMMOBILISATION.AcqDt, IMMOBILISATION.SvcDt) \
	and IMMOBILISATION.FisBas and X1.Choix(1) and IMMOBILISATION.EcoTyp <> 3 and IMMOBILISATION.FisTyp <> 3
		Dotations_Cumuls
	endif

	if Harmony.Key = K_TAB \													; Tabulation
		and IMMOBILISATION.FisTyp = 4											; 	et fistyp='sans', on boucle sur le 1er champ
		Harmony.Retour = XMENEXT_PREMIERE_DONNEE
	endif

endp		; Champ_FisTyp_1_Ap


