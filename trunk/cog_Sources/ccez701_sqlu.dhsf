;*
Public Procedure Champ_NatImmo_1_Ap

; Après saisie de la nature d'immobilisation

1			i							2,0

Beginp	; Champ_NatImmo_1_Ap

	if not Harmony.DataModif					|	preturn	|	endif
	if IMMOBILISATION.NatImmo = ' '			|	preturn	|	endif

	Lectab_HD(IMMOBILISATION.NatImmo, 9163)
	X1.Lib(9) 				 = HD.Lib

	IMMOBILISATION.EcoTyp = HD.EcoTyp
	IMMOBILISATION.EcoDur = HD.EcoDur
	IMMOBILISATION.FisTyp = HD.FisTyp
	IMMOBILISATION.FisDur = HD.FisDur

	IMMOBILISATION.Cpt	 = HD.Cpt
	Recherche_Compte(IMMOBILISATION.Cpt)
	X1.Lib(1) 			 	 = C3.Lib
	
	IMMOBILISATION.CptAux = HD.CptAux
	Recherche_Compte(IMMOBILISATION.CptAux, Conf = condition(CA.ConfEnr(1) <> 2 , false, true))
	X1.Lib(2) 				 = C3.Lib

	IMMOBILISATION.UoCode = HD.UoCode
	Rechercher_planUO(IMMOBILISATION.UoCode)
	X1.Lib(7) 				 = I3.Lib

	IMMOBILISATION.AxeTb(*) = HD.AxeTb(*)
	for i = 1 to index(IMMOBILISATION.Axe(), 1)
		Recherche_Axe(i, IMMOBILISATION.Axe(i), Conf = condition(CA.ConfEnr2(i) <> 2 , false, true))
		X1.Lib(2 + i) = C5.Lib
	next

	if	(Reglage_Parametres(C_Plan_Eco, IMMOBILISATION.EcoTyp, IMMOBILISATION.EcoDur, IMMOBILISATION.EcoCoe, IMMOBILISATION.EcoTau, IMMOBILISATION.DotEco, IMMOBILISATION.UoCode, IMMOBILISATION.AcqDt, IMMOBILISATION.SvcDt)		\
	or	 Reglage_Parametres(C_Plan_Fiscal, IMMOBILISATION.FisTyp, IMMOBILISATION.FisDur, IMMOBILISATION.FisCoe, IMMOBILISATION.FisTau, IMMOBILISATION.DotFis, IMMOBILISATION.UoCode, IMMOBILISATION.AcqDt, IMMOBILISATION.SvcDt))	\
	and (IMMOBILISATION.EcoBas or IMMOBILISATION.FisBas) and X1.Choix(1) and IMMOBILISATION.EcoTyp <> 3 and IMMOBILISATION.FisTyp <> 3
		Dotations_Cumuls
	endif

Endp		; Champ_NatImmo_1_Ap


