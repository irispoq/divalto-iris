;*
;*		zoom sur les biens																													cctez701.dhsp
;*

;*		log	 version	  n°modif		date		modifications
;*		YLEF		P205						10/2014	Controles champs comptes de cession 

;*
;*	 Déclarations
;*

;
Include	"CCTC000.dhsp"

;
Module "GTPM000.DHOP"
Module "GTPMFIC.DHOP"
Module "GTTM000.DHOP"

;
Public RecordSql "ccrsimmo.dhoq" immo IMMOBILISATION

;
Public Record "ccfdd.dhsd" x1 			x1
Public Record "ccfdd.dhsd" cd 			CD
Public Record "ccfdd.dhsd" ca 			ca
Public Record "ccfdd.dhsd" C3				C3
Public Record "ccfdd.dhsd" C5				C5
Public Record "ccfdd.dhsd" HD				HD
Public Record "ddsys.dhsd" rowinfo 		zoomselection
Public Record "ccfdd.dhsd" i1 			i1_sel
public record "gtfdd.dhsd"	PRJAP														; Affaire
public record "gtfdd.dhsd"	XZ				G3XZ
public record "gtfdd.dhsd"	ART

;*
;*  Procédures 
;*

;*
procedure Champ_Axe_1_Av
;
BeginP

	If Harmony.Dataind1 <> 2
		Standard.Champ_Axe_1_Av
	EndIf

EndP

;* 
procedure Champ_Axe_1_Ap
;
BeginP

	Switch Harmony.Dataind1
		Case 2		; l'axe 2 est un code affaire
			x1.Lib(4) = ' '
			If Controle_Affaire(IMMOBILISATION.Axe(2), 5000, tiers='', conf=false, niv=1) = 0
				x1.Lib(4) = PRJAP.Lib80
				IMMOBILISATION.Cog_AxeAnael = PRJAP.Cog_AxeAnael
			EndIf

		Default
			Standard.Champ_Axe_1_Ap

	EndSwitch

EndP

;*
Procedure Champ_NatImmo_1_Ap
;
BeginP

	Standard.Champ_NatImmo_1_Ap

	If Harmony.Datamodif AND IMMOBILISATION.NatImmo <> ' ' AND HD.NatImmo <> ' '

		IMMOBILISATION.CptAmo		= HD.CptAmo
		IMMOBILISATION.CptDotAmo	= HD.CptDotAmo
		IMMOBILISATION.CptProDro	= HD.CptProDro
		IMMOBILISATION.CptDotDro	= HD.CptDotDro
		IMMOBILISATION.CptRepDro	= HD.CptRepDro
		IMMOBILISATION.CptProDep	= HD.CptProDep
		IMMOBILISATION.CptDotDep	= HD.CptDotDep
		IMMOBILISATION.CptDotRep	= HD.CptDotRep
		IMMOBILISATION.EcoTyp		= Condition(HD.EcoTyp <> 0, HD.EcoTyp, 1)	; mode économique = 1:linéaire par défaut
		IMMOBILISATION.EcoDur		= HD.EcoDur
		IMMOBILISATION.FisTyp		= Condition(HD.FisTyp <> 0, HD.FisTyp, 1)	; mode fiscal = 1:linéaire
		IMMOBILISATION.FisDur		= HD.FisDur
		IMMOBILISATION.UoCode		= HD.UoCode	

	EndIf

EndP

;*
Procedure Champ_SvcDt_1_Av
; avant date de mise en service
BeginP	; Champ_SvcDt_1_Av

	If IMMOBILISATION.EcoAmDt <> ' '
		Harmony.Retour = XMENEXT_SAUT_DONNEE
	EndIf

	Standard.Champ_SvcDt_1_Av

endp		; Champ_SvcDt_1_Av

;*
Procedure Champ_AcqDt_1_Ap
; après date d'acquisition
1		wDt		>CCFDD.DHSD		EcoAmDt
BeginP

	wDt = IMMOBILISATION.EcoAmDt
	Standard.Champ_AcqDt_1_Ap
	IMMOBILISATION.EcoAmDt = Condition(wDt >= IMMOBILISATION.AcqDt, wDt, ' ')

EndP


;*
Procedure Champ_EcoAmDt_1_Av
; avant date de debut d'amortissement
BeginP	; Champ_EcoAmDt_1_Av

	Standard.Champ_EcoAmDt_1_Av

	If CD.Dos = " "
		Seek_CD()
	EndIf

	If G3_Protection(CD.Cog_ConfSvcDt, vide=TRUE) <> 0
		MessageBox("Champ soumis à confidentialité.", "Information", MB_ICONINFORMATION)
		Harmony.Retour = XMENEXT_SAUT_DONNEE
	EndIF

EndP

;*
Procedure Champ_EcoAmDt_1_Ap
; après date de debut d'amortissement
BeginP

	If Harmony.Datamodif AND IMMOBILISATION.EcoAmDt <> ' '
		IMMOBILISATION.SvcDt = IMMOBILISATION.EcoAmDt
		Standard.Champ_SvcDt_1_Ap
	ElsIf	IMMOBILISATION.EcoAmDt = ' '
		IMMOBILISATION.Cog_AmoFlag = 1
	EndIf

	;Standard.Champ_EcoAmDt_1_Ap		  le standard ne permet pas de laisser le champ à espace
	if IMMOBILISATION.EcoAmDt <> ' ' AND IMMOBILISATION.EcoAmDt < IMMOBILISATION.SvcDt
		Mes_Alert(19718)
		Harmony.Retour = XmeNext_Traitement_Avant
		preturn
	endif

EndP

;*
;*		Champs spécifiques
;*

;*
Private Procedure Champ_Cog_CptCesVal_1_Ap
;
Beginp
	IMMOBILISATION.CptCesVal_Lib = ' '
	if Controle_Compte(IMMOBILISATION.Cog_CptCesVal, etablissement = true, etb = IMMOBILISATION.Etb) < 2
		IMMOBILISATION.CptCesVal_Lib = C3.Lib
	endif
Endp

;*
Private Procedure Champ_Cog_CptCesAmo_1_Ap
;
Beginp
	IMMOBILISATION.CptCesAmo_Lib = ' '
	if Controle_Compte(IMMOBILISATION.Cog_CPTCesAmo, etablissement = true, etb = IMMOBILISATION.Etb) < 2
		IMMOBILISATION.CptCesAmo_Lib = C3.Lib
	endif
Endp


;*
Private Procedure Champ_Cog_CptCes_1_Ap
;
Beginp
 	IMMOBILISATION.CptCes_Lib = ' '
	if Controle_Compte(IMMOBILISATION.Cog_CPTCes, etablissement = true, etb = IMMOBILISATION.Etb) < 2
		IMMOBILISATION.CptCes_Lib = C3.Lib
	endif
Endp

;*
Private Procedure Champ_Cog_AmoFlag_1_Av
;
Beginp
	If IMMOBILISATION.Cog_BwId = ' ' OR IMMOBILISATION.EcoAmDt = ' '
		Harmony.Retour = XMENEXT_SAUT_DONNEE
	EndIf
Endp


;*
Private Procedure Champ_Cog_AxeAnael_1_Ap
;
Beginp
	If Harmony.Datamodif
		IMMOBILISATION.AxeAnael_Lib = ' '
		If IMMOBILISATION.Cog_AxeAnael <> ' ' AND Controle_Axe(2, IMMOBILISATION.Cog_AxeAnael) = 0
			IMMOBILISATION.AxeAnael_Lib = C5.Lib	
		EndIF
	EndIf
Endp


;*
Private Procedure Champ_Cog_BwId_1_Ap
;
Beginp
	IF IMMOBILISATION.Cog_BwId = ' '
		IMMOBILISATION.Cog_AmoFlag = 1
	EndIf
Endp





;*
Private Procedure Champ_Lib_1_Ap
;
Beginp
	IMMOBILISATION.Des = ' '
	If Seek_ARt(IMMOBILISATION.Lib) = 0
		IMMOBILISATION.Des = ART.DES
	EndIf
Endp
[/diva]
