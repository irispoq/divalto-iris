;*
public procedure Champ_Etat_1_Ap

; après état

record	CCFDD.dhsd	I1		i1p												; bien 'père'
1		rpere						1,0												; résultat recherche père

beginp	; Champ_Etat_1_Ap

	if Harmony.DataModif

		; controle de l'état en création
		if ZOOM.ACTION = ZOOM_CREATION
			if IMMOBILISATION.Etat not in (1,4)								; seuls les état 'en-cours'(1) ou 'en construction' sont autorisés
				IMMOBILISATION.Etat = 1
				Harmony.Retour = XmeNext_Traitement_Avant
				preturn
			endif
		endif

		; Mise 'en construction' d'un bien : seulement si aucun amortissement
		if IMMOBILISATION.Etat <> ETAT and IMMOBILISATION.Etat = 4
			if IMMOBILISATION.EcoTotMt > 0 or IMMOBILISATION.FisTotMt > 0
				;"Le bien a déjà des amortissements, mise 'en construction' interdite"
				Mes_Alert(19728)
				Harmony.Retour = XmeNext_Traitement_Avant
				preturn
			endif
		endif

		; Mise 'en cours' (ou vente/rebut) d'un bien composant
		if IMMOBILISATION.Etat <> ETAT and IMMOBILISATION.Etat in (1,2,3)
			; test pour un composant
			if Controler_Bien_Structure(IMMOBILISATION.Immo, IMMOBILISATION.ImmoComp) = 2		; il s'agit d'un composant
				if (rpere := Rechercher_BienPere(IMMOBILISATION.Immo, IMMOBILISATION.ImmoComp, i1p)) = 1
					if i1p.Etat <> 1
						;"Le bien composé père n'est pas 'en cours' | Changement d'état du composant interdit"
						Mes_Alert(19729)
						Harmony.Retour = XmeNext_Traitement_Avant
						preturn
					endif
				else
					if rpere = -1
						;"Bien composé 'père' impossible à lire "
						Mes_Alert(19730)
						Harmony.Retour = XmeNext_Traitement_Avant
						preturn
					endif
				endif
			endif
		endif

		; Cession ou mise au rebut
		if IMMOBILISATION.Etat <> ETAT and IMMOBILISATION.Etat in (2,3)
			; Pour un bien composé : controler la cession (ou mise au rebut) de tous les composants
			if Controler_Bien_CessionCompose(I1) <> 0
				; au moins un composant est actif
				Harmony.Retour = XmeNext_Traitement_Avant
				preturn
			endif
		endif

		; Réactivation d'un bien cédé ou mis au rebut
		if IMMOBILISATION.Etat <> ETAT and IMMOBILISATION.Etat not in (2,3) and ETAT in (2,3)
			; Pour un bien composant : controler si son composé 'père' est actif
			if Controler_Bien_Reactiver(I1) <> 0
				; le composé 'père' est inactif
				Harmony.Retour = XmeNext_Traitement_Avant
				preturn
			endif
		endif
		
		switch IMMOBILISATION.Etat
		case vsw in (1,4)									
			; en cours (1) ou en construction(4)
			XmeSetAttribut("DateCession", AN_VISIBILITE, AV_GRISE)
			XmeSetAttribut("PrixCession", AN_VISIBILITE, AV_GRISE)
			XmeSetAttribut("DateCession", AN_STYLE, "CHAMP_NON_SAISI")
			XmeSetAttribut("PrixCession", AN_STYLE, "CHAMP_NON_SAISI")
		case 2
			; vendu
			XmeResetAttribut("DateCession", AN_TOUS_ATTRIBUTS)
			XmeResetAttribut("PrixCession", AN_TOUS_ATTRIBUTS)
		case 3
			; au rebut
			XmeResetAttribut("DateCession", AN_TOUS_ATTRIBUTS)
			XmeSetAttribut("PrixCession", AN_VISIBILITE, AV_GRISE)
			XmeSetAttribut("PrixCession", AN_STYLE, "CHAMP_NON_SAISI")
		endswitch

	endif

	X1.Choix(5) = true
	if ETAT = IMMOBILISATION.Etat	|	preturn	|	endif

	if IMMOBILISATION.Etat = 1																	; l'état avant modification était une cession
		IMMOBILISATION.CesDt = ' '
		IMMOBILISATION.CesMt	= ' '
	elsif IMMOBILISATION.Etat = 3																; l'état après modification est une mise au rebut
		IMMOBILISATION.CesMt = ' '
	endif
		
	if	Calcul_Possible
		Dotations_Cumuls
	endif

	Harmony.Retour = XMENEXT_DONNEE_SUIVANTE

endp		; Champ_Etat_1_Ap


