;*
;*		fonctions et procédures locales
;*

;*
procedure RAZ_Cumuls

; initialise à ' ' (ou 0), les zones de cumuls

beginp	; RAZ_Cumuls
	
	IMMOBILISATION.ExoDDt			= ' '
	IMMOBILISATION.ExoFDt			= ' '
	; zones cumuls economiques
	IMMOBILISATION.EcoLinTau		= ' '
	IMMOBILISATION.EcoDDt			= ' '
	IMMOBILISATION.EcoFDt			= ' '
	IMMOBILISATION.EcoJourNb		= ' '
	IMMOBILISATION.EcoMoisNb		= ' '
	IMMOBILISATION.EcoLinMt			= ' '
	IMMOBILISATION.EcoDegMt			= ' '
	IMMOBILISATION.EcoManMt			= ' '
	IMMOBILISATION.EcoExcMt			= ' '
	IMMOBILISATION.EcoPuoMt			= ' '
	IMMOBILISATION.EcoTotMt			= ' '
	IMMOBILISATION.CumReval			= ' '
	IMMOBILISATION.CumDprec			= ' '
	IMMOBILISATION.EcoResMt			= IMMOBILISATION.EcoBas
	;zones cumuls fiscaux
	IMMOBILISATION.FisLinTau		= ' '
	IMMOBILISATION.FisDDt			= ' '
	IMMOBILISATION.FisFDt			= ' '
	IMMOBILISATION.FisJourNb		= ' '
	IMMOBILISATION.FisMoisNb		= ' '
	IMMOBILISATION.FisLinMt			= ' '
	IMMOBILISATION.FisDegMt			= ' '
	IMMOBILISATION.FisManMt			= ' '
	IMMOBILISATION.FisExcMt			= ' '
	IMMOBILISATION.FisTotMt			= ' '
	IMMOBILISATION.FisResMt			= IMMOBILISATION.FisBas

endp		; RAZ_Cumuls


;*
public function int Calcul_Possible

; vérifie que tous les paramètres nécessaires au calcul des cumuls sont renseignés
; et que l'on a le droit de saisir ou modifier les cumuls

; true	tous les paramètres sont renseignés
; false	il manque des paramètres

beginf	; calcul_possible

;	if not X1.Choix(1)							|	freturn(false)	|	endif
	if (not X1.Choix(1))																		\
	and (IMMOBILISATION.EcoTyp <> 4 or IMMOBILISATION.SvcDt >= CD.ExoDDt)	\
	and (IMMOBILISATION.FisTyp <> 4 or IMMOBILISATION.SvcDt >= CD.ExoDDt)
		freturn(false)
	endif
	if IMMOBILISATION.EcoTyp < 1				|	freturn(false)	|	endif
	if IMMOBILISATION.FisTyp < 1				|	freturn(false)	|	endif
	if IMMOBILISATION.AcqDt  = ' '			|	freturn(false)	|	endif
	if IMMOBILISATION.EcoTyp = 3				|	freturn(false)	|	endif
	if IMMOBILISATION.FisTyp = 3				|	freturn(false)	|	endif

	if IMMOBILISATION.EcoTyp <> 4
		if IMMOBILISATION.EcoBas = 0			|	freturn(false)	|	endif
		if IMMOBILISATION.EcoDur = 0			|	freturn(false)	|	endif
		switch IMMOBILISATION.EcoTyp
		case 1
			if IMMOBILISATION.SvcDt = ' '		|	freturn(false)	|	endif
		case 2
			if IMMOBILISATION.EcoCoe = 0		|	freturn(false)	|	endif
		case 3
			if IMMOBILISATION.SvcDt  = ' '	|	freturn(false)	|	endif
			if IMMOBILISATION.DotEco = 0		|	freturn(false)	|	endif
		case 5
			if IMMOBILISATION.UoCode = ' '	|	freturn(false)	|	endif
		endswitch
	endif

	if IMMOBILISATION.FisTyp <> 4
		if IMMOBILISATION.FisBas = 0			|	freturn(false)	|	endif
		if IMMOBILISATION.FisDur = 0			|	freturn(false)	|	endif
		switch IMMOBILISATION.FisTyp
		case 1
			if IMMOBILISATION.SvcDt = ' '		|	freturn(false)	|	endif
		case 2
			if IMMOBILISATION.FisCoe = 0		|	freturn(false)	|	endif
		case 3
			if IMMOBILISATION.SvcDt  = ' '	|	freturn(false)	|	endif
			if IMMOBILISATION.DotFis = 0		|	freturn(false)	|	endif
		endswitch
	endif

	freturn(true)

endf		; calcul_possible


