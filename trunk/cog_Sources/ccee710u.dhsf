;*
public procedure Champ_Immo_6_Ap

; après saisie du nouveau numéro de bien

beginp	; Champ_Immo_6_Ap

	; trt du zoom eventuel
	if C3XZ.ModifFlg = 1
		PongReceive("c3xz", C3XZ)
		NEWI1.Immo = C3XZ.Immo
		NEWI1.ImmoComp = C3XZ.ImmoComp
		Harmony.Retour = XMENEXT_POINT_SEQUENCE
		Harmony.CplRetour = 1
		preturn
	endif

	if X1.Choix(1) <> 2				; Contrôle du numéro de bien seulement
											; s'il s'agit d'un bien indépendant

		if NEWI1.Immo <> IMMORES
	
			; l'utilisateur veut un numéro particulier, il faut vérifier qu'il est valide et disponible
	
			if NEWI1.Immo <= 0													  ; n° valide > 0
				Mes_Alert(19707)
				Harmony.Retour = XmeNext_Traitement_Avant
				preturn
			endif
	
			if NEWI1.Immo = OLDI1.Immo												; n° identique à l'ancien bien
				Mes_Alert(7120)
				Harmony.Retour = XmeNext_Traitement_Avant
				preturn
			endif

			if Res_I1(NEWI1.Immo)<>0												; n° de bien déjà réservé
				Harmony.Retour = XmeNext_Traitement_Avant
				preturn
			endif
	
			if hseek(CCFIMMO, I1, [ 'B' MZ.Dos NEWI1.Immo NEWI1.ImmoComp ], 'F') = 0	; le n° du nouveau bien existe déjà					
				Lib_I1(NEWI1.Immo)
				Mes_Alert(7114)
				Harmony.Retour = XmeNext_Traitement_Avant
				preturn
			endif

			; ok, ce numéro est disponible
	
			Lib_I1(IMMORES)
			IMMORES = NEWI1.Immo
	
		endif

	else

		; controles pour un composant
		if NEWI1.Immo > 0

			if Controler_Bien_Structure(NEWI1.Immo) <> 1
				; "Le bien XXX n'est pas une structure"
				Mes_Alert(7118, NoSpaces(NEWI1.Immo))
				Harmony.Retour = XmeNext_Traitement_Avant
				NEWI1.Immo = 0
				preturn
			else
				; recherche cle composant...
				if hpread(CCFIMMO, I1, [ 'B' MZ.Dos NEWI1.Immo $FF ], 'F') = 0 and I1.Dos = MZ.Dos and I1.Immo = NEWI1.Immo
					NEWI1.ImmoComp = I1.ImmoComp + Composant_I1_Valeur_Increment
				endif
			endif
			if Res_I1(NEWI1.Immo)
				Harmony.Retour = XmeNext_Traitement_Avant
				preturn
			endif
	
			if hseek(CCFIMMO, I1, [ 'B' MZ.Dos NEWI1.Immo NEWI1.ImmoComp ], 'F') = 0					
				Lib_I1(NEWI1.Immo)
				Mes_Alert(7114)
				Harmony.Retour = XmeNext_Traitement_Avant
				preturn
			endif

			Lib_I1(IMMORES)
			IMMORES = NEWI1.Immo

		else

			Harmony.Retour = XmeNext_Traitement_Avant

		endif

	endif

endp		; Champ_Immo_6_Ap


