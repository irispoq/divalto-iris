;*
public procedure Regler_Choix_Scission(mode)

;	Gère la définition d'un nouveau bien selon qu'il sera indépendant ou composant

1		mode							1					; mode création nouveau bien en scission
															; "C" = création composant, "B" =  création bien indépendant

1		wimmocomp	>CCFDD.dhsd	IMMOCOMP			; n° composant

beginp	; Regler_Choix_Scission

	if mode = "C"				
	
	; *** le nouveau bien en scission	sera un composant	d'une structure existante ***

		XmeResetAttribut("composant", AN_TOUS_ATTRIBUTS)

		NEWI1.Immo = 0
		NEWI1.ImmoComp = 0

		if IMMORES > 0
			Lib_I1(IMMORES)														; libération ancienne réservation
			IMMORES = 0
		endif

	else

	;	*** le nouveau bien sera indépendant	(ni composant, ni composé) ***

		XmeSetAttribut("composant", AN_STYLE, "CHAMP_NON_SAISI")
		XmeSetAttribut("composant", AN_VISIBILITE, AV_GRISE)

		; pré-attribution et réservation d'un nouveau numéro de bien

		if IMMORES > 0
			Lib_I1(IMMORES)														; libération ancienne réservation
			IMMORES = 0
		endif

		;	1ère recherche du prochain n° de bien dispo
		if hpread(CCFIMMO, I1, [ 'B' MZ.Dos $FF ], 'F') = 0 and I1.Dos = MZ.Dos
			NEWI1.Immo = I1.Immo + 1
		else
			NEWI1.Immo = 1
		endif
		NEWI1.ImmoComp = 0													
	
		;	Réservation du prochain n° de bien réservable et disponible
		do
			loop Res_I1(NEWI1.Immo, nowait)
				NEWI1.Immo++
			endloop
		while hseek(CCFIMMO, I1, [ 'B' MZ.Dos NEWI1.Immo NEWI1.ImmoComp ], 'F') = 0
			Lib_I1(NEWI1.Immo)
			NEWI1.Immo++
		wend
	
		IMMORES = NEWI1.Immo

	endif
	
endp		; Regler_Choix_Scission


;*
public procedure Regler_Acces_Amotyp(amotyp, plan, composant)

; regle l'affichage/saisie des champs  selon la méthode d'amortissement
; Concerne les écrans de saisie des paramètres de décomposition

1		amotyp		>CCFDD.dhsd	ECOTYP					; méthode d'amortissement (ECOTYP ou FISTYP)
1		plan							1							; plan eco ou fiscal = C_Plan_Eco ou C_Plan_Fiscal
1		composant					2							; concerne composant 1 ("C1") ou composant 2 ("C2")

1		sufx							3							; suffixe
1		xdot							10 = 'dotation'		; 'nom' champs dotation
1		xdur							10 = 'duree'			; 'nom' champs durée
1		xplan							10 = 'planUO'			; 'nom' champs plan uo

beginp	; Regler_Acces_Amotyp

	sufx  = plan  & composant
	xdot  = xdot  & sufx
	xdur  = xdur  & sufx
	xplan = xplan & sufx
		
	switch amotyp
	case 1																		; méthode linéaire
		XmeSetAttribut("xdot",AN_STYLE, "CHAMP_NON_SAISI")
		XmeResetAttribut(xdur, AN_TOUS_ATTRIBUTS)
		XmeSetAttribut(xplan, AN_VISIBILITE, AV_CACHE)
	case 2																		; méthode dégressive
		XmeSetAttribut("xdot",AN_STYLE, "CHAMP_NON_SAISI")
		XmeResetAttribut(xdur, AN_TOUS_ATTRIBUTS)
		XmeSetAttribut(xplan, AN_VISIBILITE, AV_CACHE)
	case 3																		; méthode manuelle
		XmeResetAttribut(xdot, AN_TOUS_ATTRIBUTS)
		XmeResetAttribut(xdur, AN_TOUS_ATTRIBUTS)
		XmeSetAttribut(xplan, AN_VISIBILITE, AV_CACHE)
	case 4																		; sans amortissement
		XmeSetAttribut("xdot",AN_STYLE, "CHAMP_NON_SAISI")
		XmeSetAttribut("xdur",AN_STYLE, "CHAMP_NON_SAISI")
		XmeSetAttribut(xplan, AN_VISIBILITE, AV_CACHE)
	case 5																		; méthode par plan unites d'oeuvre
		XmeSetAttribut("xdot",AN_STYLE, "CHAMP_NON_SAISI")
		XmeResetAttribut(xdur, AN_TOUS_ATTRIBUTS)
		XmeResetAttribut(xplan, AN_TOUS_ATTRIBUTS)
	endswitch

endp		; Regler_Acces_Amotyp
		

;*
public procedure Regler_Test_Depreciation

;	Test le montant de dépréciation
;	 et Règle l'affichage

beginp	; Regler_Test_Depreciation

	;	Initialisation des paramètres pour le contrôle
	CC700 				= ' '
	CC700.DpDepMt 		= OLDI2.EcoDotDepMt 									; si la dépréciation a déjà été saisie, on vérifie sa valeur
	CC700.DpValActu	= CC700Dp.DpValActu									; Valeur actuelle
	CC700.DpResMt		= CC700Dp.DpResMt

	if Controler_Parametres_Depreciation(mess = true) = 1				; On peut déprécier
		XmeResetAttribut("dprecmt", AN_TOUS_ATTRIBUTS)
	endif

	; Après controle (Controler_parametres_depreciation()), le résultat se trouve dans CC700.DpDepMt
	OLDI2.EcoDotDepMt = CC700.DpDepMt										; maj montant reprise controlé et calculé

endp		; Regler_Test_Depreciation


;*
public procedure Regler_Test_Reprise

;	Test le montant de reprise de dépréciation
;	 et Règle l'affichage

beginp	; Regler_Test_Reprise

	; Initialiser les paramètres pour le controle
	CC700 = ' '
	CC700.DpRepMt 		= OLDI2.EcoDotRepMt 									; si la dépréciation a déjà été saisie, on vérifie sa valeur
	CC700.DpResTheoMt = OLDI2.EcoResTheoMt									; Valeur résiduelle théorique (hors dépréciations/reprises)
	CC700.DpResMt		= CC700Dp.DpResMt										; Montant residuel hors dépréciation/reprise de l'exercice courant
	CC700.DpCumDprec 	= CC700Dp.DpCumDprec
	CC700.DpValActu	= CC700Dp.DpValActu

 	if Controler_Parametres_Depreciation(mess = true) = 1				; Reprise de dépréciation possible
		XmeResetAttribut("reprisemt", AN_TOUS_ATTRIBUTS)
	endif

	; Après controle (Controler_parametres_depreciation()), le résultat se trouve dans CC700.DpDepMt
	OLDI2.EcoDotRepMt = CC700.DpRepMt										; maj montant reprise controlé et calculé

endp		; Regler_Test_Reprise


;*
public procedure Regler_Choix_Depreciation(choixDprec)

;	Règle l'affichage des variables selon le choix de l'action : dépréciation ou reprise de dépréciation

1		choixDprec			1,0												; Choix de l'action : 	1 = Dépréciation
																					;								2 = Reprise de dépréciation

beginp	; Regler_Choix_Depreciation

	 choixDprec = 	Changer_Mode_Depreciation(choixDprec)			; change le mode : modifie les flags
	 	
	 switch choixDprec
	
	 case 1			; Depréciation
	 	XmeSetAttribut("reprisetit", AN_STYLE, "STD")
		XmeSetAttribut("deprectit", AN_STYLE, "STD_GRAS")
		XmeResetAttribut("deprec", AN_TOUS_ATTRIBUTS)
		XmeResetAttribut("reprisemt", AN_TOUS_ATTRIBUTS)
		XmeSetAttribut("reprise", AN_VISIBILITE, AV_CACHE)
		Harmony.Retour = XMENEXT_POINT_SEQUENCE
		Harmony.CplRetour = 1
	
	 case 2			; Reprise de dépréciation
	 	XmeSetAttribut("reprisetit", AN_STYLE, "STD_GRAS")
		XmeSetAttribut("deprectit", AN_STYLE, "STD")
		XmeResetAttribut("reprise", AN_TOUS_ATTRIBUTS)
