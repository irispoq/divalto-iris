;*
;*		Saisie des paramètres de Dépréciation et/ou Reprise de dépréciation
;*

;*
public procedure Page_DepreciationParametrage_1_Av

;	avant saisie des paramètres de dépréciation/reprise

beginp	; Page_DepreciationParametrage_1_Av

	 X1.Choix(3) = Choix_Depreciation_Autorisee					; Recupération de la valeur du choix autorisé (1 2 ou 3)

	 if X1.Choix(3) < 3
	 	; Un seul choix autorisé													
	 	CC710.Choix(1) = X1.Choix(3)									; c'est le choix imposé
		
		if CC710.Choix(1)=1
			XmeReSetAttribut("deprectit", AN_TOUS_ATTRIBUTS)
			XmeSetAttribut("reprisetit", AN_VISIBILITE, AV_GRISE)
		else
			XmeReSetAttribut("reprisetit", AN_TOUS_ATTRIBUTS)
			XmeSetAttribut("deprectit", AN_VISIBILITE, AV_GRISE)
		endif
		Harmony.Retour = XMENEXT_DONNEE_SUIVANTE

	else
		; les 2 choix dépréciation et reprise sont autorisés
		XmeReSetAttribut("deprectit", AN_TOUS_ATTRIBUTS)		
		XmeReSetAttribut("reprisetit", AN_TOUS_ATTRIBUTS)

	endif

	;	Initialisation des variables d'affichage
	Regler_Choix_Depreciation(CC710.Choix(1))

	CC700Dp.DpValActu = Calculer_Valeur_Actuelle(OLDI2.VenalMt, OLDI2.UsagMt)
	if CC700Dp.DpValActu > 0
		if CC710.Choix(1) = 1
			Regler_Test_Depreciation
		else
			Regler_Test_Reprise
		endif
	endif

endp		; Page_DepreciationParametrage_1_Av


;*
public procedure Champ_Choix_1_Ap

;	après saisie choix depréciation ou reprise

beginp	; Champ_Choix_1_Ap

	if Harmony.DataNotif or Harmony.DataModif

		Regler_Choix_Depreciation(CC710.Choix(1))

	   pinglocalreceive("CC710_I2ORI", OLDI2)							; restaurer valeur initiales de l'amortissement courant
		
		CC700Dp.DpValActu = Calculer_Valeur_Actuelle(OLDI2.VenalMt, OLDI2.UsagMt)
		if CC700Dp.DpValActu > 0
			if CC710.Choix(1) = 1
				Regler_Test_Depreciation
			else
				Regler_Test_Reprise
			endif
		endif
		Harmony.Retour = XMENEXT_DONNEE_SUIVANTE
   endif

endp		; Champ_Choix_1_Ap


