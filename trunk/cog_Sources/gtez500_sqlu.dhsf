;*
Procedure Cog_Affaire_Champ_Modifiable(wMsg)
;  Controle si la facturation a débuté
; Oui --> modification du champ non permise, saut de donnée
1		wMsg		1,0 = TRUE
BeginP

	If Cog_UT125_FacturationDebutee(Affaire.Affaire)
		If wMsg
			MessageBox("Facturation débutée, ce champ n'est pas modifiable.", "Information", MB_ICONINFORMATION)
		EndIf
		Harmony.Retour = XMENEXT_SAUT_DONNEE
	EndIf

EndP

;*
Function int Cog_Seek_Immo(wImmo, wErr)
; Recherche fiche bien (sans composant)
; 0 --> CogRs_Immo chargé
; si wErr > 0; msg  + retour en saisir
1		wImmo		>ccfdd.dhsd		Immo
1		wIcomp	>ccfdd.dhsd		ImmoComp	= ' '
1		wErr		1,0							= TRUE
BeginF

	If wImmo <> ' '
		CogRs_Immo.Init()
		CogRs_Immo.Where.RemoveCondition("")
		CogRs_Immo.Where.Zoom_PK(wImmo, wIcomp)
		If CogRs_Immo.Select() = 0
			If wErr > 0
				Harmony.Retour = XMENEXT_RESSAISIE
				MessageBox("N° fiche bien inconnu.", "Alerte", MB_ICONERROR)
			EndIf	
			FReturn(1)
		EndIf
	EndIf

	FReturn(0)

EndF

;*
Private Procedure Affiche_Gantt
Beginp

	Ident	= Xmelistgetid(harmony.Masque, cPageGantt,"COG_GANTT",g7)
	Xmelisterase(Ident)
	Xmelistresetattribut("","",an_couleur_fond)
	
	grfevt.key = 'J' & mz.dos & affaire.affaire & '99999999999999'
	If Hpread(grfevt,evttiers) = 0 and evttiers.dos = mz.dos and evttiers.affaire = affaire.affaire
		datdeb = evttiers.evtdt
		datfin = evttiers.evtdt
		XmeListGetColumn (Ident, 2, ColumnInfo) 
		ColumnInfo.Libelle = formatd("J0/M0/AA",datdeb)             
		XmeListSetColumn (Ident, 2, ColumnInfo)
		For i = 3 to 21
			datfin = dtadd(datfin,1)
			If Dayweek(datfin) = 6 And Soc.Cog_Samedi   < 2 | i-- | Redofor | Endif
			If Dayweek(datfin) = 7 And Soc.Cog_Dimanche < 2 | i-- | Redofor | Endif
			XmeListGetColumn (Ident, i, ColumnInfo) 
			ColumnInfo.Libelle = formatd("J0/M0/AA",datfin)            
			XmeListSetColumn (Ident, i, ColumnInfo)
		Next

		grfevt.key = 'J' & mz.dos & affaire.affaire & '99999999999999'
		Loop Hpread(grfevt,evttiers) = 0 and evttiers.dos = mz.dos and evttiers.affaire = affaire.affaire
			If Seek_Grtevt(evttiers.evenement) = 0
				G7.Winchnc = grtevt.lib
			Else
				G7.Winchnc = 'Inconnu !'
			Endif
			cog_gantt.Nomligne  = evttiers.evenement
			For i = 1 to 20
				wgnum.n1d0(i) = system.esp
				If i < 10 | jour = "0" & nospaces(i)
				Else		 | jour = i | Endif
				XmeListGetColumn (Ident, i+1, ColumnInfo)
				Fdate(wdate,ColumnInfo.Libelle)					
				If wdate >= evttiers.evtdt and wdate <= evttiers.evtfindt
					Xmelistsetattribut(evttiers.evenement,jour,an_couleur_fond,"AFFOK")
					If i = 1  And wdate > evttiers.evtdt	| wgnum.n1d0(1)  = 2 | Endif
					If i = 20 And wdate < evttiers.evtfindt| wgnum.n1d0(20) = 3 | Endif
				Endif
			Next
			Xmelistinsert(Ident,g7)
		EndLoop
		Xmelistbegin(Ident)
		Xmelistdisplay(Ident)

	Endif
	
Endp

;*
Private Procedure Btn_Precedent_Clic
Beginp
	Xmelisterase(Ident)
	Xmelistresetattribut("","",an_couleur_fond)
	If Isdate(datdeb)
		If Isdate(datret)
			datdeb = datret
			datfin = datret
			datret = system.esp
		Else
			datdeb = dtadd(datfin,-39)
			datfin = dtadd(datfin,-39)
			If Soc.Cog_Samedi < 2 
				datdeb = dtadd(datdeb,-8)
				datfin = dtadd(datfin,-8)
			Endif
			If Soc.Cog_Dimanche < 2
				datdeb = dtadd(datdeb,-8)
				datfin = dtadd(datfin,-8)
			Endif
			If Dayweek(datdeb) = 6 And Soc.Cog_Samedi < 2 
				datdeb = dtadd(datfin,-1)
				datfin = dtadd(datfin,-1)
			ElsIf Dayweek(datdeb) = 7 And Soc.Cog_Dimanche < 2
				datdeb = dtadd(datfin,-2)
				datfin = dtadd(datfin,-2)
			Endif
		Endif
		XmeListGetColumn (Ident, 2, ColumnInfo) 
		ColumnInfo.Libelle = formatd("J0/M0/AA",datdeb)             
		XmeListSetColumn (Ident, 2, ColumnInfo)
		For i = 3 to 21
			datfin = dtadd(datfin,1)
			If Dayweek(datfin) = 6 And Soc.Cog_Samedi   < 2 | i-- | Redofor | Endif
			If Dayweek(datfin) = 7 And Soc.Cog_Dimanche < 2 | i-- | Redofor | Endif
			XmeListGetColumn (Ident, i, ColumnInfo) 
			ColumnInfo.Libelle = formatd("J0/M0/AA",datfin)            
			XmeListSetColumn (Ident, i, ColumnInfo)
		Next

		grfevt.key = 'J' & mz.dos & affaire.affaire & '99999999999999'
		Loop Hpread(grfevt,evttiers) = 0 and evttiers.dos = mz.dos and evttiers.affaire = affaire.affaire
			If Seek_Grtevt(evttiers.evenement) = 0
				G7.Winchnc = grtevt.lib
			Else
				G7.Winchnc = 'Inconnu !'
			Endif
			cog_gantt.Nomligne  = evttiers.evenement
			For i = 1 to 20
				wgnum.n1d0(i) = system.esp
				If i < 10 | jour = "0" & nospaces(i)
				Else		 | jour = i | Endif
				XmeListGetColumn (Ident, i+1, ColumnInfo)
				Fdate(wdate,ColumnInfo.Libelle)
				If wdate >= evttiers.evtdt and wdate <= evttiers.evtfindt
					Xmelistsetattribut(evttiers.evenement,jour,an_couleur_fond,"AFFOK")
					If i = 1  And wdate > evttiers.evtdt	| wgnum.n1d0(1)  = 2 | Endif
					If i = 20 And wdate < evttiers.evtfindt| wgnum.n1d0(20) = 3 | Endif
				Endif
			Next
			Xmelistinsert(Ident,g7)
		EndLoop
		Xmelistbegin(Ident)
		Xmelistdisplay(Ident)
	Endif
Endp


