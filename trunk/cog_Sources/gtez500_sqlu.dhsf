;*
public procedure Champ_Tiers_1_Ap	 ; controle tiers
beginp
	affaire.CLI_PRI_NOM = ' '

	if affaire.Tiers = ' '
		; pas de gestion d'adresse
		affaire.AdrTiers = ' '
		affaire.AdrCod	= ' '
		G7.Adr(1) = ' '
		affaire.CLI_ADR_NOM = ' '
		XmeSetAttribut("tiers", AN_STYLE, "CHAMP_NON_SAISI")
	elsif Controle_Client(affaire.Tiers, 2000, ctrvisa = false)  = 0	; on accepte client uniquement
		affaire.CLI_PRI_NOM = CLI.Nom
		if affaire.Repr = ' '
			affaire.Repr = CLI.Repr(1)
			Recherche_Vrp(affaire.Repr)
			affaire.VRP_NOM = VRP.Nom
		endif
		XmeResetAttribut("tiers", AN_TOUS_ATTRIBUTS)
	endif
endp

